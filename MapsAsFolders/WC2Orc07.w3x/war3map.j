    local unit u= GetTriggerUnit()

    set ConstructEvent__finished[(GetUnitUserData((u)))]=true // INLINED!!
    call ConstructEvent__FireEvent(EVENT_UNIT_CONSTRUCTION_FINISH , null , u)

