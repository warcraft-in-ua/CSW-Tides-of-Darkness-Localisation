//===========================================================================
// Trigger: Load Maim
//===========================================================================
function Trig_Load_Maim_Func002C takes nothing returns boolean
    if ( not ( GetStoredBooleanBJ("Completed", "WC2Orc05", udg_Cache) == true ) ) then
        return false
    endif
    return true
endfunction

function Trig_Load_Maim_Actions takes nothing returns nothing
    call DisableTrigger(GetTriggeringTrigger())
    if ( Trig_Load_Maim_Func002C() ) then
        call RestoreUnitLocFacingPointBJ("Maim", "WC2Orc05", udg_Cache, Player(20), GetRectCenter(gg_rct_MaimStart), GetRectCenter(gg_rct_IntroGrunt03))
        set udg_Maim=GetLastRestoredUnitBJ()
    else
        call CreateNUnitsAtLocFacingLocBJ(1, 'N60K', Player(20), GetRectCenter(gg_rct_MaimStart), GetRectCenter(gg_rct_IntroGrunt03))
        set udg_Maim=GetLastCreatedUnit()
        call SetHeroLevelBJ(udg_Maim, 5, false)
        call SelectHeroSkill(udg_Maim, 'A00K')
        call SelectHeroSkill(udg_Maim, 'A00K')
        call SelectHeroSkill(udg_Maim, 'A00M')
        call SelectHeroSkill(udg_Maim, 'A00J')
        call SelectHeroSkill(udg_Maim, 'A00J')
        call CreateItemLoc('I60A', GetRectCenter(gg_rct_MaimStart))
        call UnitAddItemSwapped(GetLastCreatedItem(), udg_Maim)
    endif
endfunction

