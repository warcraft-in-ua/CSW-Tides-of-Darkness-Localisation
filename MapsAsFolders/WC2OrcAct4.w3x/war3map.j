//===========================================================================
// Trigger: Cinematic END
//===========================================================================
function Trig_Cinematic_END_Conditions takes nothing returns boolean
    if ( not ( IsTriggerEnabled(GetTriggeringTrigger()) == true ) ) then
        return false
    endif
    return true
endfunction

function Trig_Cinematic_END_Actions takes nothing returns nothing
    call DisableTrigger(GetTriggeringTrigger())
    // CC disabled
    call SetNextLevelBJ("WC2Orc12.w3x")
    // Zorrots Walkaround
    // Victory
    call TriggerSleepAction(2)
    call CustomVictoryBJ(Player(0), false, false)
endfunction

//===========================================================================
function InitTrig_Cinematic_END takes nothing returns nothing
    set gg_trg_Cinematic_END=CreateTrigger()
    call TriggerAddCondition(gg_trg_Cinematic_END, Condition(function Trig_Cinematic_END_Conditions))
    call TriggerAddAction(gg_trg_Cinematic_END, function Trig_Cinematic_END_Actions)
endfunction

//===========================================================================
// Trigger: ToS Prep
//===========================================================================
function Trig_ToS_Prep_Actions takes nothing returns nothing
    call CinematicFadeBJ(bj_CINEFADETYPE_FADEOUT, 0.00, "ReplaceableTextures\\CameraMasks\\Black_mask.blp", 0, 0, 0, 0)
    call ClearMapMusicBJ()
    call EndThematicMusicBJ()
    call EnableDawnDusk(false)
    call CinematicModeBJ(true, GetPlayersAll())
    //  
    call SetCineModeVolumeGroupsBJ()
    //  
    set udg_Cinematic_ToS_Playing=true
    set udg_Cinematic_ToS_Skipped=false
    //  
    call SetDestructableAnimationBJ(gg_dest_B008_2803, "stand")
    call SetDestructableAnimationBJ(gg_dest_B008_2802, "stand")
    call SetDestructableAnimationBJ(gg_dest_B008_2804, "stand")
    call SetDestructableAnimationBJ(gg_dest_B008_2801, "stand")
    call SetDestructableAnimationBJ(gg_dest_B008_2798, "stand")
    call SetDestructableAnimationBJ(gg_dest_B008_2799, "stand")
    call SetDestructableAnimationBJ(gg_dest_B008_2800, "stand")
    call SetDestructableAnimationBJ(gg_dest_B008_2797, "stand")
    call SetDestructableAnimationBJ(gg_dest_B008_2805, "stand")
    call SetDestructableAnimationBJ(gg_dest_B008_2806, "stand")
    //  
    call TriggerSleepAction(1.00)
    call EnableTrigger(gg_trg_ToS_Skipped)
    // ToSGuldanTheme ----------------------------------------------------------------------------------------------------------------------------------------------------
    call PlayThematicMusicBJ("Warcraft2\\Music\\ToSGuldanTheme.mp3")
    call ConditionalTriggerExecute(gg_trg_ToS_1_Intro_Fleet)
endfunction

//===========================================================================
function InitTrig_ToS_Prep takes nothing returns nothing
    set gg_trg_ToS_Prep=CreateTrigger()
    call TriggerAddAction(gg_trg_ToS_Prep, function Trig_ToS_Prep_Actions)
endfunction

//===========================================================================
// Trigger: ToS Skipped
//===========================================================================
function Trig_ToS_Skipped_Conditions takes nothing returns boolean
    if ( not ( udg_Cinematic_ToS_Playing == true ) ) then
        return false
    endif
    if ( not ( udg_Cinematic_ToS_Skipped == false ) ) then
        return false
    endif
    return true
endfunction

function Trig_ToS_Skipped_Actions takes nothing returns nothing
    call DisableTrigger(GetTriggeringTrigger())
    set udg_Cinematic_ToS_Skipped=true
    // Fade
    call CinematicFadeBJ(bj_CINEFADETYPE_FADEOUTIN, 0.00, "ReplaceableTextures\\CameraMasks\\Black_mask.blp", 0, 0, 0, 0)
    // Cleanup
    call TriggerSleepAction(1.00)
    call ConditionalTriggerExecute(gg_trg_ToS_Cleanup)
    set udg_Cinematic_ToS_Playing=false
    // Next Scene
    call TriggerSleepAction(0.10)
    call ConditionalTriggerExecute(gg_trg_Alterac_Prep)
endfunction

//===========================================================================
function InitTrig_ToS_Skipped takes nothing returns nothing
    set gg_trg_ToS_Skipped=CreateTrigger()
    call DisableTrigger(gg_trg_ToS_Skipped)
    call TriggerRegisterPlayerEventEndCinematic(gg_trg_ToS_Skipped, Player(0))
    call TriggerAddCondition(gg_trg_ToS_Skipped, Condition(function Trig_ToS_Skipped_Conditions))
    call TriggerAddAction(gg_trg_ToS_Skipped, function Trig_ToS_Skipped_Actions)
endfunction

//===========================================================================
// Trigger: ToS Cleanup
//===========================================================================
function Trig_ToS_Cleanup_Conditions takes nothing returns boolean
    if ( not ( udg_Cinematic_ToS_Playing == true ) ) then
        return false
    endif
