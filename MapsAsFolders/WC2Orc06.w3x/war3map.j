    // //
    if ( Trig_BCSetUp_Func040C() ) then
        call ShowUnitShow(udg_IntroCatapult[1])
        call SetUnitPositionLocFacingBJ(udg_IntroCatapult[1], GetRectCenter(gg_rct_CatapultBridge01), 90.00)
