function Trig_BridgeCollapseCinematicQ_Actions takes nothing returns nothing
    call DisableTrigger(GetTriggeringTrigger())
    // //
    call ForGroupBJ(GetUnitsSelectedAll(Player(20)), function Trig_BridgeCollapseCinematicQ_Func003A)
    call ClearSelectionForPlayer(Player(20))
    // //
    call ConditionalTriggerExecute(gg_trg_OilUIHide)
    call CinematicModeBJ(true, GetPlayersAll())
    call CinematicFadeBJ(bj_CINEFADETYPE_FADEOUT, 1.00, "ReplaceableTextures\\CameraMasks\\Black_mask.blp", 0, 0, 0, 0)
    call TriggerSleepAction(1.00)
    // //
    call ConditionalTriggerExecute(gg_trg_BCSetUp)
    call CameraSetupApplyForPlayer(true, gg_cam_Bridge01, Player(20), 0)
    call SetCineModeVolumeGroupsBJ()
    // //
    call IssuePointOrderLocBJ(udg_FakeZuluhed, "move", GetRectCenter(gg_rct_PostBridgeZuluhed))
    call IssuePointOrderLocBJ(udg_FakeUtok, "move", GetRectCenter(gg_rct_PostBridgeUtok))
    // //
    call PlayThematicMusicBJ("Tension")
    call CinematicFadeBJ(bj_CINEFADETYPE_FADEIN, 1.00, "ReplaceableTextures\\CameraMasks\\Black_mask.blp", 0, 0, 0, 0)
    call TriggerSleepAction(1.00)
    // //
    set udg_BridgeCollapseCinematic=true
    // //
    call IssuePointOrderLocBJ(udg_FakeChogall, "move", GetRectCenter(gg_rct_ChogallBridge02))
    call IssuePointOrderLocBJ(udg_IntroCatapult[1], "move", GetRectCenter(gg_rct_CatapultBridge01Dies))
    call IssuePointOrderLocBJ(udg_IntroCatapult[2], "move", GetRectCenter(gg_rct_CatapultBridge02Dies))
    // Bridge should crack here
    call TriggerSleepAction(2.00)
    if ( (udg_BridgeCollapsedSkipped == true) ) then // INLINED!!
        return
    else
        call DoNothing()
    endif
    // //
    call CameraSetupApplyForPlayer(true, gg_cam_Bridge02, Player(20), 0)
    call CameraSetupApplyForPlayer(true, gg_cam_Bridge03, Player(20), 6.00)
    // //
    call TriggerSleepAction(( DistanceBetweenPoints(GetUnitLoc(udg_FakeChogall), GetRectCenter(gg_rct_ChogallBridge02)) / GetUnitMoveSpeed(udg_FakeChogall) ))
    if ( (udg_BridgeCollapsedSkipped == true) ) then // INLINED!!
        return
    else
        call DoNothing()
    endif
    // //
    // DestroyBridge
    call KillDestructable(gg_dest_B00T_6542)
    call TriggerSleepAction(0.50)
    if ( (udg_BridgeCollapsedSkipped == true) ) then // INLINED!!
        return
    else
        call DoNothing()
    endif
    call KillUnit(udg_IntroCatapult[1])
    call KillUnit(udg_IntroCatapult[2])
    // //
    call TriggerSleepAction(3.00)
    if ( (udg_BridgeCollapsedSkipped == true) ) then // INLINED!!
        return
    else
        call DoNothing()
    endif
    call SetUnitAnimation(udg_FakeChogall, "cinematic angry one")
    call PlayDialogueFromSpeakerEx(bj_FORCE_PLAYER[0], udg_FakeChogall, 'O61R', gg_snd_WC2Orc06Chogall_BrokenBridge1, bj_TIMETYPE_ADD, 0, true)
    if ( (udg_BridgeCollapsedSkipped == true) ) then // INLINED!!
        return
    else
        call DoNothing()
    endif
    // //
    call AddSpecialEffectLocBJ(GetRectCenter(gg_rct_ChogallBridge02), "Abilities\\Spells\\Orc\\ChogallSpells\\VoidTeleportTarget.mdx")
    set udg_ChogalTeleportTargetSE[1]=GetLastCreatedEffectBJ()
    call AddSpecialEffectLocBJ(GetRectCenter(gg_rct_ChogallBridgeTeleport), "Abilities\\Spells\\Orc\\ChogallSpells\\VoidTeleportTo.mdx")
    set udg_ChogalTeleportTargetSE[2]=GetLastCreatedEffectBJ()
    call SetUnitAnimation(udg_FakeChogall, "spell")
    call TriggerSleepAction(1.20)
    if ( (udg_BridgeCollapsedSkipped == true) ) then // INLINED!!
        return
    else
        call DoNothing()
    endif
    // //
    call SetUnitPositionLoc(udg_FakeChogall, GetRectCenter(gg_rct_ChogallBridgeTeleport))
    call DestroyEffectBJ(udg_ChogalTeleportTargetSE[1])
    call DestroyEffectBJ(udg_ChogalTeleportTargetSE[2])
    call SetUnitAnimation(udg_FakeChogall, "cinematic proud one")
    call QueueUnitAnimationBJ(udg_FakeChogall, "cinematic listening one")
    call PlayDialogueFromSpeakerEx(bj_FORCE_PLAYER[0], udg_FakeChogall, 'O61R', gg_snd_WC2Orc06Chogall_BrokenBridge2, bj_TIMETYPE_ADD, 0, true)
    if ( (udg_BridgeCollapsedSkipped == true) ) then // INLINED!!
        return
    else
        call DoNothing()
    endif
    // Clean Up
    call ConditionalTriggerExecute(gg_trg_BDCleanUp)
endfunction

