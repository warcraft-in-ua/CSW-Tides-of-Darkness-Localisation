    call SetUnitAnimation(udg_Unit_Tharbek, "cinematic salute one")
    call QueueUnitAnimationBJ(udg_Unit_Tharbek, "cinematic listening one")
    call TriggerSleepAction(0.25)
    if ( (udg_Scene_2_Skipped == true) ) then // INLINED!!
        return
    else
        call DoNothing()
    endif
    //  
    call CameraSetFocalDistance(150.00)
    call CameraSetupApplyForPlayer(true, gg_cam_Scene_1_Doomhammer_4, Player(0), 0)
    //  
    call SetPlayerColorBJ(Player(0), PLAYER_COLOR_RED, false)
    call PlayDialogueFromSpeakerEx(bj_FORCE_PLAYER[0], udg_Unit_Tharbek, 'o00J', gg_snd_WC2ORCACT02Tharbek1, bj_TIMETYPE_ADD, 0, false)
    call WaitForSoundBJ(gg_snd_WC2ORCACT02Tharbek1, 0.50)
    if ( (udg_Scene_2_Skipped == true) ) then // INLINED!!
        return
    else
        call DoNothing()
    endif
    //  
    call SetUnitAnimation(udg_Unit_Doomhammer_1, "spell")
    call QueueUnitAnimationBJ(udg_Unit_Doomhammer_1, "cinematic listening one")
    call WaitForSoundBJ(gg_snd_WC2ORCACT02Tharbek1, 0.00)
    if ( (udg_Scene_2_Skipped == true) ) then // INLINED!!
        return
    else
        call DoNothing()
    endif
    //  
    call TriggerSleepAction(0.50)
    if ( (udg_Scene_2_Skipped == true) ) then // INLINED!!
        return
    else
        call DoNothing()
    endif
    // --------------------------------------------------------------------------------------------------------------------------------------------------------------
    // Doomhammer 7
    // --------------------------------------------------------------------------------------------------------------------------------------------------------------
    call SetPlayerColorBJ(Player(0), PLAYER_COLOR_RED, false)
    call PlayDialogueFromSpeakerEx(bj_FORCE_PLAYER[0], udg_Unit_Doomhammer_1, 'O001', gg_snd_WC2ORCACT02Doomhammer7, bj_TIMETYPE_ADD, 0, false)
    call TriggerSleepAction(1.00)
    if ( (udg_Scene_2_Skipped == true) ) then // INLINED!!
        return
    else
        call DoNothing()
    endif
    //  
    call CameraResetSmoothingFactorBJ()
    call CameraSetFocalDistance(300.00)
    call CameraSetupApplyForPlayer(true, gg_cam_Scene_1_Doomhammer_2_a, Player(0), 0)
    call CameraSetupApplyForPlayerSmooth(true, gg_cam_Scene_2_Throneroom_1_b, Player(0), 10.00, 3.00, 0.00, 3.00)
    //  
    call ShowUnitHide(udg_Unit_Doomhammer_Weapon)
    call SetUnitTimeScalePercent(udg_Unit_Doomhammer_1, 50.00)
    call SetUnitPositionLoc(udg_Unit_Doomhammer_Animation, udg_Point_Horny_Jail)
    call SetUnitPositionLocFacingBJ(udg_Unit_Doomhammer_1, udg_Point_Doomhammer_Start_1, 354.00)
    call WaitForSoundBJ(gg_snd_WC2ORCACT02Doomhammer7, 0.00)
    if ( (udg_Scene_2_Skipped == true) ) then // INLINED!!
        return
    else
        call DoNothing()
    endif
    //  
    call QueueUnitAnimationBJ(udg_Unit_Doomhammer_1, "cinematic listening one")
    call TriggerSleepAction(0.50)
    if ( (udg_Scene_2_Skipped == true) ) then // INLINED!!
        return
    else
        call DoNothing()
    endif
    //  
    call SetUnitTimeScalePercent(udg_Unit_Doomhammer_1, 100.00)
    call AddUnitAnimationPropertiesBJ(true, "cinematic", udg_Unit_Doomhammer_1)
    call IssuePointOrderLocBJ(udg_Unit_Doomhammer_1, "move", udg_Point_Tharbek_Goal_3)
    call TriggerSleepAction(1.00)
    if ( (udg_Scene_2_Skipped == true) ) then // INLINED!!
        return
    else
        call DoNothing()
    endif
    //  
    call CinematicFadeBJ(bj_CINEFADETYPE_FADEOUT, 1.50, "ReplaceableTextures\\CameraMasks\\Black_mask.blp", 0, 0, 0, 0)
    call TriggerSleepAction(2.00)
    if ( (udg_Scene_2_Skipped == true) ) then // INLINED!!
        return
    else
        call DoNothing()
    endif
    // ///
    // --------------------------------------------------------------------------------------------------------------------------------------------------------------
    call ConditionalTriggerExecute(gg_trg_Scene_2_Arrival)
