function InitTrig_Scene_1_Prep takes nothing returns nothing
    set gg_trg_Scene_1_Prep=CreateTrigger()
    call TriggerAddAction(gg_trg_Scene_1_Prep, function Trig_Scene_1_Prep_Actions)
endfunction

//===========================================================================
// Trigger: Scene 1 Skipped
//===========================================================================
function Trig_Scene_1_Skipped_Conditions takes nothing returns boolean
    if ( not ( udg_Scene_1_Playing == true ) ) then
        return false
    endif
    if ( not ( udg_Scene_1_Skipped == false ) ) then
        return false
    endif
    return true
endfunction

function Trig_Scene_1_Skipped_Actions takes nothing returns nothing
    call DisableTrigger(GetTriggeringTrigger())
