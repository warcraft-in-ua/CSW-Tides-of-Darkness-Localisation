    call SetSoundVolumeBJ(gg_snd_SoldiersLeatherArmorFoley, 45.00)
    call SetSoundVolumeBJ(gg_snd_WalkingGroup_Reverb, 30.00)
    call TriggerSleepAction(1.00)
    if ( (udg_Scene_2_Skipped == true) ) then // INLINED!!
