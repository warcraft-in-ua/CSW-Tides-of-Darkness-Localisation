    call StopSoundBJ(gg_snd_SoldiersLeatherArmorFoley, true)
    call StopSoundBJ(gg_snd_WalkingGroup_Reverb, true)
    call SetUnitPositionLocFacingLocBJ(udg_Unit_Tharbek, udg_Point_Tharbek_Start_2, udg_Point_Tharbek_Goal_2)
    call IssuePointOrderLocBJ(udg_Unit_Tharbek, "move", udg_Point_Tharbek_Goal_2)
    call TriggerSleepAction(4.00)
    if ( (udg_Scene_2_Skipped == true) ) then // INLINED!!
        return
    else
        call DoNothing()
    endif
    //  
    call IssuePointOrderLocBJ(udg_Unit_Tharbek, "move", udg_Point_Tharbek_Goal_3)
    call SetUnitLookAt(udg_Unit_Saurfang, "bone_head", udg_Unit_Tharbek, 0, 0, 90)
    call TriggerSleepAction(0.50)
    if ( (udg_Scene_2_Skipped == true) ) then // INLINED!!
        return
    else
        call DoNothing()
    endif
    //  
    call SetUnitLookAt(udg_Unit_Tharbek, "bone_head", udg_Unit_Doomhammer_Animation, 0, 0, 90)
    call TriggerSleepAction(2.00)
    if ( (udg_Scene_2_Skipped == true) ) then // INLINED!!
        return
    else
        call DoNothing()
    endif
    //  
    call IssueImmediateOrderBJ(udg_Unit_Tharbek, "stop")
