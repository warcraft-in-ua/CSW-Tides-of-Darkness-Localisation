//===========================================================================
// Trigger: Scene 1 Prep
//===========================================================================
function Trig_Scene_1_Prep_Actions takes nothing returns nothing
    call CinematicFadeBJ(bj_CINEFADETYPE_FADEOUT, 0.00, "ReplaceableTextures\\CameraMasks\\Black_mask.blp", 0, 0, 0, 0)
    call CinematicModeBJ(true, GetPlayersAll())
    call ClearMapMusicBJ()
    call EndThematicMusicBJ()
    call VolumeGroupSetVolumeBJ(SOUND_VOLUMEGROUP_FIRE, 0.00)
    call EnableDawnDusk(false)
    call SetThematicMusicVolumeBJ(0.00)
    //  
    set udg_Point_Horny_Jail=GetRectCenter(gg_rct_Horny_Jail)
    //  
    set udg_Unit_Doomhammer_Weapon=gg_unit_o00C_0023
    set udg_Unit_Doomhammer_1=gg_unit_O001_0068
    set udg_Unit_Doomhammer_Animation=gg_unit_O001_0125
    set udg_Unit_Saurfang=gg_unit_ogrk_0000
    //  
    set udg_Point_Doomhammer_Start_1=GetUnitLoc(udg_Unit_Doomhammer_1)
    //  
    // ///
    //  
    set udg_Scene_1_Playing=true
    set udg_Scene_1_Skipped=false
    //  
    call ShowDestructableBJ(false, gg_dest_B006_0908)
    call ShowDestructableBJ(false, gg_dest_B007_0909)
    //  
    set udg_Point_Doomhammer_Goal_1=GetRectCenter(gg_rct_Doomhammer_Goal_1)
    set udg_Point_Saurfang_Start_1=GetUnitLoc(udg_Unit_Saurfang)
    set udg_Point_Saurfang_Goal_1=GetRectCenter(gg_rct_Scene_1_Saurfang_Closer)
    set udg_Point_Goblin_Start_1=GetRectCenter(gg_rct_Goblin_Start_1)
    set udg_Point_Goblin_Start_1=OffsetLocation(GetRectCenter(gg_rct_Goblin_Start_1), 0, 5.00)
    set udg_Point_Goblin_Start_2=GetRectCenter(gg_rct_Goblin_Start_2)
    set udg_Point_Goblin_Start_3=GetRectCenter(gg_rct_Goblin_Start_3)
    set udg_Point_Goblin_Goal_1=GetRectCenter(gg_rct_Goblin_Goal_1)
    set udg_Point_Goblin_Goal_2=GetRectCenter(gg_rct_Goblin_Goal_2)
    set udg_Point_Hobbert_Start_1=GetRectCenter(gg_rct_Hobbert_Start_1)
    set udg_Point_Hobbert_Start_2=GetRectCenter(gg_rct_Hobbert_Start_2)
    set udg_Point_Hobbert_Start_3=GetRectCenter(gg_rct_Hobbert_Start_3)
    set udg_Point_Gobbert_Start_1=GetRectCenter(gg_rct_Gobbert_Start_1)
    set udg_Point_Gobbert_Start_2=GetRectCenter(gg_rct_Gobbert_Start_3)
    //  
    call CreateNUnitsAtLoc(1, 'n005', Player(0), udg_Point_Hobbert_Start_1, 175.00)
    set udg_Unit_Hobbert=GetLastCreatedUnit()
    //  
    call CreateNUnitsAtLoc(1, 'n006', Player(0), udg_Point_Gobbert_Start_1, 190.00)
    set udg_Unit_Gobbert=GetLastCreatedUnit()
    //  
    call CreateNUnitsAtLoc(1, 'N004', Player(0), udg_Point_Goblin_Start_1, 180.00)
    set udg_Unit_Steamwheedle=GetLastCreatedUnit()
    //  
    call AddUnitAnimationPropertiesBJ(true, "cinematic", udg_Unit_Doomhammer_1)
    call AddUnitAnimationPropertiesBJ(true, "cinematic", udg_Unit_Doomhammer_Animation)
    call AddUnitAnimationPropertiesBJ(true, "cinematic", udg_Unit_Steamwheedle)
    call AddUnitAnimationPropertiesBJ(true, "cinematic", udg_Unit_Saurfang)
    //  
    call SetUnitMoveSpeed(udg_Unit_Doomhammer_1, 100.00)
    call BlzSetUnitRealFieldBJ(udg_Unit_Doomhammer_1, UNIT_RF_ANIMATION_WALK_SPEED, 130.00)
    call SetUnitPathing(udg_Unit_Saurfang, false)
    call SetUnitMoveSpeed(udg_Unit_Saurfang, 100.00)
    call BlzSetUnitRealFieldBJ(udg_Unit_Saurfang, UNIT_RF_ANIMATION_WALK_SPEED, 110.00)
    call SetUnitMoveSpeed(udg_Unit_Steamwheedle, 100.00)
    call BlzSetUnitRealFieldBJ(udg_Unit_Steamwheedle, UNIT_RF_ANIMATION_WALK_SPEED, 75.00)
    call SetUnitMoveSpeed(udg_Unit_Gobbert, 100.00)
    call BlzSetUnitRealFieldBJ(udg_Unit_Gobbert, UNIT_RF_ANIMATION_WALK_SPEED, 100.00)
    call SetUnitMoveSpeed(udg_Unit_Hobbert, 100.00)
    call BlzSetUnitRealFieldBJ(udg_Unit_Hobbert, UNIT_RF_ANIMATION_WALK_SPEED, 100.00)
    //  
    call SetUnitBlendTimeBJ(udg_Unit_Doomhammer_1, 0.50)
    call SetUnitBlendTimeBJ(udg_Unit_Doomhammer_Animation, 5.00)
    call SetUnitBlendTimeBJ(udg_Unit_Steamwheedle, 0.50)
    call SetUnitBlendTimeBJ(udg_Unit_Gobbert, 0.50)
    call SetUnitBlendTimeBJ(udg_Unit_Hobbert, 0.50)
    call SetUnitBlendTimeBJ(udg_Unit_Saurfang, 0.50)
    //  
    call SetUnitAnimation(udg_Unit_Doomhammer_1, "cinematic sitting one")
    call SetUnitAnimation(udg_Unit_Doomhammer_Animation, "cinematic sitting one")
    call SetUnitAnimation(udg_Unit_Steamwheedle, "cinematic listening one")
    //  
    call SetDestructableAnimationBJ(gg_dest_B004_3182, "death")
    //  
    // ///
    //  
    call TriggerSleepAction(1.00)
    call EnableTrigger(gg_trg_Scene_1_Skipped)
    call ConditionalTriggerExecute(gg_trg_Scene_1_Intro_Zeppelin)
endfunction

