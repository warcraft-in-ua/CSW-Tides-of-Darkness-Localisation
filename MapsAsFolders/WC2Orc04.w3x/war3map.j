function Trig_MidgameCinematicCleanUp_Actions takes nothing returns nothing
    call DisableTrigger(GetTriggeringTrigger())
    call CinematicFadeBJ(bj_CINEFADETYPE_FADEOUT, 2, "ReplaceableTextures\\CameraMasks\\Black_mask.blp", 0, 0, 0, 0)
    call TriggerSleepAction(2)
    set udg_MidgameCinematic=false
    call RemoveUnit(udg_FakeMeryn)
    call ConditionalTriggerExecute(gg_trg_RemoveFakeActors)
    set bj_forLoopAIndex=1
    set bj_forLoopAIndexEnd=5
    loop
        exitwhen bj_forLoopAIndex > bj_forLoopAIndexEnd
        call RemoveUnit(udg_CinGrunt[GetForLoopIndexA()])
        set bj_forLoopAIndex=bj_forLoopAIndex + 1
    endloop
    call DestroyEffectBJ(udg_Nets)
    set bj_forLoopBIndex=1
    set bj_forLoopBIndexEnd=3
    loop
        exitwhen bj_forLoopBIndex > bj_forLoopBIndexEnd
        call DestroyEffectBJ(udg_Bloodsplats[GetForLoopIndexB()])
        set bj_forLoopBIndex=bj_forLoopBIndex + 1
    endloop
    call SetPlayerAllianceStateBJ(Player(0), Player(1), bj_ALLIANCE_UNALLIED)
    call SetPlayerAllianceStateBJ(Player(1), Player(0), bj_ALLIANCE_UNALLIED)
    call SetPlayerAllianceStateBJ(Player(2), Player(0), bj_ALLIANCE_UNALLIED)
    call SetPlayerAllianceStateBJ(Player(0), Player(2), bj_ALLIANCE_UNALLIED)
    if ( Trig_MidgameCinematicCleanUp_Func014C() ) then
        call StopSoundBJ(gg_snd_WC2Orc04Rend7, false)
        call StopSoundBJ(gg_snd_WC2Orc04Rend8, false)
        call StopSoundBJ(gg_snd_WC2Orc04Meryn18, false)
        call StopSoundBJ(gg_snd_WC2Orc04Meryn19, false)
    else
    endif
    // //
    call CreateItemLoc('mcou', GetRectCenter(gg_rct_MerynIntro05))
    // //
    call ForGroupBJ(udg_RefugeesStormwind, function Trig_MidgameCinematicCleanUp_Func018002)
    call ForGroupBJ(udg_HiddenTroops, function Trig_MidgameCinematicCleanUp_Func019A)
    call VolumeGroupResetBJ()
    call ResetToGameCameraForPlayer(Player(0), 0)
    call ConditionalTriggerExecute(gg_trg_CameraRestore)
    call CinematicFadeBJ(bj_CINEFADETYPE_FADEIN, 2, "ReplaceableTextures\\CameraMasks\\Black_mask.blp", 0, 0, 0, 0)
    call TriggerSleepAction(2)
    call ConditionalTriggerExecute(gg_trg_SelectionRestore)
    call CinematicModeBJ(false, GetPlayersAll())
    call ConditionalTriggerExecute(gg_trg_OilTextShow)
endfunction

