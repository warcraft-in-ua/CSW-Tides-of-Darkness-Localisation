function Trig_MidgameCinematicQ_Actions takes nothing returns nothing
    call DisableTrigger(GetTriggeringTrigger())
    call ConditionalTriggerExecute(gg_trg_CameraStore)
    call CinematicModeBJ(true, bj_FORCE_PLAYER[0])
    call ConditionalTriggerExecute(gg_trg_OilTextHide)
    call ConditionalTriggerExecute(gg_trg_SelectionStore)
    call CinematicFadeBJ(bj_CINEFADETYPE_FADEOUT, 2, "ReplaceableTextures\\CameraMasks\\Black_mask.blp", 0, 0, 0, 0)
    call TriggerSleepAction(2)
    // SetUp
    call CameraSetupApplyForPlayer(true, gg_cam_RendKillsMeryn01, Player(0), 0)
    call SetCineModeVolumeGroupsBJ()
    call SetPlayerAllianceStateBJ(Player(0), Player(1), bj_ALLIANCE_NEUTRAL)
    call SetPlayerAllianceStateBJ(Player(1), Player(0), bj_ALLIANCE_NEUTRAL)
    call SetPlayerAllianceStateBJ(Player(2), Player(0), bj_ALLIANCE_NEUTRAL)
    call SetPlayerAllianceStateBJ(Player(0), Player(2), bj_ALLIANCE_NEUTRAL)
    call SetPlayerAllianceStateBJ(Player(1), Player(20), bj_ALLIANCE_NEUTRAL)
    call SetPlayerAllianceStateBJ(Player(20), Player(1), bj_ALLIANCE_NEUTRAL)
    call ForGroupBJ(udg_RefugeesStormwind, function Trig_MidgameCinematicQ_Func017002)
    if ( Trig_MidgameCinematicQ_Func018C() ) then
        call ShowUnitHide(udg_Meryn)
    else
    endif
    call SetDoodadAnimationRectBJ("decay", 'NOfp', gg_rct_Pitfitre)
    call ForGroupBJ(GetUnitsInRectOfPlayer(gg_rct_RefugeeCamp, Player(0)), function Trig_MidgameCinematicQ_Func020A)
    call CreateNUnitsAtLocFacingLocBJ(1, 'H62D', Player(1), GetRectCenter(gg_rct_MerynIntro05), GetRectCenter(gg_rct_Pitfitre))
    set udg_FakeMeryn=GetLastCreatedUnit()
    call AddUnitAnimationPropertiesBJ(true, "Cinematic", udg_FakeMeryn)
    call SetUnitAnimation(udg_FakeMeryn, "Cinematic Injured One")
    call ConditionalTriggerExecute(gg_trg_CreateFakeActors)
    call AddSpecialEffectTargetUnitBJ("origin", udg_FakeMeryn, "Abilities\\Spells\\Orc\\Ensnare\\ensnareTarget.mdl")
    set udg_Nets=GetLastCreatedEffectBJ()
    call SetUnitPositionLocFacingLocBJ(udg_FakeRend, GetRectCenter(gg_rct_RendApproachesMeryn), GetRectCenter(gg_rct_RendKillsMeryn))
    call CreateNUnitsAtLocFacingLocBJ(1, 'o60G', Player(0), GetRectCenter(gg_rct_Grunt01), GetRectCenter(gg_rct_MerynIntro05))
    set udg_CinGrunt[1]=GetLastCreatedUnit()
    call CreateNUnitsAtLocFacingLocBJ(1, 'o60G', Player(0), GetRectCenter(gg_rct_Grunt02), GetRectCenter(gg_rct_MerynIntro05))
    set udg_CinGrunt[2]=GetLastCreatedUnit()
    call CreateNUnitsAtLocFacingLocBJ(1, 'o60G', Player(0), GetRectCenter(gg_rct_Grunt03), GetRectCenter(gg_rct_MerynIntro05))
    set udg_CinGrunt[3]=GetLastCreatedUnit()
    call CreateNUnitsAtLocFacingLocBJ(1, 'o60G', Player(0), GetRectCenter(gg_rct_Grunt04), GetRectCenter(gg_rct_MerynIntro05))
    set udg_CinGrunt[4]=GetLastCreatedUnit()
    call CreateNUnitsAtLocFacingLocBJ(1, 'o60G', Player(0), GetRectCenter(gg_rct_Grunt05), GetRectCenter(gg_rct_MerynIntro05))
    set udg_CinGrunt[5]=GetLastCreatedUnit()
    // Start
    call CinematicFadeBJ(bj_CINEFADETYPE_FADEIN, 2, "ReplaceableTextures\\CameraMasks\\Black_mask.blp", 0, 0, 0, 0)
    call PlayThematicMusicBJ("HumanDefeat")
    set udg_MidgameCinematic=true
    call IssuePointOrderLocBJ(udg_FakeRend, "move", GetRectCenter(gg_rct_RendKillsMeryn))
    call PlaySoundOnUnitBJ(gg_snd_CS_Prologue2_Foley1, 100.00, udg_FakeRend)
    if ( Trig_MidgameCinematicQ_Func045C() ) then
        set udg_MidgameCameraWaitFork=GetSoundDurationBJ(gg_snd_WC2Orc04Meryn18)
    else
        set udg_MidgameCameraWaitFork=0.00
    endif
    call CameraSetupApplyForPlayerSmooth(true, gg_cam_RendKillsMeryn02, Player(0), ( ( DistanceBetweenPoints(GetUnitLoc(udg_FakeRend), GetRectCenter(gg_rct_RendKillsMeryn)) / GetUnitMoveSpeed(udg_FakeRend) ) + ( GetSoundDurationBJ(gg_snd_WC2Orc04Rend7) + ( GetSoundDurationBJ(gg_snd_WC2Orc04Rend8) + ( udg_MidgameCameraWaitFork + ( GetSoundDurationBJ(gg_snd_WC2Orc04Meryn19) + 1 ) ) ) ) ), 0.00, 10.00, 100.00)
    call TriggerSleepAction(( ( DistanceBetweenPoints(GetUnitLoc(udg_FakeRend), GetRectCenter(gg_rct_RendKillsMeryn)) / GetUnitMoveSpeed(udg_FakeRend) ) - 1 ))
    if ( (udg_MidgameCinematicSkipped == true) ) then // INLINED!!
        return
    else
        call DoNothing()
    endif
    call PlayDialogueFromSpeakerEx(bj_FORCE_PLAYER[0], udg_FakeRend, 'O617', gg_snd_WC2Orc04Rend7, bj_TIMETYPE_ADD, 0, false)
    call TriggerSleepAction(1.00)
    if ( (udg_MidgameCinematicSkipped == true) ) then // INLINED!!
        return
    else
        call DoNothing()
    endif
    call SetUnitAnimationWithRarity(udg_FakeRend, "stand", RARITY_FREQUENT)
    call SetUnitFacingToFaceLocTimed(udg_FakeRend, GetRectCenter(gg_rct_Grunt04), 0.50)
    call TriggerSleepAction(( GetSoundDurationBJ(gg_snd_WC2Orc04Rend7) - 1 ))
    if ( (udg_MidgameCinematicSkipped == true) ) then // INLINED!!
        return
    else
        call DoNothing()
    endif
    call AddSpecialEffectTargetUnitBJ("origin", udg_FakeMeryn, "Objects\\Spawnmodels\\Human\\HumanBlood\\HumanBloodSmall0.mdl")
    set udg_Bloodsplats[1]=GetLastCreatedEffectBJ()
    if ( Trig_MidgameCinematicQ_Func059C() ) then
        call PlayDialogueFromSpeakerEx(bj_FORCE_PLAYER[0], udg_FakeMeryn, 'H62D', gg_snd_WC2Orc04Meryn18, bj_TIMETYPE_ADD, 0, true)
        if ( (udg_MidgameCinematicSkipped == true) ) then // INLINED!!
            return
        else
            call DoNothing()
        endif
    else
    endif
    call PlayDialogueFromSpeakerEx(bj_FORCE_PLAYER[0], udg_FakeMeryn, 'H62D', gg_snd_WC2Orc04Meryn19, bj_TIMETYPE_ADD, 0, true)
    if ( (udg_MidgameCinematicSkipped == true) ) then // INLINED!!
        return
    else
        call DoNothing()
    endif
    call AddSpecialEffectTargetUnitBJ("origin", udg_FakeMeryn, "Objects\\Spawnmodels\\Human\\HumanBlood\\HumanBloodSmall1.mdl")
    set udg_Bloodsplats[2]=GetLastCreatedEffectBJ()
    call PlayDialogueFromSpeakerEx(bj_FORCE_PLAYER[0], udg_FakeRend, 'O617', gg_snd_WC2Orc04Rend8, bj_TIMETYPE_ADD, 0, true)
    if ( (udg_MidgameCinematicSkipped == true) ) then // INLINED!!
        return
    else
        call DoNothing()
    endif
    call ConditionalTriggerExecute(gg_trg_MidgameCinRendAttackAnimation)
    call TriggerSleepAction(0.50)
    if ( (udg_MidgameCinematicSkipped == true) ) then // INLINED!!
        return
    else
        call DoNothing()
    endif
    call PlaySoundOnUnitBJ(gg_snd_MetalHeavySliceFlesh2, 100, udg_FakeRend)
    call AddSpecialEffectTargetUnitBJ("origin", udg_FakeMeryn, "Objects\\Spawnmodels\\Human\\HumanBlood\\HumanBloodLarge0.mdl")
    set udg_Bloodsplats[3]=GetLastCreatedEffectBJ()
    call PlaySoundOnUnitBJ(gg_snd_FootmanDeath2, 100, udg_FakeMeryn)
    call SetUnitAnimation(udg_FakeMeryn, "Cinematic Die")
    call DestroyEffectBJ(udg_Nets)
    call TriggerSleepAction(3.50)
    if ( (udg_MidgameCinematicSkipped == true) ) then // INLINED!!
        return
    else
        call DoNothing()
    endif
    call ConditionalTriggerExecute(gg_trg_MidgameCinematicCleanUp)
endfunction

