        if distCheck <= distance then
            set distance=distCheck
            set OilHarvest___tankerTarget=s__OilDepot_source[depot]
        endif
        set link=(LoadInteger(Table___ht, (((link))), (- 2))) // INLINED!!
    endloop
    return OilHarvest___tankerTarget
endfunction






