//===========================================================================
// Trigger: Boss Phase 1 Warstomp
//===========================================================================
function Trig_Boss_Phase_1_Warstomp_Conditions takes nothing returns boolean
    if ( not ( udg_Boss_Phase1 == true ) ) then
        return false
    endif
    if ( not ( GetAttacker() == udg_Boss_ChoGall ) ) then
        return false
    endif
    if ( not ( R2I(BlzGetUnitAbilityCooldownRemaining(udg_Boss_ChoGall, 'A007')) == 0 ) ) then
        return false
    endif
    return true
endfunction

function Trig_Boss_Phase_1_Warstomp_Func002C takes nothing returns boolean
    if ( not ( udg_Boss_Phase1_Chance_Warstomp_D == 1 ) ) then
        return false
    endif
    return true
endfunction

function Trig_Boss_Phase_1_Warstomp_Actions takes nothing returns nothing
    set udg_Boss_Phase1_Chance_Warstomp_D=GetRandomInt(1, 2)
    if ( Trig_Boss_Phase_1_Warstomp_Func002C() ) then
        call PlaySoundBJ(gg_snd_WC2Orc12ChoGall8)
        call DisplayTextToForce(GetPlayersAll(), "TRIGSTR_3177")
    else
        call PlaySoundBJ(gg_snd_WC2Orc12ChoGall9)
        call DisplayTextToForce(GetPlayersAll(), "TRIGSTR_3172")
    endif
    //  
    call BlzStartUnitAbilityCooldown(udg_Boss_ChoGall, 'A007', 15.00)
    call BlzSetUnitWeaponBooleanFieldBJ(udg_Boss_ChoGall, UNIT_WEAPON_BF_ATTACKS_ENABLED, 0, false)
    call IssueImmediateOrderBJ(udg_Boss_ChoGall, "stop")
    call BlzUnitInterruptAttack(udg_Boss_ChoGall)
    call SetUnitTimeScalePercent(udg_Boss_ChoGall, 15.00)
    call SetUnitAnimation(udg_Boss_ChoGall, "attack slam")
    //  
    set udg_Boss_Phase1_TP_Warstomp=GetUnitLoc(udg_Boss_ChoGall)
    call AddSpecialEffectLocBJ(udg_Boss_Phase1_TP_Warstomp, "Doodads\\Cinematic\\ToS\\BossArea.mdx")
    set udg_Boss_Phase1_Effect_Warstomp=GetLastCreatedEffectBJ()
    call BlzSetSpecialEffectScale(udg_Boss_Phase1_Effect_Warstomp, 4.00)
    call BlzSetSpecialEffectColor(udg_Boss_Phase1_Effect_Warstomp, 255, 100, 0)
    call BlzSetSpecialEffectTimeScale(udg_Boss_Phase1_Effect_Warstomp, 0.50)
    call TriggerSleepAction(1.00)
    call CreateNUnitsAtLoc(1, 'u005', Player(3), udg_Boss_Phase1_TP_Warstomp, bj_UNIT_FACING)
    set udg_Boss_Phase1_U_Warstomp=GetLastCreatedUnit()
    call UnitApplyTimedLifeBJ(2.00, 'BTLF', udg_Boss_Phase1_U_Warstomp)
    call UnitAddAbilityBJ('A00E', udg_Boss_Phase1_U_Warstomp)
    call SetUnitAbilityLevelSwapped('A00E', GetTriggerUnit(), 1)
    call IssueImmediateOrderBJ(udg_Boss_Phase1_U_Warstomp, "stomp")
    //  
    call TriggerSleepAction(1.20)
    call SetUnitTimeScalePercent(udg_Boss_ChoGall, 75.00)
    call AddSpecialEffectLocBJ(udg_Boss_Phase1_TP_Warstomp, "Abilities\\Spells\\Orc\\WarStomp\\WarStompCaster.mdl")
    set udg_Boss_Phase1_Effect_Warstomp2=GetLastCreatedEffectBJ()
    call DestroyEffectBJ(udg_Boss_Phase1_Effect_Warstomp2)
    //  
    call BlzSetUnitWeaponBooleanFieldBJ(udg_Boss_ChoGall, UNIT_WEAPON_BF_ATTACKS_ENABLED, 0, true)
    call DestroyEffectBJ(udg_Boss_Phase1_Effect_Warstomp)
    call RemoveLocation(udg_Boss_Phase1_TP_Warstomp)
endfunction

