//===========================================================================
// Trigger: Boss Phase 2 Intermission
//===========================================================================
function Trig_Boss_Phase_2_Intermission_Actions takes nothing returns nothing
    call DisableTrigger(GetTriggeringTrigger())
    //  
    // Cleanup
    //  
    call DisableTrigger(gg_trg_Boss_Phase_1_Warstomp)
    call DisableTrigger(gg_trg_Boss_Phase_1_Rune)
    call RemoveUnit(udg_Boss_Phase1_U_Warstomp)
    call DestroyEffectBJ(udg_Boss_Phase1_Effect_Warstomp2)
    //  
    call KillUnit(udg_Boss_Phase1_U_Orb_NW)
    call KillUnit(udg_Boss_Phase1_U_Orb_NE)
    call KillUnit(udg_Boss_Phase1_U_Orb_S)
    //  
    // Cho'gall Setup
    //  
    call SetUnitTimeScalePercent(udg_Boss_ChoGall, 75.00)
    call SetUnitInvulnerable(udg_Boss_ChoGall, true)
    call IssueImmediateOrderBJ(udg_Boss_ChoGall, "stop")
    call BlzUnitInterruptAttack(udg_Boss_ChoGall)
    call BlzSetUnitWeaponBooleanFieldBJ(udg_Boss_ChoGall, UNIT_WEAPON_BF_ATTACKS_ENABLED, 0, false)
    call UnitRemoveAbilityBJ('A007', udg_Boss_ChoGall)
    call TriggerSleepAction(0.50)
    //  
    // CC Heroes
    //  
    call PlaySoundBJ(gg_snd_WC2Orc12ChoGall4)
    call DisplayTextToForce(GetPlayersAll(), "TRIGSTR_3196")
    call SetUnitAnimation(udg_Boss_ChoGall, "spell throw")
    call QueueUnitAnimationBJ(udg_Boss_ChoGall, "stand")
    call ConditionalTriggerExecute(gg_trg_Boss_Phase_2_CC_Heroes)
