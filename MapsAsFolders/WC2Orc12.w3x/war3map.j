function Trig_Hermit_2_Cleanup_Actions takes nothing returns nothing
    call CameraSetFocalDistance(0.00)
    call StopSoundBJ(gg_snd_ACT_IV_CinematicTheme2, true)
    call SetMusicVolumeBJ(100.00)
    // Restore Abilities
    call SetPlayerAbilityAvailableBJ(true, 'Ashm', Player(0))
    // Remove Fake Maiev
    call RemoveUnit(udg_ZuluhedCinematic)
    // Unhide Units
    call ForGroupBJ(udg_HiddenUnits, function Trig_Hermit_2_Cleanup_Func009002)
    call EnumItemsInRectBJ(GetPlayableMapRect(), function Trig_Hermit_2_Cleanup_Func010002)
    // Position Units
    call SetUnitPositionLocFacingLocBJ(udg_Drakthul, GetRectCenter(gg_rct_Hermit2_Drakthul01), GetUnitLoc(udg_Zuluhed))
    // Unpause Units
    // Pause Time Of Day
    call UseTimeOfDayBJ(true)
    // Re-enable XP Gain
    call SuspendHeroXPBJ(true, udg_Zuluhed)
    // Return Heroes
    call SetUnitPositionLoc(udg_Zuluhed, udg_ZuluhedReturnSpot)
    // Cleanup Items
    call RemoveItem(udg_SkeletalArtifact)
    call UnitAddItemByIdSwapped('ciri', udg_Zuluhed)
    // Restore Selection
    call SelectGroupBJ(udg_SelectedUnits)
    // Reset Sky
    call SetSkyModel(null)
    // Reset Fog
    call ResetTerrainFogBJ()
    // Reset Camera
    call CameraResetSmoothingFactorBJ()
    call ResetToGameCameraForPlayer(Player(0), 0)
    call PanCameraToTimedLocForPlayer(Player(0), udg_CameraReturnPoint, 0)
    // Run Drakthul Goes Home
    call TriggerSleepAction(3.00)
    call SetUnitPathing(udg_Drakthul, false)
    call IssuePointOrderLocBJ(udg_Drakthul, "move", GetRectCenter(gg_rct_Hermit2_Drakthul02))
    call ConditionalTriggerExecute(gg_trg_Quest_Book_Completed)
endfunction

