    if ( Trig_Boss_Prep_Func037C() ) then
        call SetUnitPositionLocFacingLocBJ(udg_Rend, GetRectCenter(gg_rct_Boss_Rend_Start), GetRectCenter(gg_rct_Boss_Phase_1_Center))
