    else
        if ( Trig_Boss_Phase_2_Func034Func002C() ) then
            call BlzSetUnitArmor(udg_Boss_ChoGall, 10.00)
        else
            if ( Trig_Boss_Phase_2_Func034Func002Func002C() ) then
                call BlzSetUnitArmor(udg_Boss_ChoGall, 15.00)
            else
            endif
        endif
    endif
    call BlzSetUnitMaxHP(udg_Boss_ChoGall, 10000)
    call SetUnitLifePercentBJ(udg_Boss_ChoGall, 65.00)
    call BlzSetUnitArmor(udg_Boss_ChoGall, 15.00)
    call SetUnitTimeScalePercent(udg_Boss_ChoGall, 75.00)
    //  
    call UnitAddAbilityBJ('A65W', udg_Boss_ChoGall)
    // --------------------------------------------------------------------------------------------------------------------
    call EnableTrigger(gg_trg_Boss_Phase_3_Check_HP)
    call ConditionalTriggerExecute(gg_trg_Boss_Phase_2_Ogres)
    call SetSoundVolumeBJ(gg_snd_ACT_IV_BossThemePhase1, 35.00)
    call TransmissionFromUnitWithNameBJ(GetPlayersAll(), udg_Maim, "TRIGSTR_3314", gg_snd_WC2Orc12Rend8, "TRIGSTR_3315", bj_TIMETYPE_SUB, 1.00, true)
    call TransmissionFromUnitWithNameBJ(GetPlayersAll(), udg_Boss_Zuluhed, "TRIGSTR_3318", gg_snd_WC2Orc12Zuluhed12, "TRIGSTR_3319", bj_TIMETYPE_ADD, 0.00, true)
    call TransmissionFromUnitWithNameBJ(GetPlayersAll(), udg_Rend, "TRIGSTR_3316", gg_snd_WC2Orc12Rend9, "TRIGSTR_3317", bj_TIMETYPE_ADD, 0, true)
    call TriggerSleepAction(10.00)
    call SetSoundVolumeBJ(gg_snd_ACT_IV_BossThemePhase1, 65.00)
