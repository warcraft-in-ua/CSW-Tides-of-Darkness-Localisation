                set udg_Boss_Orb_Damage=100
                set udg_Boss_Ogre_Damage=200.00
                set udg_Boss_Twilight_Damage=500.00
            else
            endif
        endif
    endif
    call SetUnitLifePercentBJ(udg_Boss_ChoGall, 100)
    call SetUnitManaPercentBJ(udg_Boss_ChoGall, 100)
    //  
    call ShowUnitHide(udg_Guldan)
    call ShowUnitHide(udg_Zuluhed)
    call SetUnitOwner(udg_Zuluhed, Player(21), false)
    call ShowUnitHide(udg_Utok)
    call PauseAllUnitsBJ(true)
    call PauseUnitBJ(false, udg_Boss_ChoGall)
    call PauseUnitBJ(false, udg_Rend)
    call PauseUnitBJ(false, udg_Maim)
    //  
    //  
    // Heroes
