    call DestroyEffectBJ(GetLastCreatedEffectBJ())
    //  
    call SetUnitPositionLocFacingLocBJ(udg_Maim, GetRectCenter(gg_rct_Boss_Maim_Phase_2), GetRectCenter(gg_rct_Boss_Phase_2_Center))
    call AddSpecialEffectTargetUnitBJ("origin", udg_Maim, "Abilities\\Spells\\Orc\\ChogallSpells\\VoidTeleportTarget.mdx")
    call DestroyEffectBJ(GetLastCreatedEffectBJ())
    call TriggerSleepAction(1.00)
    //  
    // Cho'gall Setup
    //  
    call BlzSetUnitWeaponBooleanFieldBJ(udg_Boss_ChoGall, UNIT_WEAPON_BF_ATTACKS_ENABLED, 0, true)
    call BlzSetUnitWeaponBooleanFieldBJ(udg_Boss_ChoGall, UNIT_WEAPON_BF_ATTACK_SHOW_UI, 0, true)
    call SetUnitInvulnerable(udg_Boss_ChoGall, false)
