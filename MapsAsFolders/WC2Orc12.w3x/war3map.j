    if (OilHarvest___playerOil[GetPlayerId((Player(0)))]) < 400 then // INLINED!!
    call PlaySoundBJ(gg_snd_GruntNoOil1)
    call DisplayTextToForce(bj_FORCE_PLAYER[0], "TRIGSTR_446")
