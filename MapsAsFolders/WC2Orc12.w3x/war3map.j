function Trig_Boss_Phase_3_Intermission_Actions takes nothing returns nothing
    call DisableTrigger(GetTriggeringTrigger())
    //  
    // Cleanup
    //  
    call StopSoundBJ(gg_snd_ACT_IV_BossThemePhase1, true)
    call KillSoundWhenDoneBJ(gg_snd_ACT_IV_BossThemePhase1)
    call PlaySoundBJ(gg_snd_ACT_IV_BossThemePhase3)
    call PlaySoundBJ(gg_snd_ACT_IV_BossThemePhase301)
    call SetSoundVolumeBJ(gg_snd_ACT_IV_BossThemePhase301, 75.00)
    //  
    call DisableTrigger(gg_trg_Boss_Phase_2_Runes)
    if ( Trig_Boss_Phase_3_Intermission_Func012C() ) then
        call AddSpecialEffectTargetUnitBJ("origin", udg_Boss_Phase2_U_Ogre_N, "Abilities\\Spells\\Human\\MassTeleport\\MassTeleportTarget.mdl")
        call DestroyEffectBJ(GetLastCreatedEffectBJ())
        call RemoveUnit(udg_Boss_Phase2_U_Ogre_N)
    else
    endif
    if ( Trig_Boss_Phase_3_Intermission_Func013C() ) then
        call AddSpecialEffectTargetUnitBJ("origin", udg_Boss_Phase2_U_Ogre_E, "Abilities\\Spells\\Human\\MassTeleport\\MassTeleportTarget.mdl")
        call DestroyEffectBJ(GetLastCreatedEffectBJ())
        call RemoveUnit(udg_Boss_Phase2_U_Ogre_E)
    else
    endif
    if ( Trig_Boss_Phase_3_Intermission_Func014C() ) then
        call AddSpecialEffectTargetUnitBJ("origin", udg_Boss_Phase2_U_Ogre_W, "Abilities\\Spells\\Human\\MassTeleport\\MassTeleportTarget.mdl")
        call DestroyEffectBJ(GetLastCreatedEffectBJ())
        call RemoveUnit(udg_Boss_Phase2_U_Ogre_W)
    else
    endif
    set udg_Boss_Phase2_N=false
    set udg_Boss_Phase2_E=false
    set udg_Boss_Phase2_W=false
    //  
    // Cho'gall Setup
    //  
    call SetUnitTimeScalePercent(udg_Boss_ChoGall, 75.00)
    call SetUnitInvulnerable(udg_Boss_ChoGall, true)
    call IssueImmediateOrderBJ(udg_Boss_ChoGall, "stop")
    call BlzUnitInterruptAttack(udg_Boss_ChoGall)
    call BlzSetUnitWeaponBooleanFieldBJ(udg_Boss_ChoGall, UNIT_WEAPON_BF_ATTACKS_ENABLED, 0, false)
    call UnitRemoveAbilityBJ('A65W', udg_Boss_ChoGall)
    call TriggerSleepAction(0.50)
    //  
    // CC Heroes
    //  
    call PlaySoundBJ(gg_snd_WC2Orc12ChoGall10)
    call DisplayTextToForce(GetPlayersAll(), "TRIGSTR_3201")
    call SetUnitAnimation(udg_Boss_ChoGall, "spell throw")
    call QueueUnitAnimationBJ(udg_Boss_ChoGall, "stand")
    call ConditionalTriggerExecute(gg_trg_Boss_Phase_3_CC_Heroes)
    call TriggerSleepAction(0.50)
    //  
    // Cho'gall Teleport
    //  
    call SetUnitFacingToFaceLocTimed(udg_Boss_ChoGall, GetRectCenter(gg_rct_Boss_Phase_3_Center), 0.50)
    call TriggerSleepAction(0.50)
    call SetUnitAnimation(udg_Boss_ChoGall, "stand ready")
    call TriggerSleepAction(5.50)
    call SetUnitAnimation(udg_Boss_ChoGall, "spell throw")
    call QueueUnitAnimationBJ(udg_Boss_ChoGall, "stand ready")
    call ConditionalTriggerExecute(gg_trg_Boss_Phase_3_Chogall_Teleport)
endfunction

