    if ( Trig_Boss_Prep_Func038C() ) then
        call SetUnitPositionLocFacingLocBJ(udg_Maim, GetRectCenter(gg_rct_Boss_Maim_Start), GetRectCenter(gg_rct_Boss_Phase_1_Center))
