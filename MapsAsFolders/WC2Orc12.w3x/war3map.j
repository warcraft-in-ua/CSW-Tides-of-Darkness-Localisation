    set gg_trg_Boss_Phase_1_Warstomp=CreateTrigger()
    call DisableTrigger(gg_trg_Boss_Phase_1_Warstomp)
    call TriggerRegisterAnyUnitEventBJ(gg_trg_Boss_Phase_1_Warstomp, EVENT_PLAYER_UNIT_ATTACKED)
    call TriggerAddCondition(gg_trg_Boss_Phase_1_Warstomp, Condition(function Trig_Boss_Phase_1_Warstomp_Conditions))
    call TriggerAddAction(gg_trg_Boss_Phase_1_Warstomp, function Trig_Boss_Phase_1_Warstomp_Actions)
endfunction

