                if GetPlayerRace(s__OilDisplay_owner[s__OilPatch_display[this]]) == RACE_ORC then
                    call PlaySound(OilHarvest___LOW_WARNING_ORC)
                elseif GetPlayerRace(s__OilDisplay_owner[s__OilPatch_display[this]]) == RACE_HUMAN then
