            set udg_Guldan_TempPoint_Q=GetUnitLoc(GetTriggerUnit())
            // Dummy (Spells)
            call CreateNUnitsAtLoc(1, 'u004', GetOwningPlayer(GetTriggerUnit()), udg_Guldan_TempPoint_Q, bj_UNIT_FACING)
            set udg_Guldan_Dummy_Q=GetLastCreatedUnit()
