function Trig_CityHouseDestroyed_Actions takes nothing returns nothing
    // Sceams
    // //
    set udg_ScreamType=GetRandomInt(1, 4)
    // //
    if ( Trig_CityHouseDestroyed_Func005C() ) then
        call PlaySoundBJ(gg_snd_ebonhold_crowdscream1)
    else
        if ( Trig_CityHouseDestroyed_Func005Func003C() ) then
            call PlaySoundBJ(gg_snd_ebonhold_crowdscream2)
        else
            if ( Trig_CityHouseDestroyed_Func005Func003Func003C() ) then
                call PlaySoundBJ(gg_snd_ebonhold_crowdscream3)
            else
                call PlaySoundBJ(gg_snd_ebonhold_crowdscream4)
            endif
        endif
    endif
    // //
    // Effects
    // //
    call AddSpecialEffectLocBJ(GetRandomLocInRect(RectFromCenterSizeBJ(GetUnitLoc(GetDyingUnit()), 128.00, 128.00)), "Environment\\LargeBuildingFire\\LargeBuildingFire0.mdl")
    call AddSpecialEffectLocBJ(GetRandomLocInRect(RectFromCenterSizeBJ(GetUnitLoc(GetDyingUnit()), 128.00, 128.00)), "Environment\\LargeBuildingFire\\LargeBuildingFire0.mdl")
    call CreateUbersplatBJ(GetUnitLoc(GetDyingUnit()), "DHLB", 100, 100, 100, 0, true, false)
    set udg_UberSplatArray=( udg_UberSplatArray + 1 )
    set udg_UberSplat[udg_UberSplatArray]=GetLastCreatedUbersplat()
    call SetUbersplatRenderAlways(udg_UberSplat[udg_UberSplatArray], true)
    call ShowUbersplatBJ(true, udg_UberSplat[udg_UberSplatArray])
    // //
    // CitizenAmount Roll
    // //
    set udg_CityzenSpawnAmoun=GetRandomInt(1, 3)
    // //
    // Spawn
    // //
    set bj_forLoopAIndex=1
    set bj_forLoopAIndexEnd=udg_CityzenSpawnAmoun
    loop
        exitwhen bj_forLoopAIndex > bj_forLoopAIndexEnd
        // CitizenType Roll
        // //
        set udg_CityzenSpawnType=GetRandomInt(1, 14)
        // //
        if ( Trig_CityHouseDestroyed_Func025Func005C() ) then
            call CreateNUnitsAtLoc(1, 'n00Z', Player(8), GetUnitLoc(GetDyingUnit()), GetRandomDirectionDeg())
        else
            if ( Trig_CityHouseDestroyed_Func025Func005Func001C() ) then
                call CreateNUnitsAtLoc(1, 'n010', Player(8), GetUnitLoc(GetDyingUnit()), GetRandomDirectionDeg())
            else
                if ( Trig_CityHouseDestroyed_Func025Func005Func001Func001C() ) then
                    call CreateNUnitsAtLoc(1, 'n011', Player(8), GetUnitLoc(GetDyingUnit()), GetRandomDirectionDeg())
                else
                    if ( Trig_CityHouseDestroyed_Func025Func005Func001Func001Func001C() ) then
                        call CreateNUnitsAtLoc(1, 'n012', Player(8), GetUnitLoc(GetDyingUnit()), GetRandomDirectionDeg())
                    else
                        if ( Trig_CityHouseDestroyed_Func025Func005Func001Func001Func001Func001C() ) then
                            call CreateNUnitsAtLoc(1, 'n013', Player(8), GetUnitLoc(GetDyingUnit()), GetRandomDirectionDeg())
                        else
                            if ( Trig_CityHouseDestroyed_Func025Func005Func001Func001Func001Func001Func001C() ) then
                                call CreateNUnitsAtLoc(1, 'n00S', Player(8), GetUnitLoc(GetDyingUnit()), GetRandomDirectionDeg())
