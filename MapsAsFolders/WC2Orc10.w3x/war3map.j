function Trig_MainQuestStratholmeDiscovered_Actions takes nothing returns nothing
    call DisableTrigger(GetTriggeringTrigger())
    // //
    call SetDestructableInvulnerableBJ(gg_dest_LTg2_0086, false)
    call SetDestructableInvulnerableBJ(gg_dest_LTg4_0087, false)
    // //
    call QuestSetDiscoveredBJ(udg_MainQuestStratholme, true)
    // //
    call TriggerSleepAction(bj_QUEUE_DELAY_QUEST)
    // //
    call QuestMessageBJ(GetPlayersAll(), bj_QUESTMESSAGE_DISCOVERED, "TRIGSTR_329")
    // //
    call CreateFogModifierRadiusLocBJ(true, Player(0), FOG_OF_WAR_VISIBLE, GetRectCenter(gg_rct_StratholmeQuest), 512)
    set udg_StratholmeKeepVisibility=GetLastCreatedFogModifier()
    // //
    call PingMinimapLocForForceEx(GetPlayersAll(), GetRectCenter(gg_rct_StratholmeQuest), 2.00, bj_MINIMAPPINGSTYLE_SIMPLE, 100, 100, 0.00)
    call SetCameraQuickPositionLocForPlayer(Player(0), GetRectCenter(gg_rct_StratholmeQuest))
    // //
    call CampaignMinimapIconLocBJ(GetRectCenter(gg_rct_StratholmeQuest), bj_CAMPPINGSTYLE_PRIMARY_RED)
    set udg_StratholmeQuestIcon=GetLastCreatedMinimapIcon()
    // //
    call EnableTrigger(gg_trg_MainQuestStratholmeCompleted)
    if ( Trig_MainQuestStratholmeDiscovered_Func022C() ) then
        call StartTimerBJ(udg_KaboomDiscoverTimer, false, 15.00)
    else
        call StartTimerBJ(udg_KaboomDiscoverTimer, false, GetRandomReal(15.00, 30.00))
    endif
    // //
    call ConditionalTriggerExecute(gg_trg_CheckPoint03)
