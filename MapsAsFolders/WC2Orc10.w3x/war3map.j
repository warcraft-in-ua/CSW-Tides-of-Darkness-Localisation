function Trig_ArrivalCleanUp_Actions takes nothing returns nothing
    call DisableTrigger(GetTriggeringTrigger())
    set udg_ArrivalCinematic=false
    // //
    call CinematicFadeBJ(bj_CINEFADETYPE_FADEOUT, 2.00, "ReplaceableTextures\\CameraMasks\\Black_mask.blp", 100.00, 100.00, 100.00, 0)
    call TriggerSleepAction(2.00)
    // //
    call SetTerrainFogExBJ(0, 3000.00, 12500.00, 0, 75.30, 75.30, 75.33)
    // //
    call SetStackedSoundBJ(false, gg_snd_OrcArmyMarchLoop, gg_rct_HideBlackRockArmy)
    call ForGroupBJ(udg_HiddenUnits, function Trig_ArrivalCleanUp_Func010002)
    call GroupClear(udg_HiddenUnits)
    call ForGroupBJ(udg_HordeArmy, function Trig_ArrivalCleanUp_Func012002)
    call ForGroupBJ(udg_HordeArmy, function Trig_ArrivalCleanUp_Func013002)
    call ForGroupBJ(udg_HordeArmy, function Trig_ArrivalCleanUp_Func014002)
    // //
    call ForGroupBJ(GetUnitsOfPlayerAll(Player(0)), function Trig_ArrivalCleanUp_Func016A)
    call ForGroupBJ(GetUnitsOfPlayerAll(Player(1)), function Trig_ArrivalCleanUp_Func017A)
    call ForGroupBJ(GetUnitsOfPlayerAll(Player(21)), function Trig_ArrivalCleanUp_Func018A)
    call ForGroupBJ(GetUnitsOfPlayerAll(Player(19)), function Trig_ArrivalCleanUp_Func019A)
    call ForGroupBJ(GetUnitsOfPlayerAll(Player(14)), function Trig_ArrivalCleanUp_Func020A)
    call ForGroupBJ(GetUnitsOfPlayerAll(Player(9)), function Trig_ArrivalCleanUp_Func021A)
    // //
    // Cirty Entrance
    call SetDestructableInvulnerableBJ(gg_dest_LTg2_0086, false)
    call SetDestructableInvulnerableBJ(gg_dest_LTg4_0087, false)
    // Gates
    call RemoveUnit(udg_FakeChogall)
    set bj_forLoopAIndex=1
    set bj_forLoopAIndexEnd=4
    loop
        exitwhen bj_forLoopAIndex > bj_forLoopAIndexEnd
        call RemoveUnit(udg_FakeTHCinematicUnit[GetForLoopIndexA()])
        set bj_forLoopAIndex=bj_forLoopAIndex + 1
    endloop
    // //
    if ( Trig_ArrivalCleanUp_Func030C() ) then
        call ShowUnitShow(udg_Chogall)
    else
    endif
    // //
    call AddUnitAnimationPropertiesBJ(false, "cinematic", udg_Guldan)
    call ResetUnitLookAt(udg_Guldan)
    call SetUnitBlendTimeBJ(udg_Guldan, 0.15)
    call SetUnitMoveSpeed(udg_Guldan, 320.00)
    call BlzSetUnitRealFieldBJ(udg_Guldan, UNIT_RF_ANIMATION_WALK_SPEED, 320.00)
    call SetUnitPathing(udg_Guldan, true)
    // //
    call AddUnitAnimationPropertiesBJ(false, "cinematic", udg_DoomHammer)
    call SetUnitBlendTimeBJ(udg_DoomHammer, 0.15)
    call ResetUnitLookAt(udg_DoomHammer)
    call SetUnitMoveSpeed(udg_DoomHammer, 320.00)
    call BlzSetUnitRealFieldBJ(udg_DoomHammer, UNIT_RF_ANIMATION_WALK_SPEED, 320.00)
    call SetUnitPathing(udg_DoomHammer, true)
    // //
    call VolumeGroupResetBJ()
    // //
    call ConditionalTriggerExecute(gg_trg_CinematicPrepOut)
    call CinematicFadeBJ(bj_CINEFADETYPE_FADEIN, 2.00, "ReplaceableTextures\\CameraMasks\\Black_mask.blp", 100.00, 100.00, 100.00, 0)
    call TriggerSleepAction(2.00)
    // //
    call CinematicModeBJ(false, GetPlayersAll())
    call ConditionalTriggerExecute(gg_trg_OilUIShow)
    // //
    // NewQuest event -  reach the center of the city
    // //
    call ForGroupBJ(GetUnitsInRectOfPlayer(gg_rct_HideBlackRockArmy, Player(12)), function Trig_ArrivalCleanUp_Func057A)
    call RescueUnitBJ(udg_TeronOrc, Player(0), false)
    call RescueUnitBJ(udg_Guldan, Player(0), false)
    // //
    call ConditionalTriggerExecute(gg_trg_MainQuestAssaultDiscovered)
endfunction

