function Trig_IntroCinematic_Actions takes nothing returns nothing
    call DisableTrigger(GetTriggeringTrigger())
    // Settings
    call ConditionalTriggerExecute(gg_trg_OilUIHide)
    call CinematicModeBJ(true, GetPlayersAll())
    call CinematicFadeBJ(bj_CINEFADETYPE_FADEOUT, 0.00, "ReplaceableTextures\\CameraMasks\\Black_mask.blp", 0, 0, 0, 0)
    call StopMusicBJ(false)
    call ClearMapMusicBJ()
    call TriggerSleepAction(0.50)
    call SetCineModeVolumeGroupsBJ()
    // SetDoodads
    call SetDoodadAnimationRectBJ("decay", 'NOfp', gg_rct_IntroFirePit)
    // SetActors
    // Creeps
    call ForGroupBJ(GetUnitsInRectOfPlayer(gg_rct_ArchwayVisibility01, Player(PLAYER_NEUTRAL_AGGRESSIVE)), function Trig_IntroCinematic_Func014002)
    call ForGroupBJ(udg_HiddenCreeps, function Trig_IntroCinematic_Func015002)
    call ShowUnitHide(gg_unit_nwns_0471)
    // Peasants
    call SetUnitPathing(gg_unit_h60J_0377, false)
    call AddUnitAnimationPropertiesBJ(true, "gold", gg_unit_h60J_0377)
    call SetUnitAnimation(gg_unit_nvil_0382, "cinematic standard one")
    // Footmans on the shore
    call SetUnitAnimation(gg_unit_h003_0383, "cinematic kneeling one")
    call SetUnitAnimation(gg_unit_h003_0384, "cinematic sitting one")
    call SetUnitAnimation(gg_unit_h003_0385, "cinematic sitting one")
    call SetUnitBlendTimeBJ(gg_unit_h003_0386, 0.25)
    call SetUnitBlendTimeBJ(gg_unit_h60J_0377, 0.25)
    // Footman
    set udg_StromgardeFootman=gg_unit_h004_0260
    call SetUnitBlendTimeBJ(udg_StromgardeFootman, 0.25)
    call AddUnitAnimationPropertiesBJ(true, "cinematic", udg_StromgardeFootman)
    call BlzSetUnitRealFieldBJ(udg_StromgardeFootman, UNIT_RF_ANIMATION_WALK_SPEED, 100.00)
    call SetUnitMoveSpeed(udg_StromgardeFootman, 100.00)
    call SetUnitLookAt(udg_StromgardeFootman, "bone_head", udg_SaidanDathrohan, 0, 0, 90)
    call AddUnitAnimationPropertiesBJ(true, "cinematic", gg_unit_h003_0258)
    call BlzSetUnitRealFieldBJ(gg_unit_h003_0258, UNIT_RF_ANIMATION_WALK_SPEED, 100.00)
    call SetUnitMoveSpeed(gg_unit_h003_0258, 100.00)
    // //
    // Saidan
    // //
    call SetUnitBlendTimeBJ(udg_SaidanDathrohan, 0.25)
    call AddUnitAnimationPropertiesBJ(true, "cinematic", udg_SaidanDathrohan)
    call BlzSetUnitRealFieldBJ(udg_SaidanDathrohan, UNIT_RF_ANIMATION_WALK_SPEED, 100.00)
    call SetUnitMoveSpeed(udg_SaidanDathrohan, 100.00)
    // //
    // Ships
    // //
    call CreateNUnitsAtLocFacingLocBJ(1, 'h60P', Player(1), GetRectCenter(gg_rct_IntroHumanShip01), GetRectCenter(gg_rct_IntroHumanShip02))
    set udg_IntroTransportShip=GetLastCreatedUnit()
    call SetUnitScalePercent(gg_unit_o60Q_0388, 150.00, 150.00, 150.00)
    call SetUnitPathing(udg_IntroTransportShip, false)
    call SetUnitScalePercent(gg_unit_o60Q_0388, 170.00, 170.00, 170.00)
    // Mage
    set udg_MageBoss=gg_unit_H00D_0051
    call SetUnitBlendTimeBJ(udg_MageBoss, 0.25)
    call AddUnitAnimationPropertiesBJ(true, "cinematic", udg_MageBoss)
    call BlzSetUnitRealFieldBJ(udg_MageBoss, UNIT_RF_ANIMATION_WALK_SPEED, 100.00)
    call SetUnitMoveSpeed(udg_MageBoss, 100.00)
    call SetUnitColor(udg_MageBoss, PLAYER_COLOR_VIOLET)
    // Utok
    call AddUnitAnimationPropertiesBJ(true, "Cinematic", udg_Utok)
    call SetUnitBlendTimeBJ(udg_Utok, 0.25)
    call SetUnitMoveSpeed(udg_Utok, 100.00)
    call BlzSetUnitRealFieldBJ(udg_Utok, UNIT_RF_ANIMATION_WALK_SPEED, 100.00)
    call SetUnitOwner(udg_Utok, Player(12), true)
