function Trig_QuestAllianceNavyUpdated_Actions takes nothing returns nothing
    // Shipyards only
    if ( Trig_QuestAllianceNavyUpdated_Func005C() ) then
        call QuestItemSetCompletedBJ(udg_QAllianceNavyRequirement[1], true)
        call TriggerSleepAction(bj_QUEUE_DELAY_QUEST)
        if ( (IsQuestCompleted(udg_QuestAllianceNavy) == true) ) then // INLINED!!
            return
        else
            call DoNothing()
        endif
        call QuestMessageBJ(bj_FORCE_PLAYER[0], bj_QUESTMESSAGE_UPDATED, "TRIGSTR_140")
    else
    endif
    // Foundries only
    if ( Trig_QuestAllianceNavyUpdated_Func007C() ) then
        call QuestItemSetCompletedBJ(udg_QAllianceNavyRequirement[2], true)
        call TriggerSleepAction(bj_QUEUE_DELAY_QUEST)
        if ( (IsQuestCompleted(udg_QuestAllianceNavy) == true) ) then // INLINED!!
            return
        else
            call DoNothing()
        endif
        if ( Trig_QuestAllianceNavyUpdated_Func007Func004C() ) then
            call QuestMessageBJ(bj_FORCE_PLAYER[0], bj_QUESTMESSAGE_UPDATED, "TRIGSTR_3076")
        else
            if ( Trig_QuestAllianceNavyUpdated_Func007Func004Func002C() ) then
                call QuestMessageBJ(bj_FORCE_PLAYER[0], bj_QUESTMESSAGE_UPDATED, "TRIGSTR_3075")
            else
                if ( Trig_QuestAllianceNavyUpdated_Func007Func004Func002Func002C() ) then
                    call QuestMessageBJ(bj_FORCE_PLAYER[0], bj_QUESTMESSAGE_UPDATED, "TRIGSTR_3074")
                else
                endif
            endif
        endif
    else
    endif
    // Refineries
    if ( Trig_QuestAllianceNavyUpdated_Func009C() ) then
        call QuestItemSetCompletedBJ(udg_QAllianceNavyRequirement[3], true)
        call TriggerSleepAction(bj_QUEUE_DELAY_QUEST)
        if ( (IsQuestCompleted(udg_QuestAllianceNavy) == true) ) then // INLINED!!
            return
        else
            call DoNothing()
        endif
        if ( Trig_QuestAllianceNavyUpdated_Func009Func004C() ) then
            call QuestMessageBJ(bj_FORCE_PLAYER[0], bj_QUESTMESSAGE_UPDATED, "TRIGSTR_3082")
        else
            if ( Trig_QuestAllianceNavyUpdated_Func009Func004Func002C() ) then
                call QuestMessageBJ(bj_FORCE_PLAYER[0], bj_QUESTMESSAGE_UPDATED, "TRIGSTR_3081")
            else
                if ( Trig_QuestAllianceNavyUpdated_Func009Func004Func002Func002C() ) then
                    call QuestMessageBJ(bj_FORCE_PLAYER[0], bj_QUESTMESSAGE_UPDATED, "TRIGSTR_3080")
                else
                endif
            endif
        endif
    else
    endif
    // Shipyard And Foundy
