    if ( not ( IsUnitAliveBJ(udg_Maim) == false ) ) then
        return false
    endif
    return true
endfunction

function Trig_NextLevelPrep_Func003C takes nothing returns boolean
    if ( not ( IsUnitAliveBJ(udg_Rend) == false ) ) then
        return false
    endif
    return true
endfunction

function Trig_NextLevelPrep_Func008C takes nothing returns boolean
    if ( not ( IsUnitAliveBJ(udg_Utok) == true ) ) then
        return false
    endif
    return true
endfunction

function Trig_NextLevelPrep_Actions takes nothing returns nothing
    // Need to replace amphibious units with default one
    if ( Trig_NextLevelPrep_Func002C() ) then
        call ReviveHeroLoc(udg_Maim, GetRectCenter(gg_rct_HoldTheLineMaim), false)
    else
    endif
    if ( Trig_NextLevelPrep_Func003C() ) then
        call ReviveHeroLoc(udg_Rend, GetRectCenter(gg_rct_HoldTheLineRend), false)
    else
    endif
    call ReplaceUnitBJ(udg_Maim, 'N60K', bj_UNIT_STATE_METHOD_RELATIVE)
    set udg_Maim=GetLastReplacedUnitBJ()
    call ReplaceUnitBJ(udg_Rend, 'O617', bj_UNIT_STATE_METHOD_RELATIVE)
    set udg_Rend=GetLastReplacedUnitBJ()
    if ( Trig_NextLevelPrep_Func008C() ) then
        call ReplaceUnitBJ(udg_Utok, 'o605', bj_UNIT_STATE_METHOD_RELATIVE)
        set udg_Utok=GetLastReplacedUnitBJ()
    else
    endif
    // Store characters into cache
    call StoreUnitBJ(udg_Utok, "Utok", "WC2Orc09", udg_Cache)
    call StoreUnitBJ(udg_Rend, "Rend", "WC2Orc09", udg_Cache)
    call StoreUnitBJ(udg_Maim, "Maim", "WC2Orc09", udg_Cache)
    call StoreBooleanBJ(true, "Completed", "WC2Orc09", udg_Cache)
    call TriggerSleepAction(0.50)
    call SaveGameCacheBJ(udg_Cache)
    call ShowCustomCampaignButton(true, 15)
    call SetNextLevelBJ("WC2Orc10.w3x")
    call CustomVictoryBJ(Player(0), true, false)
endfunction

