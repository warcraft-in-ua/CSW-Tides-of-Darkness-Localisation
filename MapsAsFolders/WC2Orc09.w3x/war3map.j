            // if x >= zone.xMin and x <= zone.xMax and y >= zone.yMin and y <= zone.yMax then
            if ( s__Zone_xMin[zone] <= x ) and ( x <= s__Zone_xMax[zone] ) and ( s__Zone_yMin[zone] <= y ) and ( y <= s__Zone_yMax[zone] ) then
