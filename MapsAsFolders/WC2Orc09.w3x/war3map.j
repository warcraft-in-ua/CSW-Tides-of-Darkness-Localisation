    call BlzSetUnitRealFieldBJ(udg_Utok, UNIT_RF_ANIMATION_WALK_SPEED, 270.00)
    call SetUnitOwner(udg_Utok, Player(0), false)
    // Rend
    call ResetUnitLookAt(udg_Rend)
    call AddUnitAnimationPropertiesBJ(false, "Cinematic", udg_Rend)
    call SetUnitBlendTimeBJ(udg_Rend, 0.15)
    call SetUnitMoveSpeed(udg_Rend, GetUnitDefaultMoveSpeed(udg_Rend))
    call BlzSetUnitRealFieldBJ(udg_Rend, UNIT_RF_ANIMATION_WALK_SPEED, 270.00)
    call SetUnitOwner(udg_Rend, Player(0), false)
    // Orcs
    call SetUnitScalePercent(gg_unit_o60Q_0388, 120.00, 120.00, 120.00)
    call AddUnitAnimationPropertiesBJ(false, "cinematic", gg_unit_o60G_0005)
    call AddUnitAnimationPropertiesBJ(false, "cinematic", gg_unit_o60G_0008)
    call AddUnitAnimationPropertiesBJ(false, "cinematic", gg_unit_o60G_0132)
    call AddUnitAnimationPropertiesBJ(false, "cinematic", gg_unit_o60G_0432)
    call AddUnitAnimationPropertiesBJ(false, "cinematic", gg_unit_o60G_0433)
