    call SetPlayerAllianceStateBJ(Player(20), Player(1), bj_ALLIANCE_NEUTRAL_VISION)
    call SetPlayerAllianceStateBJ(Player(1), Player(20), bj_ALLIANCE_NEUTRAL_VISION)
