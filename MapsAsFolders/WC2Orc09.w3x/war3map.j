function Trig_FreezeUp_Actions takes nothing returns nothing
    set udg_Transporting01Blocked=true
    set udg_Transporting02Blocked=true
    set udg_Transporting03Blocked=true
    set udg_SeaIsForzen=true
    call EnumDestructablesInRectAll(GetPlayableMapRect(), function Trig_FreezeUp_Func005A)
    call PlaySoundBJ(gg_snd_Ice_Cracking)
    call SetWaterBaseColorBJ(50.00, 50.00, 100, 10.00)
    call TriggerSleepAction(0.50)
    call SetWaterBaseColorBJ(50.00, 50.00, 100, 20.00)
    call TriggerSleepAction(0.50)
    call SetWaterBaseColorBJ(50.00, 50.00, 100, 30.00)
    call TriggerSleepAction(0.50)
    call SetWaterBaseColorBJ(50.00, 50.00, 100, 40.00)
    call TriggerSleepAction(0.50)
    call ConditionalTriggerExecute(gg_trg_MakeUnitAmphibious)
    call SetWaterBaseColorBJ(50.00, 50.00, 100, 50.00)
    call TriggerSleepAction(0.50)
    call SetWaterBaseColorBJ(50.00, 50.00, 100, 60.00)
    call TriggerSleepAction(0.50)
    call SetWaterBaseColorBJ(50.00, 50.00, 100, 70.00)
    // Make Units capable on ice walking
    // Ulnload from Transports
    call ForGroupBJ(GetUnitsOfPlayerAndTypeId(Player(0), 'o60Q'), function Trig_FreezeUp_Func023A)
    call TriggerSleepAction(1.00)
    // Freeze Naval Units
    call ForGroupBJ(GetUnitsInRectMatching(GetPlayableMapRect(), Condition(function Trig_FreezeUp_Func026001002)), function Trig_FreezeUp_Func026A)
    call ForGroupBJ(GetUnitsInRectMatching(GetPlayableMapRect(), Condition(function Trig_FreezeUp_Func027001002)), function Trig_FreezeUp_Func027A)
    call TriggerSleepAction(0.50)
    call SetWaterBaseColorBJ(50.00, 50.00, 100, 80.00)
    call TriggerSleepAction(0.50)
    call SetWaterBaseColorBJ(50.00, 50.00, 100, 90.00)
    call TriggerSleepAction(0.50)
    call SetWaterBaseColorBJ(50.00, 50.00, 100, 100.00)
    call TriggerSleepAction(2.00)
    call ForGroupBJ(GetUnitsInRectMatching(GetPlayableMapRect(), Condition(function Trig_FreezeUp_Func035001002)), function Trig_FreezeUp_Func035A)
    call ForGroupBJ(GetUnitsInRectMatching(GetPlayableMapRect(), Condition(function Trig_FreezeUp_Func036001002)), function Trig_FreezeUp_Func036A)
    call ForGroupBJ(udg_FrozenUnits, function Trig_FreezeUp_Func037002)
    call ForGroupBJ(udg_FrozenUnits, function Trig_FreezeUp_Func038002)
    call SetPlayerUnitAvailableBJ('o60V', false, Player(0))
    call SetPlayerUnitAvailableBJ('o60N', false, Player(0))
    call SetPlayerUnitAvailableBJ('u002', false, Player(0))
    call SetPlayerUnitAvailableBJ('o60M', false, Player(0))
    call SetPlayerAllianceStateBJ(Player(5), Player(17), bj_ALLIANCE_NEUTRAL)
    call SetPlayerAllianceStateBJ(Player(17), Player(5), bj_ALLIANCE_NEUTRAL)
endfunction

