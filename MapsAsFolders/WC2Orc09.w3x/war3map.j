        call SetUnitOwner(udg_Rend, Player(0), false)
        // Orcs
        call ForGroupBJ(udg_IntroOrcsSquad, function Trig_IntroCleanUp_Func005Func040A)
        call SetUnitPositionLocFacingLocBJ(gg_unit_o60G_0432, GetRectCenter(gg_rct_Grunt02Cin), GetRectCenter(gg_rct_BlackhandStart))
        call SetUnitPositionLocFacingLocBJ(gg_unit_o60G_0433, GetRectCenter(gg_rct_Grunt01Cin), GetRectCenter(gg_rct_BlackhandStart))
        call SetUnitPositionLocFacingLocBJ(gg_unit_o60G_0008, GetRectCenter(gg_rct_Grunt03Cin), GetRectCenter(gg_rct_BlackhandStart))
        call SetUnitPositionLocFacingLocBJ(gg_unit_o60G_0005, GetRectCenter(gg_rct_Grunt05Cin), GetRectCenter(gg_rct_BlackhandStart))
        call SetUnitPositionLocFacingLocBJ(gg_unit_o60G_0132, GetRectCenter(gg_rct_Grunt04Cin), GetRectCenter(gg_rct_BlackhandStart))
