    set udg_GateIsDown=true
    call SetDestructableInvulnerableBJ(gg_dest_DTlv_2227, false)
    call SetDestructableLifePercentBJ(gg_dest_DTlv_2227, 1.00)
    if ( Trig_GateDown_Func004C() ) then
        call KillDestructable(gg_dest_DTlv_2227)
    else
    endif
    // Down the Bridge
    call SetDestructableInvulnerableBJ(gg_dest_DTlv_4160, false)
    call KillDestructable(gg_dest_DTlv_4160)
    if ( Trig_GateDown_Func008C() ) then
        call TriggerSleepAction(1.00)
    else
    endif
    call SetDestructableAnimationBJ(gg_dest_B000_4154, "morph")
    call PlaySoundAtPointBJ(gg_snd_GateOpen, 50.00, GetRectCenter(gg_rct_BridgePatch03), 100.00)
    call TriggerSleepAction(0.67)
    set bj_forLoopAIndex=1
    set bj_forLoopAIndexEnd=8
    loop
        exitwhen bj_forLoopAIndex > bj_forLoopAIndexEnd
        call EnumDestructablesInRectAll(udg_CastleBridgePlatform[GetForLoopIndexA()], function Trig_GateDown_Func012Func001A)
        set bj_forLoopAIndex=bj_forLoopAIndex + 1
    endloop
    call SetDestructableAnimationBJ(gg_dest_B000_4154, "stand alternate")
    call TriggerSleepAction(1.00)
    // OpenGates
    call ModifyGateBJ(bj_GATEOPERATION_OPEN, gg_dest_YTcx_1011)
    call PlaySoundAtPointBJ(gg_snd_LargeCityGateOpen1, 50.00, GetRectCenter(gg_rct_BridgePatch04), 100.00)
