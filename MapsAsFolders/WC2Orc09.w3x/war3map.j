        call TriggerSleepAction(0.50)
        set bj_forLoopAIndex=bj_forLoopAIndex + 1
    endloop
    call TriggerSleepAction(0.50)
    set bj_forLoopAIndex=1
    set bj_forLoopAIndexEnd=udg_TyrsHandAttackSize
    loop
        exitwhen bj_forLoopAIndex > bj_forLoopAIndexEnd
        call CreateNUnitsAtLoc(1, 'h00C', Player(17), GetRectCenter(gg_rct_ArchMage), bj_UNIT_FACING)
        call RemoveGuardPosition(GetLastCreatedUnit())
        call GroupAddUnitSimple(GetLastCreatedUnit(), udg_StromgardeAttackWave)
        call IssuePointOrderLocBJ(GetLastCreatedUnit(), "attack", GetRandomLocInRect(gg_rct_IntroBlockers))
        call TriggerSleepAction(0.50)
        set bj_forLoopAIndex=bj_forLoopAIndex + 1
    endloop
    call TriggerSleepAction(0.50)
    set bj_forLoopAIndex=1
    set bj_forLoopAIndexEnd=( udg_TyrsHandAttackSize / 2 )
    loop
        exitwhen bj_forLoopAIndex > bj_forLoopAIndexEnd
        call CreateNUnitsAtLoc(1, 'h008', Player(17), GetRectCenter(gg_rct_ArchMage), bj_UNIT_FACING)
        call RemoveGuardPosition(GetLastCreatedUnit())
        call GroupAddUnitSimple(GetLastCreatedUnit(), udg_StromgardeAttackWave)
        call IssuePointOrderLocBJ(GetLastCreatedUnit(), "attack", GetRandomLocInRect(gg_rct_IntroBlockers))
        call TriggerSleepAction(0.50)
        set bj_forLoopAIndex=bj_forLoopAIndex + 1
    endloop
    call TriggerSleepAction(0.50)
    set bj_forLoopAIndex=1
    set bj_forLoopAIndexEnd=( udg_TyrsHandAttackSize / 2 )
    loop
        exitwhen bj_forLoopAIndex > bj_forLoopAIndexEnd
        call CreateNUnitsAtLoc(1, 'e002', Player(17), GetRectCenter(gg_rct_ArchMage), bj_UNIT_FACING)
        call RemoveGuardPosition(GetLastCreatedUnit())
        call GroupAddUnitSimple(GetLastCreatedUnit(), udg_StromgardeAttackWave)
        call IssuePointOrderLocBJ(GetLastCreatedUnit(), "attack", GetRandomLocInRect(gg_rct_IntroBlockers))
        call TriggerSleepAction(0.50)
        set bj_forLoopAIndex=bj_forLoopAIndex + 1
    endloop
    call TriggerSleepAction(bj_QUEUE_DELAY_QUEST)
    call ConditionalTriggerExecute(gg_trg_STRLaunchAttackWave)
endfunction

