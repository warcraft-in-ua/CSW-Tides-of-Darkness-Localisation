    if ( not ( IsQuestCompleted(udg_QuestHoldTheLine) == false ) ) then
        return false
    endif
    return true
endfunction

function Trig_TrainAttackWave_Actions takes nothing returns nothing
    set bj_forLoopAIndex=1
    set bj_forLoopAIndexEnd=udg_TyrsHandAttackSize
    loop
        exitwhen bj_forLoopAIndex > bj_forLoopAIndexEnd
        call CreateNUnitsAtLoc(1, 'h623', Player(9), GetRectCenter(gg_rct_SpawnPoint), bj_UNIT_FACING)
        call RemoveGuardPosition(GetLastCreatedUnit())
        call GroupAddUnitSimple(GetLastCreatedUnit(), udg_TyrsKeepAttackWave)
        call IssuePointOrderLocBJ(GetLastCreatedUnit(), "attack", GetRandomLocInRect(gg_rct_RedirectionPoint))
        call TriggerSleepAction(0.50)
        set bj_forLoopAIndex=bj_forLoopAIndex + 1
    endloop
    call TriggerSleepAction(0.50)
    set bj_forLoopAIndex=1
    set bj_forLoopAIndexEnd=udg_TyrsHandAttackSize
    loop
        exitwhen bj_forLoopAIndex > bj_forLoopAIndexEnd
        call CreateNUnitsAtLoc(1, 'h60B', Player(9), GetRectCenter(gg_rct_SpawnPoint), bj_UNIT_FACING)
        call RemoveGuardPosition(GetLastCreatedUnit())
        call GroupAddUnitSimple(GetLastCreatedUnit(), udg_TyrsKeepAttackWave)
        call IssuePointOrderLocBJ(GetLastCreatedUnit(), "attack", GetRandomLocInRect(gg_rct_RedirectionPoint))
        call TriggerSleepAction(0.50)
        set bj_forLoopAIndex=bj_forLoopAIndex + 1
    endloop
    call TriggerSleepAction(0.50)
    set bj_forLoopAIndex=1
    set bj_forLoopAIndexEnd=( udg_TyrsHandAttackSize / 2 )
    loop
        exitwhen bj_forLoopAIndex > bj_forLoopAIndexEnd
        call CreateNUnitsAtLoc(1, 'h006', Player(9), GetRectCenter(gg_rct_SpawnPoint), bj_UNIT_FACING)
        call RemoveGuardPosition(GetLastCreatedUnit())
        call GroupAddUnitSimple(GetLastCreatedUnit(), udg_TyrsKeepAttackWave)
        call IssuePointOrderLocBJ(GetLastCreatedUnit(), "attack", GetRandomLocInRect(gg_rct_RedirectionPoint))
        call TriggerSleepAction(0.50)
        set bj_forLoopAIndex=bj_forLoopAIndex + 1
    endloop
    call TriggerSleepAction(0.50)
    set bj_forLoopAIndex=1
    set bj_forLoopAIndexEnd=( udg_TyrsHandAttackSize / 2 )
    loop
        exitwhen bj_forLoopAIndex > bj_forLoopAIndexEnd
        call CreateNUnitsAtLoc(1, 'h009', Player(9), GetRectCenter(gg_rct_SpawnPoint), bj_UNIT_FACING)
        call RemoveGuardPosition(GetLastCreatedUnit())
        call GroupAddUnitSimple(GetLastCreatedUnit(), udg_TyrsKeepAttackWave)
        call IssuePointOrderLocBJ(GetLastCreatedUnit(), "attack", GetRandomLocInRect(gg_rct_RedirectionPoint))
        call TriggerSleepAction(0.50)
        set bj_forLoopAIndex=bj_forLoopAIndex + 1
    endloop
    call TriggerSleepAction(0.50)
    set bj_forLoopAIndex=1
    set bj_forLoopAIndexEnd=( udg_TyrsHandAttackSize / 2 )
    loop
        exitwhen bj_forLoopAIndex > bj_forLoopAIndexEnd
        call CreateNUnitsAtLoc(1, 'e002', Player(9), GetRectCenter(gg_rct_SpawnPoint), bj_UNIT_FACING)
        call RemoveGuardPosition(GetLastCreatedUnit())
        call GroupAddUnitSimple(GetLastCreatedUnit(), udg_TyrsKeepAttackWave)
        call IssuePointOrderLocBJ(GetLastCreatedUnit(), "attack", GetRandomLocInRect(gg_rct_RedirectionPoint))
        call TriggerSleepAction(0.50)
        set bj_forLoopAIndex=bj_forLoopAIndex + 1
    endloop
    call TriggerSleepAction(10.00)
    call ConditionalTriggerExecute(gg_trg_LaunchAttackWave)
endfunction

