function Trig_QuestBerserkerUpdated01_Actions takes nothing returns nothing
    call DisableTrigger(GetTriggeringTrigger())
    call QuestItemSetCompletedBJ(udg_QBerserkerRequirement[1], true)
    // TrollSpirits
    call PlaySoundAtPointBJ(gg_snd_DarkSummoningLaunch1, 100, GetRectCenter(gg_rct_TrollGhost01), 0)
    call AddSpecialEffectLocBJ(GetRectCenter(gg_rct_TrollGhost01), "Abilities\\Spells\\Other\\ANrm\\ANrmTarget.mdl")
    set udg_TrollGhostSE[1]=GetLastCreatedEffectBJ()
    call AddSpecialEffectLocBJ(GetRectCenter(gg_rct_TrollGhost02), "Abilities\\Spells\\Other\\ANrm\\ANrmTarget.mdl")
    set udg_TrollGhostSE[2]=GetLastCreatedEffectBJ()
    call AddSpecialEffectLocBJ(GetRectCenter(gg_rct_TrollGhost03), "Abilities\\Spells\\Other\\ANrm\\ANrmTarget.mdl")
    set udg_TrollGhostSE[3]=GetLastCreatedEffectBJ()
    call AddSpecialEffectLocBJ(GetRectCenter(gg_rct_TrollGhost04), "Abilities\\Spells\\Other\\ANrm\\ANrmTarget.mdl")
    set udg_TrollGhostSE[4]=GetLastCreatedEffectBJ()
    call AddSpecialEffectLocBJ(GetRectCenter(gg_rct_TrollGhost05), "Abilities\\Spells\\Other\\ANrm\\ANrmTarget.mdl")
    set udg_TrollGhostSE[5]=GetLastCreatedEffectBJ()
    call AddSpecialEffectLocBJ(GetRectCenter(gg_rct_TrollGhost06), "Abilities\\Spells\\Other\\ANrm\\ANrmTarget.mdl")
    set udg_TrollGhostSE[6]=GetLastCreatedEffectBJ()
    call TriggerSleepAction(1.00)
    call CreateNUnitsAtLocFacingLocBJ(1, 'ngh1', Player(PLAYER_NEUTRAL_AGGRESSIVE), GetRectCenter(gg_rct_TrollGhost02), GetRectCenter(gg_rct_TrollGhost01))
    set udg_TrollGhostUnit[1]=GetLastCreatedUnit()
    call CreateNUnitsAtLocFacingLocBJ(1, 'ngh1', Player(PLAYER_NEUTRAL_AGGRESSIVE), GetRectCenter(gg_rct_TrollGhost03), GetRectCenter(gg_rct_TrollGhost01))
    set udg_TrollGhostUnit[2]=GetLastCreatedUnit()
    call CreateNUnitsAtLocFacingLocBJ(1, 'ngh1', Player(PLAYER_NEUTRAL_AGGRESSIVE), GetRectCenter(gg_rct_TrollGhost04), GetRectCenter(gg_rct_TrollGhost01))
    set udg_TrollGhostUnit[3]=GetLastCreatedUnit()
    call CreateNUnitsAtLocFacingLocBJ(1, 'ngh1', Player(PLAYER_NEUTRAL_AGGRESSIVE), GetRectCenter(gg_rct_TrollGhost05), GetRectCenter(gg_rct_TrollGhost01))
    set udg_TrollGhostUnit[4]=GetLastCreatedUnit()
    call CreateNUnitsAtLocFacingLocBJ(1, 'ngh1', Player(PLAYER_NEUTRAL_AGGRESSIVE), GetRectCenter(gg_rct_TrollGhost06), GetRectCenter(gg_rct_TrollGhost01))
    set udg_TrollGhostUnit[5]=GetLastCreatedUnit()
    set bj_forLoopBIndex=1
    set bj_forLoopBIndexEnd=5
    loop
        exitwhen bj_forLoopBIndex > bj_forLoopBIndexEnd
        call UnitShareVisionBJ(true, udg_TrollGhostUnit[GetForLoopIndexB()], Player(0))
        set bj_forLoopBIndex=bj_forLoopBIndex + 1
    endloop
    call TriggerSleepAction(1.00)
    call CreateNUnitsAtLoc(1, 'ngh2', Player(PLAYER_NEUTRAL_AGGRESSIVE), GetRectCenter(gg_rct_TrollGhost01), bj_UNIT_FACING)
    set udg_TrollGhostUnit[6]=GetLastCreatedUnit()
    call UnitShareVisionBJ(true, udg_TrollGhostUnit[6], Player(0))
    set bj_forLoopAIndex=1
    set bj_forLoopAIndexEnd=6
    loop
        exitwhen bj_forLoopAIndex > bj_forLoopAIndexEnd
        call DestroyEffectBJ(udg_TrollGhostSE[GetForLoopIndexA()])
        set bj_forLoopAIndex=bj_forLoopAIndex + 1
    endloop
    // Quest
    call TriggerSleepAction(bj_QUEUE_DELAY_QUEST)
    call QuestMessageBJ(bj_FORCE_PLAYER[0], bj_QUESTMESSAGE_UPDATED, "TRIGSTR_2552")
    call TriggerSleepAction(bj_QUEUE_DELAY_QUEST)
    call QuestMessageBJ(bj_FORCE_PLAYER[0], bj_QUESTMESSAGE_ALWAYSHINT, "TRIGSTR_2883")
    call QueuedTriggerRemoveBJ(GetTriggeringTrigger())
endfunction

