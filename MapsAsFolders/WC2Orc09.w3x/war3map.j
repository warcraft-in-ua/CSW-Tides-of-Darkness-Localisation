function Trig_MidCinematicsSetUp_Actions takes nothing returns nothing
    // Save Current Camera and Current Selection
    set udg_VarCurrentCamera=GetCurrentCameraSetup()
    call GroupClear(udg_P01Selection)
    call ForGroupBJ(GetUnitsSelectedAll(Player(0)), function Trig_MidCinematicsSetUp_Func004A)
    call ClearSelectionForPlayer(Player(0))
    // CreateFakeActors
    // Utok
    call CreateNUnitsAtLoc(1, 'o605', Player(12), GetRectCenter(gg_rct_FakeUtokCreate), bj_UNIT_FACING)
    set udg_FakeUtok=GetLastCreatedUnit()
    call AddUnitAnimationPropertiesBJ(true, "Cinematic", udg_FakeUtok)
    call SetUnitBlendTimeBJ(udg_FakeUtok, 0.25)
    call SetUnitMoveSpeed(udg_FakeUtok, 100.00)
    call BlzSetUnitRealFieldBJ(udg_FakeUtok, UNIT_RF_ANIMATION_WALK_SPEED, 100.00)
    call ShowUnitHide(udg_FakeUtok)
    // Maim
    call CreateNUnitsAtLoc(1, 'N60K', Player(20), GetRectCenter(gg_rct_FakeMaimCreate), bj_UNIT_FACING)
    set udg_FakeMaim=GetLastCreatedUnit()
    call AddUnitAnimationPropertiesBJ(true, "Cinematic", udg_FakeMaim)
    call SetUnitBlendTimeBJ(udg_FakeMaim, 0.25)
    call SetUnitMoveSpeed(udg_FakeMaim, 100.00)
    call BlzSetUnitRealFieldBJ(udg_FakeMaim, UNIT_RF_ANIMATION_WALK_SPEED, 100.00)
    call ShowUnitHide(udg_FakeMaim)
    // Rend
    call CreateNUnitsAtLoc(1, 'O617', Player(20), GetRectCenter(gg_rct_FakeRendCreate), bj_UNIT_FACING)
    set udg_FakeRend=GetLastCreatedUnit()
    call AddUnitAnimationPropertiesBJ(true, "Cinematic", udg_FakeRend)
    call SetUnitBlendTimeBJ(udg_FakeRend, 0.25)
    call SetUnitMoveSpeed(udg_FakeRend, 100.00)
    call BlzSetUnitRealFieldBJ(udg_FakeRend, UNIT_RF_ANIMATION_WALK_SPEED, 100.00)
    call ShowUnitHide(udg_FakeRend)
    // Grunts
    call CreateNUnitsAtLoc(1, 'o60G', Player(20), GetRandomLocInRect(gg_rct_FakeUnitsParty), bj_UNIT_FACING)
    set udg_FakeGrunt[1]=GetLastCreatedUnit()
    call CreateNUnitsAtLoc(1, 'o60G', Player(20), GetRandomLocInRect(gg_rct_FakeUnitsParty), bj_UNIT_FACING)
    set udg_FakeGrunt[2]=GetLastCreatedUnit()
    call CreateNUnitsAtLoc(1, 'o60G', Player(20), GetRandomLocInRect(gg_rct_FakeUnitsParty), bj_UNIT_FACING)
    set udg_FakeGrunt[3]=GetLastCreatedUnit()
    call CreateNUnitsAtLoc(1, 'o60G', Player(20), GetRandomLocInRect(gg_rct_FakeUnitsParty), bj_UNIT_FACING)
    set udg_FakeGrunt[4]=GetLastCreatedUnit()
    call CreateNUnitsAtLoc(1, 'o60G', Player(20), GetRandomLocInRect(gg_rct_FakeUnitsParty), bj_UNIT_FACING)
    set udg_FakeGrunt[5]=GetLastCreatedUnit()
    // ***
    set bj_forLoopAIndex=1
    set bj_forLoopAIndexEnd=5
    loop
        exitwhen bj_forLoopAIndex > bj_forLoopAIndexEnd
        call GroupAddUnitSimple(udg_FakeGrunt[GetForLoopIndexA()], udg_FakeGruntsGroup)
        set bj_forLoopAIndex=bj_forLoopAIndex + 1
    endloop
    call ForGroupBJ(udg_FakeGruntsGroup, function Trig_MidCinematicsSetUp_Func044A)
endfunction

