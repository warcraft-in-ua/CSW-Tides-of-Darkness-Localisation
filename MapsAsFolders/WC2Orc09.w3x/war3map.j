function Trig_BTCCinematicCleanUp_Actions takes nothing returns nothing
    call DisableTrigger(GetTriggeringTrigger())
    call CinematicFilterGenericBJ(1.00, BLEND_MODE_BLEND, "ReplaceableTextures\\CameraMasks\\Black_mask.blp", 100, 100, 100, 0.00, 100.00, 100.00, 100.00, 0)
    call TriggerSleepAction(1.00)
    set udg_BTCampCinematic=false
    // Cleaning Up Scene
    call CameraClearNoiseForPlayer(Player(0))
    call CameraSetFocalDistance(0.00)
    call CameraSetDepthOfFieldScale(0.00)
    call ConditionalTriggerExecute(gg_trg_MidCinematicsCleanUp)
    call VolumeGroupResetBJ()
    call PauseUnitBJ(false, gg_unit_o60D_0022)
    call PauseUnitBJ(false, gg_unit_o60D_0021)
    call ForGroupBJ(udg_P1GroundTroopsToHide, function Trig_BTCCinematicCleanUp_Func013002)
    call ForGroupBJ(GetUnitsOfPlayerMatching(Player(0), Condition(function Trig_BTCCinematicCleanUp_Func014001002)), function Trig_BTCCinematicCleanUp_Func014A)
    call SetUnitPositionLocFacingLocBJ(udg_Maim, GetRectCenter(gg_rct_ReunionCinMaim), GetRectCenter(gg_rct_BlackToothCamp))
    call SetUnitPositionLocFacingLocBJ(udg_Rend, GetRectCenter(gg_rct_ReunionCinRend02), GetRectCenter(gg_rct_ReunionCinMaim))
    call SetUnitPositionLocFacingBJ(udg_Utok, GetRectCenter(gg_rct_ReunionCinUtok05), 109.71)
    call RemoveUnit(udg_FakeOgre[1])
    set bj_forLoopBIndex=1
    set bj_forLoopBIndexEnd=4
    loop
        exitwhen bj_forLoopBIndex > bj_forLoopBIndexEnd
        call RemoveUnit(udg_FakePeon[GetForLoopIndexB()])
        set bj_forLoopBIndex=bj_forLoopBIndex + 1
    endloop
    call KillDestructable(gg_dest_YT30_2437)
    call DestructableRestoreLife(gg_dest_DTep_8308, GetDestructableMaxLife(gg_dest_DTep_8308), false)
    call DestructableRestoreLife(gg_dest_DTep_8307, GetDestructableMaxLife(gg_dest_DTep_8307), false)
    // Need to provide Oil Rig
    if ( Trig_BTCCinematicCleanUp_Func024C() ) then
        // SoundsOff
        call StopSoundBJ(gg_snd_WC2Orc09Rend6, false)
        call StopSoundBJ(gg_snd_WC2Orc09Rend7, false)
        call StopSoundBJ(gg_snd_WC2Orc09Utok4, false)
        call StopSoundBJ(gg_snd_WC2Orc09Utok5, false)
        call StopSoundBJ(gg_snd_WC2Orc09Utok6, false)
        call StopSoundBJ(gg_snd_WC2Orc09Grunt1, false)
        call StopSoundBJ(gg_snd_WC2Orc09Maim3, false)
        call StopSoundBJ(gg_snd_WC2Orc09Maim2, false)
        // //
        call SetUnitPositionLoc(gg_unit_h001_0526, GetRectCenter(gg_rct_TankerStop))
    else
    endif
    call SetUnitOwner(gg_unit_n60P_0104, Player(0), false)
    call ReplaceUnitBJ(gg_unit_n60P_0104, 'u002', bj_UNIT_STATE_METHOD_DEFAULTS)
    set udg_PlatformCreated=GetLastReplacedUnitBJ()
    call UnitAddAbilityBJ('A637', udg_PlatformCreated)
    // ***
    call CinematicFadeBJ(bj_CINEFADETYPE_FADEIN, 1.00, "ReplaceableTextures\\CameraMasks\\Black_mask.blp", 0.00, 0.00, 0.00, 0)
    call TriggerSleepAction(1.00)
    call CinematicModeBJ(false, GetPlayersAll())
    call ConditionalTriggerExecute(gg_trg_OilUIShow)
    // ***
    call ConditionalTriggerExecute(gg_trg_QuestCampCompleted)
endfunction

