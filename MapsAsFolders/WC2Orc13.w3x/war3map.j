function Trig_Escape_Boss_Prep_Actions takes nothing returns nothing
    set udg_Event_Boss_TP1=GetRectCenter(gg_rct_Boss_Fight_1)
    set udg_Event_Boss_TP2=GetRectCenter(gg_rct_Boss_Fight_2)
    set udg_Event_Boss_TP3=GetRectCenter(gg_rct_Boss_Fight_3)
    set udg_Event_Boss_TP4=GetRectCenter(gg_rct_Boss_Fight_4)
    // ///
    call SetUnitManaPercentBJ(udg_Zuluhed, 100)
    call SetUnitLifePercentBJ(udg_Zuluhed, 100)
    call UnitResetCooldown(udg_Zuluhed)
    call UnitRemoveBuffsBJ(bj_REMOVEBUFFS_ALL, udg_Zuluhed)
    // ///
    call EnableTrigger(gg_trg_Event_Boss_Fight_1)
    call EnableTrigger(gg_trg_Event_Boss_Fight_2)
    // ----------------------------------------------------------------------------------------------------
    // Barriers
    // ----------------------------------------------------------------------------------------------------
    call CreateFogModifierRectBJ(true, Player(0), FOG_OF_WAR_VISIBLE, gg_rct_Boss_Fight_Visibility)
    call ShowUnitShow(gg_unit_u004_0329)
    call ShowUnitShow(gg_unit_u004_0344)
    call ShowUnitShow(gg_unit_u004_0332)
    call ShowUnitShow(gg_unit_u004_0345)
    call ShowUnitShow(gg_unit_u004_0346)
    call EnumDestructablesInRectAll(gg_rct_Boss_Blocker_West, function Trig_Escape_Boss_Prep_Func022A)
    call EnumDestructablesInRectAll(gg_rct_Boss_Blocker_West, function Trig_Escape_Boss_Prep_Func023A)
    call EnumDestructablesInRectAll(gg_rct_Boss_Blocker_West, function Trig_Escape_Boss_Prep_Func024A)
    call EnumDestructablesInRectAll(gg_rct_Boss_Blocker_North, function Trig_Escape_Boss_Prep_Func025A)
    call EnumDestructablesInRectAll(gg_rct_Boss_Blocker_North, function Trig_Escape_Boss_Prep_Func026A)
    call EnumDestructablesInRectAll(gg_rct_Boss_Blocker_North, function Trig_Escape_Boss_Prep_Func027A)
    call EnumDestructablesInRectAll(gg_rct_Boss_Blocker_SouthEast, function Trig_Escape_Boss_Prep_Func028A)
    // ----------------------------------------------------------------------------------------------------
    // Rakmar
    // ----------------------------------------------------------------------------------------------------
    call PauseUnitBJ(false, udg_Zuluhed)
    call PauseUnitBJ(false, udg_Rakmar)
    call SetUnitManaPercentBJ(udg_Zuluhed, 100)
    call CreateNUnitsAtLoc(1, 'U006', Player(PLAYER_NEUTRAL_PASSIVE), GetRectCenter(gg_rct_Cin_3_Rakmar_1), 270.00)
    set udg_Rakmar=GetLastCreatedUnit()
    call SetUnitColor(udg_Rakmar, PLAYER_COLOR_BLUE)
    call SetHeroLevelBJ(udg_Rakmar, 10, false)
    call SetUnitAnimation(udg_Rakmar, "stand channel")
    call QueueUnitAnimationBJ(udg_Rakmar, "stand ready")
    // ///
    call SetUnitScalePercent(udg_RakmarCinematic, 125.00, 125.00, 125.00)
    call UnitAddAbilityBJ('AUdc', udg_Rakmar)
    call UnitAddAbilityBJ('A01G', udg_Rakmar)
    call UnitAddAbilityBJ('AUdr', udg_Rakmar)
    if ( Trig_Escape_Boss_Prep_Func046C() ) then
        call BlzSetUnitMaxHP(udg_Rakmar, 3000)
        call SetUnitAbilityLevelSwapped('AUdc', udg_Rakmar, 1)
        call SetUnitAbilityLevelSwapped('A01G', udg_Rakmar, 1)
        call SetUnitAbilityLevelSwapped('AUdr', udg_Rakmar, 1)
    else
        if ( Trig_Escape_Boss_Prep_Func046Func002C() ) then
            call BlzSetUnitMaxHP(udg_Rakmar, 4000)
            call SetUnitAbilityLevelSwapped('AUdc', udg_Rakmar, 2)
            call SetUnitAbilityLevelSwapped('A01G', udg_Rakmar, 2)
            call SetUnitAbilityLevelSwapped('AUdr', udg_Rakmar, 2)
        else
            if ( Trig_Escape_Boss_Prep_Func046Func002Func002C() ) then
                call BlzSetUnitMaxHP(udg_Rakmar, 5000)
                call SetUnitAbilityLevelSwapped('AUdc', udg_Rakmar, 3)
                call SetUnitAbilityLevelSwapped('A01G', udg_Rakmar, 3)
                call SetUnitAbilityLevelSwapped('AUdr', udg_Rakmar, 3)
            else
            endif
        endif
    endif
    call SetUnitLifePercentBJ(udg_Rakmar, 100)
    // ///
endfunction

