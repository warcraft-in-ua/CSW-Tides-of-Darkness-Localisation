            else
                if (HaveSavedInteger(Table___ht, (s__OilRig_structTable), (GetHandleId((target))))) then // INLINED!!
                    call s__OilCollector_retrieveOil(collector,target)
                endif
            endif
        endif
    endif
    set source=null
    set target=null
endfunction

