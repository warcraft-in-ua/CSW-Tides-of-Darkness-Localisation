function Trig_Artifact_Acquired_Actions takes nothing returns nothing
    set udg_ArtifactFragmentsFound=( udg_ArtifactFragmentsFound + 1 )
    // Replace the item
    if ( (udg_ArtifactItem != null) ) then // INLINED!!
        call RemoveItem(udg_ArtifactItem)
    else
        call DoNothing()
    endif
    call UnitAddItemByIdSwapped(udg_ArtifactTypes[udg_ArtifactFragmentsFound], udg_Zuluhed)
    set udg_ArtifactItem=GetLastCreatedItem()
    // Handle Quest Messages
    if ( (udg_ArtifactFragmentsFound == 1) ) then // INLINED!!
        call QueuedTriggerAddBJ(gg_trg_Artifact_01_Q, true)
    else
        call DoNothing()
    endif
    if ( (udg_ArtifactFragmentsFound == 2) ) then // INLINED!!
        call QueuedTriggerAddBJ(gg_trg_Artifact_02_Q, true)
    else
        call DoNothing()
    endif
    if ( (udg_ArtifactFragmentsFound == 3) ) then // INLINED!!
        call QueuedTriggerAddBJ(gg_trg_Artifact_03_Q, true)
    else
        call DoNothing()
    endif
    if ( (udg_ArtifactFragmentsFound == 4) ) then // INLINED!!
        call QueuedTriggerAddBJ(gg_trg_Artifact_04_Q, true)
    else
        call DoNothing()
    endif
    if ( (udg_ArtifactFragmentsFound == 5) ) then // INLINED!!
        call QueuedTriggerAddBJ(gg_trg_Artifact_05_Q, true)
    else
        call DoNothing()
    endif
    if ( (udg_ArtifactFragmentsFound == 6) ) then // INLINED!!
        call QueuedTriggerAddBJ(gg_trg_Artifact_06_Q, true)
    else
        call DoNothing()
    endif
    if ( (udg_ArtifactFragmentsFound == 7) ) then // INLINED!!
        call QueuedTriggerAddBJ(gg_trg_Artifact_07_Q, true)
    else
        call DoNothing()
    endif
    if ( (udg_ArtifactFragmentsFound == 8) ) then // INLINED!!
        call QueuedTriggerAddBJ(gg_trg_Artifact_08_Q, true)
    else
        call DoNothing()
    endif
    if ( (udg_ArtifactFragmentsFound == 9) ) then // INLINED!!
        call QueuedTriggerAddBJ(gg_trg_Artifact_09_Q, true)
    else
        call DoNothing()
    endif
    if ( (udg_ArtifactFragmentsFound == 10) ) then // INLINED!!
        call QueuedTriggerAddBJ(gg_trg_Artifact_10_Q, true)
    else
        call DoNothing()
    endif
