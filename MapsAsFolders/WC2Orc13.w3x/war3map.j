function Trig_Boss_Cleanup_Actions takes nothing returns nothing
    call CameraSetFocalDistance(0.00)
    call CameraSetupApplyForPlayer(true, gg_cam_Cin_3_Gameplay_Cam, Player(0), 0)
    call SetCameraBoundsToRectForPlayerBJ(Player(0), gg_rct_Boss_Fight_Limit)
    call BlzChangeMinimapTerrainTex("MinimapTombBossFight.tga")
    call CinematicFadeBJ(bj_CINEFADETYPE_FADEIN, 2.00, "ReplaceableTextures\\CameraMasks\\White_mask.blp", 0, 0, 0, 0)
    call CinematicModeBJ(false, GetPlayersAll())
    call ConditionalTriggerExecute(gg_trg_OilUIShow)
    // ///
    set udg_BossCinematic=false
    set udg_InCinematic=false
    // ----------------------------------------------------------------------------------------------------
    // Sounds and Music
    // ----------------------------------------------------------------------------------------------------
    call StopSoundBJ(gg_snd_Demonic_Theme_2, false)
    call StopMusicBJ(false)
    call VolumeGroupSetVolumeBJ(SOUND_VOLUMEGROUP_MUSIC, 100)
    call PlayMusicBJ(gg_snd_Doom)
    call ResumeMusicBJ()
    // ///
    call StopSoundBJ(gg_snd_WC2Orc13Zuluhed_RakmarEncounter1, true)
    call StopSoundBJ(gg_snd_WC2Orc13Zuluhed_RakmarEncounter2, true)
    call StopSoundBJ(gg_snd_WC2Orc13Zuluhed_RakmarEncounter3, true)
    call StopSoundBJ(gg_snd_WC2Orc13Zuluhed_RakmarEncounter4, true)
    call StopSoundBJ(gg_snd_WC2Orc13Rakmar_Encounter1, true)
    call StopSoundBJ(gg_snd_WC2Orc13Rakmar_Encounter2, true)
    call StopSoundBJ(gg_snd_WC2Orc13Rakmar_Encounter3, true)
    // Unhide items
    // ----------------------------------------------------------------------------------------------------
    call EnumItemsInRectBJ(GetPlayableMapRect(), function Trig_Boss_Cleanup_Func029A)
    // ----------------------------------------------------------------------------------------------------
    // ----------------------------------------------------------------------------------------------------
    // Clear Units
    // ----------------------------------------------------------------------------------------------------
    call ShowUnitHide(udg_RakmarCinematic)
    call RemoveUnit(udg_RakmarShade)
    call SetUnitVertexColorBJ(udg_RakmarCinematic, 100, 100, 100, 0)
    call SetUnitTimeScalePercent(udg_RakmarCinematic, 100.00)
    // ///
    call ResetUnitLookAt(udg_ZuluhedCinematic)
    call ShowUnitHide(udg_ZuluhedCinematic)
    // ///
    // ///
    call ShowDestructableBJ(false, gg_dest_B015_8765)
    call ShowDestructableBJ(false, gg_dest_B015_8766)
    call ShowDestructableBJ(false, gg_dest_B015_9065)
    call EnumDestructablesInRectAll(gg_rct_IntroSceneryWalls, function Trig_Boss_Cleanup_Func046A)
    call EnumDestructablesInRectAll(gg_rct_CinematicWallsBottom01, function Trig_Boss_Cleanup_Func047A)
    call EnumDestructablesInRectAll(gg_rct_CinematicWallsBottom02, function Trig_Boss_Cleanup_Func048A)
    // ///
    set bj_wantDestroyGroup=true
    call ForGroupBJ(GetUnitsOfPlayerAll(Player(0)), function Trig_Boss_Cleanup_Func051A)
    call ShowUnitShow(udg_Zuluhed)
    if ( Trig_Boss_Cleanup_Func053C() ) then
        call SetHeroLevelBJ(udg_Zuluhed, 10, false)
        call SetUnitAbilityLevelSwapped('A00S', udg_Zuluhed, 3)
        call SetUnitAbilityLevelSwapped('A00U', udg_Zuluhed, 3)
        call CreateItemLoc('I00D', GetRectCenter(GetPlayableMapRect()))
        call UnitAddItemSwapped(GetLastCreatedItem(), udg_Zuluhed)
    else
    endif
    call SetUnitPositionLocFacingBJ(udg_Zuluhed, GetRectCenter(gg_rct_Cin_3_Zuluhed_3), 90.00)
    call AddSpecialEffectTargetUnitBJ("overhead", udg_Zuluhed, "Doodads\\Terrain\\Lights\\DepthlessGlowAnimated.mdx")
    set udg_LightZuluhed=GetLastCreatedEffectBJ()
    call SetUnitAnimation(udg_Zuluhed, "stand ready")
    // ///
    call SuspendHeroXPBJ(false, udg_Rend)
    call SuspendHeroXPBJ(false, udg_Maim)
    call SuspendHeroXPBJ(true, udg_Zuluhed)
    // ///
    call ForGroupBJ(GetUnitsInRectAll(GetPlayableMapRect()), function Trig_Boss_Cleanup_Func063A)
    // ///
    call ForGroupBJ(udg_HiddenUnits, function Trig_Boss_Cleanup_Func065A)
    // ----------------------------------------------------------------------------------------------------
    // Weather
    // ----------------------------------------------------------------------------------------------------
    call SetTerrainFogExBJ(0, 500.00, 2500.00, 0.50, 6.00, 8.00, 12.00)
    call SetSkyModel(null)
    call CameraClearNoiseForPlayer(Player(0))
    // ----------------------------------------------------------------------------------------------------
    // StartQuests
    // ----------------------------------------------------------------------------------------------------
    call TriggerSleepAction(0.25)
    call ConditionalTriggerExecute(gg_trg_Escape_Boss_Prep)
    call ConditionalTriggerExecute(gg_trg_Quest_Escape_Boss_Update)
    set udg_EscapingBoss=true
endfunction

