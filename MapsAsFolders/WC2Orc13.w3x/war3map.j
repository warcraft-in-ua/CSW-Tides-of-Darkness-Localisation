function Trig_Victory_Cinematic_Q_Actions takes nothing returns nothing
    call ConditionalTriggerExecute(gg_trg_Victory_Setup)
    call EnableTrigger(gg_trg_Victory_Skipped)
    // ///
    call ConditionalTriggerExecute(gg_trg_OilUIHide)
    call CinematicModeBJ(true, GetPlayersAll())
    call CinematicFadeBJ(bj_CINEFADETYPE_FADEOUT, 0.00, "ReplaceableTextures\\CameraMasks\\White_mask.blp", 0, 0, 0, 0)
    call TriggerSleepAction(0.50)
    call CinematicFadeBJ(bj_CINEFADETYPE_FADEIN, 0.00, "ReplaceableTextures\\CameraMasks\\Black_mask.blp", 0, 0, 0, 0)
    // ///
    set udg_SargerasCinematic=true
    set udg_InCinematic=true
    call SetMusicVolumeBJ(0.00)
    call VolumeGroupSetVolumeBJ(SOUND_VOLUMEGROUP_AMBIENTSOUNDS, 0.00)
    call PlaySoundBJ(gg_snd_Horde_Intro_Theme_2)
    call SetSoundVolumeBJ(gg_snd_Horde_Intro_Theme_2, 75.00)
    // xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
    // CINEMATIC BEGINS - Cinematic Can Now Be Skipped
    // xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
    // ///
    call CameraSetFocalDistance(150.00)
    call CameraSetupApplyForPlayer(true, gg_cam_Cin_4_Victory_1_a, Player(0), 0)
    // ///
    call PlaySoundBJ(gg_snd_RollingThunder1)
    call SetUnitAnimation(udg_ZuluhedCinematic, "stand channel")
    call TriggerSleepAction(1.00)
    if ( (udg_VictorySkipped == true) ) then // INLINED!!
        return
    else
        call DoNothing()
    endif
    // ///
    call QueueUnitAnimationBJ(udg_ZuluhedCinematic, "attack")
    call TriggerSleepAction(1.00)
    if ( (udg_VictorySkipped == true) ) then // INLINED!!
        return
    else
        call DoNothing()
    endif
    // ----------------------------------------------------------------------------------------------------
    // Rakmar 1
    // ----------------------------------------------------------------------------------------------------
    call CameraSetupApplyForPlayer(true, gg_cam_Cin_3_Boss_2_a, Player(0), 0)
    // ///
    call PlaySoundBJ(gg_snd_LightningBolt)
    call ShowUnitHide(udg_ZuluhedCinematic)
    call ShowUnitShow(udg_RakmarCinematic)
    call PauseUnitBJ(false, udg_RakmarCinematic)
    call SetUnitPositionLocFacingBJ(udg_RakmarCinematic, GetRectCenter(gg_rct_Cin_3_Rakmar_1), 270.00)
    call SetUnitAnimation(udg_RakmarCinematic, "stand ready")
    call AddSpecialEffectTargetUnitBJ("chest", udg_RakmarCinematic, "Abilities\\Spells\\Orc\\Purge\\PurgeBuffTarget.mdl")
    call DestroyEffectBJ(GetLastCreatedEffectBJ())
    call PlaySoundBJ(gg_snd_ShadeDeath1)
    call SetUnitAnimation(udg_RakmarCinematic, "death")
    call SetUnitTimeScalePercent(udg_RakmarCinematic, 10.00)
    call TriggerSleepAction(0.50)
    if ( (udg_VictorySkipped == true) ) then // INLINED!!
        return
    else
        call DoNothing()
    endif
    // ///
    call StopSoundBJ(gg_snd_ShadeDeath1, true)
    call SetPlayerColorBJ(Player(23), PLAYER_COLOR_BLUE, false)
    call PlayDialogueFromSpeakerEx(bj_FORCE_PLAYER[0], udg_RakmarCinematic, 'U006', gg_snd_WC2Orc13Rakmar_death1, bj_TIMETYPE_ADD, 0, false)
    call TriggerSleepAction(6.00)
    if ( (udg_VictorySkipped == true) ) then // INLINED!!
        return
    else
        call DoNothing()
    endif
    // ///
    call SetUnitTimeScalePercent(udg_RakmarCinematic, 30.00)
    call WaitForSoundBJ(gg_snd_WC2Orc13Rakmar_death1, 0.00)
    if ( (udg_VictorySkipped == true) ) then // INLINED!!
        return
    else
        call DoNothing()
    endif
    // ///
    call AddSpecialEffectTargetUnitBJ("origin", udg_RakmarCinematic, "Objects\\Spawnmodels\\Undead\\UndeadDissipate\\UndeadDissipate.mdl")
    call DestroyEffectBJ(GetLastCreatedEffectBJ())
    call TriggerSleepAction(1.00)
    if ( (udg_VictorySkipped == true) ) then // INLINED!!
        return
    else
        call DoNothing()
    endif
    // ----------------------------------------------------------------------------------------------------
    // Zuluhed 1
    // ----------------------------------------------------------------------------------------------------
    call CameraSetupApplyForPlayer(true, gg_cam_Cin_3_Boss_3_b, Player(0), 0)
    // ///
    call ShowUnitShow(udg_ZuluhedCinematic)
    call SetUnitBlendTimeBJ(udg_ZuluhedCinematic, 1.00)
    call SetUnitLookAt(udg_ZuluhedCinematic, "bone_head", udg_RakmarCinematic, 0, 0, 90)
    call IssuePointOrderLocBJ(udg_ZuluhedCinematic, "move", GetRectCenter(gg_rct_Cin_4_Zuluhed_1))
    call TriggerSleepAction(( DistanceBetweenPoints(GetUnitLoc(udg_ZuluhedCinematic), GetRectCenter(gg_rct_Cin_4_Zuluhed_1)) / GetUnitMoveSpeed(udg_ZuluhedCinematic) ))
    if ( (udg_VictorySkipped == true) ) then // INLINED!!
        return
    else
        call DoNothing()
    endif
    // ///
    call SetUnitFacingTimed(udg_ZuluhedCinematic, 180.00, 1.00)
    call ResetUnitLookAt(udg_ZuluhedCinematic)
    call SetUnitAnimation(udg_ZuluhedCinematic, "cinematic headshake one")
    call QueueUnitAnimationBJ(udg_ZuluhedCinematic, "stand")
    call TriggerSleepAction(1.00)
    if ( (udg_VictorySkipped == true) ) then // INLINED!!
        return
    else
        call DoNothing()
    endif
    // ///
    call SetUnitBlendTimeBJ(udg_ZuluhedCinematic, 0.50)
    call SetPlayerColorBJ(Player(23), PLAYER_COLOR_SNOW, false)
    call PlayDialogueFromSpeakerEx(bj_FORCE_PLAYER[0], udg_ZuluhedCinematic, 'O61F', gg_snd_WC2Orc13Zuluhed_Final1, bj_TIMETYPE_ADD, 0, false)
    call SetUnitAnimation(udg_ZuluhedCinematic, "cinematic angry")
    call QueueUnitAnimationBJ(udg_ZuluhedCinematic, "cinematic wave one")
    call QueueUnitAnimationBJ(udg_ZuluhedCinematic, "stand")
    call WaitForSoundBJ(gg_snd_WC2Orc13Zuluhed_Final1, 0.00)
    if ( (udg_VictorySkipped == true) ) then // INLINED!!
        return
    else
        call DoNothing()
    endif
    call IssuePointOrderLocBJ(udg_ZuluhedCinematic, "move", GetRectCenter(gg_rct_Cin_1_Zuluhed_2))
    call TriggerSleepAction(1.00)
    if ( (udg_VictorySkipped == true) ) then // INLINED!!
        return
    else
        call DoNothing()
    endif
    // ///
    call CameraSetupApplyForPlayer(true, gg_cam_Cin_4_Victory_2_a, Player(0), 0)
    call CameraSetupApplyForPlayerSmooth(true, gg_cam_Cin_4_Victory_2_b, Player(0), 8.00, 1.00, 3.00, 2.00)
    call SetSkyModel("Environment\\Sky\\LordaeronFallSky\\LordaeronFallSky.mdl")
    // ///
    call SetTerrainFogExBJ(0, 500.00, 3000.00, 0.50, 2.00, 5.00, 3.00)
    call SetUnitPositionLocFacingBJ(udg_ZuluhedCinematic, GetRectCenter(gg_rct_Cin_1_Maim_1), 180.00)
    call IssuePointOrderLocBJ(udg_ZuluhedCinematic, "move", GetRectCenter(gg_rct_ZuluhedExit))
    call TriggerSleepAction(6.00)
    if ( (udg_VictorySkipped == true) ) then // INLINED!!
        return
    else
        call DoNothing()
    endif
    // ///
    call CinematicFadeBJ(bj_CINEFADETYPE_FADEOUT, 2, "ReplaceableTextures\\CameraMasks\\White_mask.blp", 0, 0, 0, 0)
    call TriggerSleepAction(2.00)
    if ( (udg_VictorySkipped == true) ) then // INLINED!!
        return
    else
        call DoNothing()
    endif
    // ///
    // ///
    // xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
    // CINEMATIC ENDS - Cinematic Can No Longer Be Skipped
    // xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
    // ///
    call ConditionalTriggerExecute(gg_trg_Victory_Cleanup)
endfunction

