function Trig_LightningStrike_Actions takes nothing returns nothing
    // //
    call CinematicFadeBJ(bj_CINEFADETYPE_FADEOUTIN, 0.30, "ReplaceableTextures\\CameraMasks\\White_mask.blp", 100.00, 100.00, 100.00, 25.00)
    // //
    if ( Trig_LightningStrike_Func012C() ) then
        set udg_LightningStrikePoint=GetRandomLocInRect(RectFromCenterSizeBJ(GetCameraTargetPositionLoc(), 2048.00, 2048.00))
        call AddSpecialEffectLocBJ(udg_LightningStrikePoint, "Doodads\\Cinematic\\Lightningbolt\\Lightningbolt.mdl")
        set udg_LightningBoltWeather=GetLastCreatedEffectBJ()
        // //
        call ForGroupBJ(GetUnitsInRectMatching(RectFromCenterSizeBJ(udg_LightningStrikePoint, 64.00, 64.00), Condition(function Trig_LightningStrike_Func012Func011001002)), function Trig_LightningStrike_Func012Func011A)
        call ForGroupBJ(GetUnitsInRectMatching(RectFromCenterSizeBJ(udg_LightningStrikePoint, 64.00, 64.00), Condition(function Trig_LightningStrike_Func012Func012001002)), function Trig_LightningStrike_Func012Func012A)
        // //
        call AddSpecialEffectLocBJ(udg_LightningStrikePoint, "Abilities\\Spells\\Human\\Thunderclap\\ThunderClapCaster.mdl")
        set udg_LightningStrikeWeather=GetLastCreatedEffectBJ()
        call TriggerSleepAction(0.50)
        call DestroyEffectBJ(udg_LightningBoltWeather)
        // //
        call EnumDestructablesInRectAll(RectFromCenterSizeBJ(udg_LightningStrikePoint, 32.00, 32.00), function Trig_LightningStrike_Func012Func019A)
        // //
        if ( Trig_LightningStrike_Func012Func021C() ) then
            call CreateNUnitsAtLoc(1, 'u005', Player(PLAYER_NEUTRAL_AGGRESSIVE), udg_LightningStrikePoint, bj_UNIT_FACING)
            set udg_FireFromLighning=GetLastCreatedUnit()
            call UnitApplyTimedLifeBJ(10.00, 'BTLF', udg_FireFromLighning)
        else
            call DoNothing()
        endif
        // //
        call TriggerSleepAction(0.50)
        call DestroyEffectBJ(udg_LightningStrikeWeather)
        // //
        call KillDestructable(udg_DamagedDestructableLightning)
        // //
        call RemoveLocation(udg_LightningStrikePoint)
        call TriggerSleepAction(1.00)
        call RemoveDestructable(udg_DamagedDestructableLightning)
    else
    endif
    // //
    call PlaySoundBJ(gg_snd_RollingThunder1)
endfunction

