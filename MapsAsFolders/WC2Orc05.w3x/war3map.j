        return false
    endif
    return true
endfunction

function Trig_FadeCameraFocus_Actions takes nothing returns nothing
    set bj_forLoopAIndex=1
    set bj_forLoopAIndexEnd=100
    loop
        exitwhen bj_forLoopAIndex > bj_forLoopAIndexEnd
        if ( Trig_FadeCameraFocus_Func002Func001C() ) then
            call CameraSetFocalDistance(udg_FocalDistance)
            call CameraSetDepthOfFieldScale(udg_DepthOfField)
            call TriggerSleepAction(0.05)
            set udg_FocalDistance=( udg_FocalDistance + 40.00 )
            set udg_DepthOfField=( udg_DepthOfField + 20.00 )
        else
        endif
        set bj_forLoopAIndex=bj_forLoopAIndex + 1
    endloop
endfunction

