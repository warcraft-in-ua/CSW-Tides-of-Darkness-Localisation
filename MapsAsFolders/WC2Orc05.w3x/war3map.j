function Trig_GoblinCinematicQ_Actions takes nothing returns nothing
    call DisableTrigger(GetTriggeringTrigger())
    // ///
    call ForGroupBJ(GetUnitsSelectedAll(Player(0)), function Trig_GoblinCinematicQ_Func003002)
    call ClearSelectionForPlayer(Player(0))
    call ConditionalTriggerExecute(gg_trg_OilUIHide)
    call CinematicModeBJ(true, GetPlayersAll())
    // ///
    call CinematicFadeBJ(bj_CINEFADETYPE_FADEOUT, 1.00, "ReplaceableTextures\\CameraMasks\\White_mask.blp", 0, 0, 0, 0)
    call TriggerSleepAction(1.00)
    // ///
    // SetEnv and Actors
    // ///
    call ConditionalTriggerExecute(gg_trg_GoblinCinematicSetUp)
    // ///
    call CameraSetupApplyForPlayer(true, gg_cam_GoblinCinShipsArrive, Player(0), 0)
    call IssuePointOrderLocBJ(gg_unit_o60R_0035, "move", GetRectCenter(gg_rct_GoblinCinDestroyer03))
    call IssuePointOrderLocBJ(gg_unit_o60Q_0031, "move", GetRectCenter(gg_rct_GoblinCinTransport01))
    call IssuePointOrderLocBJ(gg_unit_o60Q_0032, "move", GetRectCenter(gg_rct_GoblinCinTransport02))
    call IssuePointOrderLocBJ(gg_unit_o60R_0037, "move", GetRectCenter(gg_rct_GoblinCinDestroyer03Start))
    call IssuePointOrderLocBJ(gg_unit_o60R_0034, "move", GetRectCenter(gg_rct_GoblinCinDestroyer02))
    call IssuePointOrderLocBJ(gg_unit_o60R_0036, "move", GetRectCenter(gg_rct_GoblinCinDestroyer01))
    call IssuePointOrderLocBJ(udg_EnemySubmarine, "move", GetRectCenter(gg_rct_GoblinCinSubmarineAttack))
    // ///
    call TriggerSleepAction(1.00)
    // ///
    call PlayThematicMusicBJ("OrcTheme")
    call CinematicFadeBJ(bj_CINEFADETYPE_FADEIN, 1.00, "ReplaceableTextures\\CameraMasks\\White_mask.blp", 0, 0, 0, 0)
    call TriggerSleepAction(1.00)
    // ///
    set udg_GoblinCinematic=true
    // ///
    // Ships are floating
    call CameraSetupApplyForPlayer(true, gg_cam_GoblinCinSubmarineAttack, Player(0), 4.00)
    call TriggerSleepAction(( DistanceBetweenPoints(GetUnitLoc(udg_EnemySubmarine), GetRectCenter(gg_rct_GoblinCinSubmarineAttack)) / GetUnitMoveSpeed(udg_EnemySubmarine) ))
    if ( (udg_GoblinCinematicSkipped == true) ) then // INLINED!!
        return
    else
        call DoNothing()
    endif
    // ///
    // ///
    // Submarine attacks
    call PauseUnitBJ(true, gg_unit_o60R_0036)
    call PauseUnitBJ(true, gg_unit_o60R_0037)
    call IssueTargetOrderBJ(udg_EnemySubmarine, "attack", gg_unit_o60R_0036)
    // ///
    call TriggerSleepAction(1.00)
    if ( (udg_GoblinCinematicSkipped == true) ) then // INLINED!!
        return
    else
        call DoNothing()
    endif
    // ///
    call IssuePointOrderLocBJ(gg_unit_o60Q_0031, "move", GetRectCenter(gg_rct_GoblinCinTransport01Escape))
    call IssuePointOrderLocBJ(gg_unit_o60Q_0032, "move", GetRectCenter(gg_rct_GoblinCinTransport02Escape))
    // ///
    call PlayDialogueFromSpeakerTypeEx(bj_FORCE_PLAYER[0], Player(20), 'N60K', GetCameraTargetPositionLoc(), gg_snd_WC2Orc05Maim_SubMarine1, bj_TIMETYPE_SUB, 2.00, true)
    if ( (udg_GoblinCinematicSkipped == true) ) then // INLINED!!
        return
    else
        call DoNothing()
    endif
    // ///
    // ///
    call CinematicFadeBJ(bj_CINEFADETYPE_FADEOUT, 1.00, "ReplaceableTextures\\CameraMasks\\White_mask.blp", 0, 0, 0, 0)
    call TriggerSleepAction(1.00)
    // ///
    call CameraSetupApplyForPlayer(true, gg_cam_GoblinCinZeppelin01, Player(0), 0)
    call CameraSetupApplyForPlayer(true, gg_cam_SubmarineDestroyed, Player(0), ( ( DistanceBetweenPoints(GetUnitLoc(udg_CinematicZeppelin), GetRectCenter(gg_rct_GoblinCinSubmarineAttack)) / GetUnitMoveSpeed(udg_CinematicZeppelin) ) + 1 ))
    // ///
    call CinematicFadeBJ(bj_CINEFADETYPE_FADEIN, 1.00, "ReplaceableTextures\\CameraMasks\\White_mask.blp", 0, 0, 0, 0)
    call TriggerSleepAction(1.00)
    if ( (udg_GoblinCinematicSkipped == true) ) then // INLINED!!
        return
    else
        call DoNothing()
    endif
    // ///
    // Zepelin arrives
    // ///
    call IssuePointOrderLocBJ(udg_CinematicZeppelin, "move", GetRectCenter(gg_rct_GoblinCinSubmarineAttack))
    call PlaySoundOnUnitBJ(gg_snd_CS_orc3_outro_zepp, 100.00, udg_CinematicZeppelin)
    call TriggerSleepAction(( DistanceBetweenPoints(GetUnitLoc(udg_CinematicZeppelin), GetRectCenter(gg_rct_GoblinCinSubmarineAttack)) / GetUnitMoveSpeed(udg_CinematicZeppelin) ))
    if ( (udg_GoblinCinematicSkipped == true) ) then // INLINED!!
        return
    else
        call DoNothing()
    endif
    // ///
    call PlayDialogueFromSpeakerEx(bj_FORCE_PLAYER[0], udg_CinematicZeppelin, 'h000', gg_snd_WC2Orc05Goblin_Submarine1, bj_TIMETYPE_ADD, 0, true)
    if ( (udg_GoblinCinematicSkipped == true) ) then // INLINED!!
        return
    else
        call DoNothing()
    endif
    // ///
    call IssueTargetOrderBJ(udg_CinematicZeppelin, "attack", udg_EnemySubmarine)
    call TriggerSleepAction(2.00)
    call SetUnitAnimation(udg_CinematicZeppelin, "spell throw")
    call QueueUnitAnimationBJ(udg_CinematicZeppelin, "stand")
    if ( (udg_GoblinCinematicSkipped == true) ) then // INLINED!!
        return
    else
        call DoNothing()
    endif
    call TriggerSleepAction(1.50)
    if ( (udg_GoblinCinematicSkipped == true) ) then // INLINED!!
        return
    else
        call DoNothing()
    endif
    call AddSpecialEffectLocBJ(GetRectCenter(gg_rct_GoblinCinSubmarineAttack), "Objects\\Spawnmodels\\Naga\\NagaDeath\\NagaDeath.mdl")
    set udg_SubmarineExplosionSE=GetLastCreatedEffectBJ()
    call BlzSetSpecialEffectZ(udg_SubmarineExplosionSE, - 166.40)
    call KillUnit(udg_EnemySubmarine)
    call TriggerSleepAction(3.00)
    if ( (udg_GoblinCinematicSkipped == true) ) then // INLINED!!
        return
    else
        call DoNothing()
    endif
    call DestroyEffectBJ(udg_SubmarineExplosionSE)
    // ///
    call CameraSetupApplyForPlayer(true, gg_cam_GoblinCinUnload, Player(0), ( ( DistanceBetweenPoints(GetUnitLoc(udg_CinematicZeppelin), GetRectCenter(gg_rct_GoblinCinGoblin01)) / GetUnitMoveSpeed(udg_CinematicZeppelin) ) - 0.50 ))
    call IssuePointOrderLocBJ(udg_CinematicZeppelin, "move", GetRectCenter(gg_rct_GoblinCinGoblin01))
    call PlaySoundOnUnitBJ(gg_snd_CS_orc1x_vol_quest_intro_zepp_arrive, 100.00, udg_CinematicZeppelin)
    call TriggerSleepAction(( DistanceBetweenPoints(GetUnitLoc(udg_CinematicZeppelin), GetRectCenter(gg_rct_GoblinCinGoblin01)) / GetUnitMoveSpeed(udg_CinematicZeppelin) ))
    if ( (udg_GoblinCinematicSkipped == true) ) then // INLINED!!
        return
    else
        call DoNothing()
    endif
    // ///
    // Zepelin Unloads
    // ///
    call TriggerSleepAction(1.00)
    if ( (udg_GoblinCinematicSkipped == true) ) then // INLINED!!
        return
    else
        call DoNothing()
    endif
    call SetUnitAnimation(udg_CinematicZeppelin, "spell")
    call QueueUnitAnimationBJ(udg_CinematicZeppelin, "stand")
    call TriggerSleepAction(1.00)
    if ( (udg_GoblinCinematicSkipped == true) ) then // INLINED!!
        return
    else
        call DoNothing()
    endif
    call ShowUnitShow(udg_GoblinCinEngineer)
    call PlaySoundAtPointBJ(gg_snd_Loading, 50.00, GetRectCenter(gg_rct_GoblinCinGoblin01), 0)
    call TriggerSleepAction(1.00)
    if ( (udg_GoblinCinematicSkipped == true) ) then // INLINED!!
        return
    else
        call DoNothing()
    endif
    call IssuePointOrderLocBJ(udg_GoblinCinEngineer, "move", GetRectCenter(gg_rct_GoblinCinGoblin02))
    // ///
    call PlayDialogueFromSpeakerEx(bj_FORCE_PLAYER[0], udg_GoblinCinEngineer, 'h609', gg_snd_WC2Orc05Goblin_Zeppelin1, bj_TIMETYPE_ADD, 0, true)
    if ( (udg_GoblinCinematicSkipped == true) ) then // INLINED!!
        return
    else
        call DoNothing()
    endif
    // ///
    call CameraSetupApplyForPlayer(true, gg_cam_GoblinCinTalk01, Player(0), GetSoundDurationBJ(gg_snd_WC2Orc05Zuluhed_Submarine1))
    // //
    call SetUnitAnimation(udg_FakeZuluhed, "cinematic standard one")
    call PlayDialogueFromSpeakerEx(bj_FORCE_PLAYER[0], udg_FakeZuluhed, 'O61F', gg_snd_WC2Orc05Zuluhed_Submarine1, bj_TIMETYPE_ADD, 0, true)
    if ( (udg_GoblinCinematicSkipped == true) ) then // INLINED!!
        return
    else
        call DoNothing()
    endif
    call SetUnitAnimation(udg_FakeZuluhed, "cinematic listening one")
    // //
    call PlayDialogueFromSpeakerEx(bj_FORCE_PLAYER[0], udg_GoblinCinEngineer, 'h609', gg_snd_WC2Orc05Goblin_Zeppelin2, bj_TIMETYPE_ADD, 0, true)
    if ( (udg_GoblinCinematicSkipped == true) ) then // INLINED!!
        return
    else
        call DoNothing()
    endif
    // ///
    call IssueBuildOrderByIdLocBJ(udg_GoblinCinEngineer, 'o000', GetRectCenter(gg_rct_GoblinLab))
    call SetUnitFacingToFaceLocTimed(udg_FakeZuluhed, GetRectCenter(gg_rct_GoblinLab), ( DistanceBetweenPoints(GetUnitLoc(udg_GoblinCinEngineer), GetRectCenter(gg_rct_GoblinLab)) / GetUnitMoveSpeed(udg_GoblinCinEngineer) ))
    // ///
    call CameraSetupApplyForPlayer(true, gg_cam_GoblinCinTalk02, Player(0), ( GetSoundDurationBJ(gg_snd_WC2Orc05Utok_Zepplin1) + 2.00 ))
    // //
    call SetUnitAnimation(udg_FakeUtok, "cinematic agree one")
    call QueueUnitAnimationBJ(udg_FakeUtok, "cinematic standard one")
    call PlayDialogueFromSpeakerEx(bj_FORCE_PLAYER[0], udg_FakeUtok, 'o605', gg_snd_WC2Orc05Utok_Zepplin1, bj_TIMETYPE_ADD, 0, true)
    if ( (udg_GoblinCinematicSkipped == true) ) then // INLINED!!
        return
    else
        call DoNothing()
    endif
    call SetUnitAnimation(udg_FakeUtok, "cinematic listening one")
    // //
    call TriggerSleepAction(3.00)
    if ( (udg_GoblinCinematicSkipped == true) ) then // INLINED!!
        return
    else
        call DoNothing()
    endif
    call ConditionalTriggerExecute(gg_trg_GoblinCinematicCleanUp)
