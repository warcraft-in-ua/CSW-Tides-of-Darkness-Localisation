    call CinematicFadeBJ(bj_CINEFADETYPE_FADEIN, 1.00, "ReplaceableTextures\\CameraMasks\\White_mask.blp", 0, 0, 0, 0)
    call TriggerSleepAction(1.00)
    // ///
    // Cinematic Started and can be skipped now
    set udg_DunModrNorthCinematic=true
    // ///
    call CameraSetupApplyForPlayer(true, gg_cam_EnterObervationPoint, Player(0), 10.00)
    // ///
    call TriggerSleepAction(( DistanceBetweenPoints(GetUnitLoc(udg_FakeMaim), GetRectCenter(gg_rct_MidCinRend02)) / GetUnitMoveSpeed(udg_FakeMaim) ))
    if ( (udg_DunModrNothCinSkipped == true) ) then // INLINED!!
