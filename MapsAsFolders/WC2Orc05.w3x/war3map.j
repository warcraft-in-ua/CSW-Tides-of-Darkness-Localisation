        return
    else
        call DoNothing()
    endif
    // ///
    call TriggerSleepAction(5.00)
    if ( (udg_DunModrNothCinSkipped == true) ) then // INLINED!!
        return
    else
        call DoNothing()
    endif
    // ///
    call CinematicFadeBJ(bj_CINEFADETYPE_FADEOUT, 1.00, "ReplaceableTextures\\CameraMasks\\White_mask.blp", 0, 0, 0, 0)
    call TriggerSleepAction(1.00)
    // ///
    call CameraSetupApplyForPlayer(true, gg_cam_DunModr05, Player(0), 0)
    // ///
    // TelescopeEffect
    // ///
    set udg_FocalDistance=400.00
    set udg_DepthOfField=200.00
    call SetCameraFieldForPlayer(Player(0), CAMERA_FIELD_FIELD_OF_VIEW, 50.00, 4.00)
    call CameraSetSourceNoiseForPlayer(Player(0), 20.00, 0.20)
    call EnableTrigger(gg_trg_FadeCameraFocus)
    call ConditionalTriggerExecute(gg_trg_FadeCameraFocus)
    // ///
    call CinematicFilterGenericBJ(1.00, BLEND_MODE_BLEND, "ReplaceableTextures\\CameraMasks\\Scope_Mask.blp", 100, 100, 100, 0.00, 100.00, 100.00, 100.00, 0.00)
    call TriggerSleepAction(1.00)
    if ( (udg_DunModrNothCinSkipped == true) ) then // INLINED!!
