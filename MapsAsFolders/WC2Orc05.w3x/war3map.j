//===========================================================================
// Trigger: NextLevelPrep
//===========================================================================
function Trig_NextLevelPrep_Func008C takes nothing returns boolean
    if ( not ( IsUnitAliveBJ(udg_Utok) == true ) ) then
        return false
    endif
    return true
endfunction

function Trig_NextLevelPrep_Func012C takes nothing returns boolean
    if ( not ( udg_DragonItemsFound == true ) ) then
        return false
    endif
    return true
endfunction

function Trig_NextLevelPrep_Actions takes nothing returns nothing
    call DisableTrigger(GetTriggeringTrigger())
    // SaveHeroues
    // cache
    call TriggerSleepAction(0.10)
    // //
    call StoreUnitBJ(udg_Zuluhed, "Zuluhed", "WC2Orc05", udg_Cache)
    // //
    if ( Trig_NextLevelPrep_Func008C() ) then
        call StoreUnitBJ(udg_Utok, "Utok", "WC2Orc05", udg_Cache)
    else
        call CreateNUnitsAtLoc(1, 'o605', Player(0), GetRectCenter(gg_rct_WindElementals), bj_UNIT_FACING)
        set udg_Utok=GetLastCreatedUnit()
        call StoreUnitBJ(udg_Utok, "Utok", "WC2Orc05", udg_Cache)
    endif
    // //
    call StoreUnitBJ(udg_Maim, "Maim", "WC2Orc05", udg_Cache)
    // //
    if ( Trig_NextLevelPrep_Func012C() ) then
        call StoreBooleanBJ(true, "ItemDragonAquired", "WC2Orc05", udg_Cache)
    else
        call RemoveItem(GetItemOfTypeFromUnitBJ(udg_Zuluhed, 'I002'))
        // //
        call RemoveItem(GetItemOfTypeFromUnitBJ(udg_Zuluhed, 'I60Q'))
        call RemoveItem(GetItemOfTypeFromUnitBJ(udg_Zuluhed, 'I60P'))
        call RemoveItem(GetItemOfTypeFromUnitBJ(udg_Zuluhed, 'I60N'))
        // //
        call RemoveItem(GetItemOfTypeFromUnitBJ(udg_Maim, 'I60N'))
        call RemoveItem(GetItemOfTypeFromUnitBJ(udg_Maim, 'I60P'))
        call RemoveItem(GetItemOfTypeFromUnitBJ(udg_Maim, 'I60Q'))
        // //
        call RemoveItem(GetItemOfTypeFromUnitBJ(udg_Utok, 'I60Q'))
        call RemoveItem(GetItemOfTypeFromUnitBJ(udg_Utok, 'I60P'))
        call RemoveItem(GetItemOfTypeFromUnitBJ(udg_Utok, 'I60N'))
        // //
        call StoreBooleanBJ(false, "ItemDragonPartsAquired", "WC2Orc05", udg_Cache)
    endif
    // //
    call StoreBooleanBJ(true, "Completed", "WC2Orc05", udg_Cache)
    call TriggerSleepAction(0.10)
    call SaveGameCacheBJ(udg_Cache)
    // NextLevel
    call ShowCustomCampaignButton(true, 8)
    call SetNextLevelBJ("WC2Orc06.w3x")
    call CustomVictoryBJ(Player(0), true, false)
endfunction

