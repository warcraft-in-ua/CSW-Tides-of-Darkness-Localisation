    call ForGroupBJ(GetUnitsOfPlayerMatching(Player(21), Condition(function Trig_GoblinCinematicCleanUp_Func032001002)), function Trig_GoblinCinematicCleanUp_Func032A)
    call ForGroupBJ(udg_HiddenUnits, function Trig_GoblinCinematicCleanUp_Func033002)
    call GroupClear(udg_HiddenUnits)
    // ///
    call DestroyGroup(udg_DragonmawShips)
    // ///
    call CinematicFadeBJ(bj_CINEFADETYPE_FADEIN, 1.00, "ReplaceableTextures\\CameraMasks\\White_mask.blp", 0, 0, 0, 0)
    call TriggerSleepAction(1.00)
