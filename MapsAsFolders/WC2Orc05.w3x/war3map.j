    // ///
    call TriggerSleepAction(bj_QUEUE_DELAY_QUEST)
    call ConditionalTriggerExecute(gg_trg_SaveCheckpoint)
    call TriggerSleepAction(bj_QUEUE_DELAY_QUEST)
    call ConditionalTriggerExecute(gg_trg_MainQuestCaptureTolBaradDiscovered)
    call QueuedTriggerAddBJ(gg_trg_ZeppelinHint, true)
endfunction

