    call PauseUnitBJ(false, gg_unit_h60Y_0369)
    call PauseUnitBJ(false, gg_unit_h60Y_0207)
    // //
    // //
    call SetPlayerAllianceStateBJ(Player(0), Player(12), bj_ALLIANCE_UNALLIED)
    call SetPlayerAllianceStateBJ(Player(12), Player(0), bj_ALLIANCE_UNALLIED)
    call SetPlayerAllianceStateBJ(Player(0), Player(17), bj_ALLIANCE_UNALLIED)
    call SetPlayerAllianceStateBJ(Player(17), Player(0), bj_ALLIANCE_UNALLIED)
    // //
    call CinematicModeBJ(false, GetPlayersAll())
    call ConditionalTriggerExecute(gg_trg_OilUIShow)
    call SelectGroupForPlayerBJ(udg_SelectedUnits, Player(0))
    call GroupClear(udg_SelectedUnits)
    // ///
    call QueuedTriggerRemoveBJ(GetTriggeringTrigger())
    call QueuedTriggerAddBJ(gg_trg_DukeMaimDialogueQ, true)
