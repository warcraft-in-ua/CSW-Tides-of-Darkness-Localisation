        call RemoveItem(GetItemOfTypeFromUnitBJ(udg_Utok, 'I60Q'))
        call RemoveItem(GetItemOfTypeFromUnitBJ(udg_Utok, 'I60N'))
        if ( Trig_DragonItem01VO_Func005Func011C() ) then
            call CreateItemLoc('I002', GetUnitLoc(GetManipulatingUnit()))
            set udg_UnassembledDragonTotem=GetLastCreatedItem()
