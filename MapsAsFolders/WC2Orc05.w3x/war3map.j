function Trig_DunModrSouthCinematicCleanUp_Actions takes nothing returns nothing
    call DisableTrigger(GetTriggeringTrigger())
    set udg_DunModrSouthCinematic=false
    // ///
    call StopMusicBJ(true)
    call CinematicFadeBJ(bj_CINEFADETYPE_FADEOUT, 1.00, "ReplaceableTextures\\CameraMasks\\White_mask.blp", 0, 0, 0, 0)
    call TriggerSleepAction(1.00)
    // ///
    call EnumItemsInRectBJ(gg_rct_QMDunModrIcon02, function Trig_DunModrSouthCinematicCleanUp_Func008002)
    call RemoveUnit(udg_FakeMaim)
    call RemoveUnit(udg_FakeZuluhed)
    if ( Trig_DunModrSouthCinematicCleanUp_Func011C() ) then
        call StopSoundBJ(gg_snd_WC2Orc05Zuluhed_DunModr1, false)
        call StopSoundBJ(gg_snd_WC2Orc05Utok_WarnTheIsland, false)
        call StopSoundBJ(gg_snd_WC2Orc05Maim_LetThemCome1, false)
        call StopSoundBJ(gg_snd_WC2Orc05Grunt1, false)
        call RemoveUnit(udg_CinematicGrunt)
    else
    endif
    // ///
    // ResetBTGStartingCamp
    call SetPlayerStateBJ(Player(0), PLAYER_STATE_RESOURCE_FOOD_CAP, 0)
    call BlzSetUnitName(gg_unit_o61Y_0068, "Toiler")
    call ResetUnitAnimation(gg_unit_o005_0041)
    call ResetUnitAnimation(gg_unit_o60X_0006)
    call ResetUnitAnimation(gg_unit_o60X_0005)
    call ResetUnitAnimation(gg_unit_o60X_0042)
    call ResetUnitAnimation(gg_unit_o60V_0029)
    call ForGroupBJ(GetUnitsInRectOfPlayer(gg_rct_BlackToothGrinCamp, Player(20)), function Trig_DunModrSouthCinematicCleanUp_Func021002)
    call SetUnitPositionLocFacingLocBJ(gg_unit_o61Y_0067, GetRectCenter(gg_rct_PeonMove03), GetRectCenter(gg_rct_PeonMoveOrder03))
    call SetUnitPositionLocFacingLocBJ(gg_unit_o61Y_0066, GetRectCenter(gg_rct_PeonMove01), GetRectCenter(gg_rct_PeonMoveOrder01))
    call SetUnitPositionLocFacingLocBJ(gg_unit_o61Y_0068, GetRectCenter(gg_rct_PeonMove02), GetRectCenter(gg_rct_PeonMoveOrder02))
    call ForGroupBJ(udg_BTGInitialGroup, function Trig_DunModrSouthCinematicCleanUp_Func025002)
    call DestroyGroup(udg_BTGInitialGroup)
    call IssuePointOrderLocBJ(gg_unit_o61Y_0068, "move", GetRectCenter(gg_rct_PeonMoveOrder02))
    call IssuePointOrderLocBJ(gg_unit_o61Y_0067, "move", GetRectCenter(gg_rct_PeonMoveOrder03))
    call IssuePointOrderLocBJ(gg_unit_o61Y_0066, "move", GetRectCenter(gg_rct_PeonMoveOrder01))
    // Deactivate Mercenary Camp
    call UnitRemoveAbilityBJ('Ane2', gg_unit_nmr0_0055)
    // Unhide Dwarves and give access to them
    call SetDestructableInvulnerableBJ(gg_dest_B014_2601, false)
    call ForGroupBJ(udg_HiddenDwarvenUnits, function Trig_DunModrSouthCinematicCleanUp_Func034002)
    call DestroyGroup(udg_HiddenDwarvenUnits)
    // Unhide and Unpause P1 units
    call ForGroupBJ(udg_HiddenUnits, function Trig_DunModrSouthCinematicCleanUp_Func037A)
    call ForGroupBJ(udg_HiddenUnits, function Trig_DunModrSouthCinematicCleanUp_Func038A)
    call ForGroupBJ(GetUnitsOfPlayerAll(Player(0)), function Trig_DunModrSouthCinematicCleanUp_Func039A)
    // //
    call ResetUnitLookAt(udg_Utok)
    call SetUnitBlendTimeBJ(udg_Utok, 0.15)
    call AddUnitAnimationPropertiesBJ(false, "cinematic", udg_Utok)
    call SetUnitMoveSpeed(udg_Utok, GetUnitDefaultMoveSpeed(udg_Utok))
    call BlzSetUnitRealFieldBJ(udg_Utok, UNIT_RF_ANIMATION_WALK_SPEED, 270.00)
    call UnitRemoveAbilityBJ('Andt', udg_Utok)
    call UnitRemoveAbilityBJ('A01I', udg_Utok)
    call UnitAddAbilityBJ('A642', udg_Utok)
    call UnitAddAbilityBJ('A641', udg_Utok)
    call UnitAddAbilityBJ('Asal', udg_Utok)
    call IssuePointOrderLocBJ(udg_Utok, "move", GetRectCenter(gg_rct_MoveTroopsHere))
    // Give player access to North Camp
    call SetUnitOwner(gg_unit_o60X_0042, Player(0), false)
    call SetUnitOwner(gg_unit_o60X_0005, Player(0), false)
    call SetUnitOwner(gg_unit_o60X_0006, Player(0), false)
    call SetUnitOwner(gg_unit_o603_0045, Player(0), false)
    call SetUnitOwner(gg_unit_o603_0046, Player(0), false)
    call SetUnitOwner(gg_unit_o603_0048, Player(0), false)
    call SetUnitOwner(gg_unit_o603_0049, Player(0), false)
    // //
    call RemoveDestructable(gg_dest_YT51_0991)
    call ShowDestructableBJ(true, gg_dest_RW07_1276)
    call DestructableRestoreLife(gg_dest_RW07_1276, GetDestructableMaxLife(gg_dest_RW07_1276), false)
    // //
    call VolumeGroupResetBJ()
    call CameraSetupApplyForPlayer(true, gg_cam_ResetGameCameraDunModr, Player(0), 0)
    call ResetToGameCameraForPlayer(Player(0), 0)
    // ///
    call CinematicFadeBJ(bj_CINEFADETYPE_FADEIN, 1.00, "ReplaceableTextures\\CameraMasks\\White_mask.blp", 0, 0, 0, 0)
    call TriggerSleepAction(1.00)
    // ///
    call CinematicModeBJ(false, GetPlayersAll())
    call ConditionalTriggerExecute(gg_trg_OilUIShow)
    call SelectGroupForPlayerBJ(udg_SelectedUnits, Player(0))
    call GroupClear(udg_SelectedUnits)
    // ///
    call ConditionalTriggerExecute(gg_trg_MainQuestFortifyDiscovered)
    // Ready to Work
    call PanCameraToTimedLocForPlayer(Player(0), GetRectCenter(gg_rct_PeonMoveOrder02), 1.00)
    call TriggerSleepAction(4.00)
    call RescueUnitBJ(gg_unit_o61Y_0067, Player(0), false)
    call RescueUnitBJ(gg_unit_o61Y_0066, Player(0), false)
    call RescueUnitBJ(gg_unit_o61Y_0068, Player(0), false)
    // ///
    call ConditionalTriggerExecute(gg_trg_StartP06AI)
    call EnableTrigger(gg_trg_PlayerBaseDestroyed)
endfunction

