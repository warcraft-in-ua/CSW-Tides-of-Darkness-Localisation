    call SetUnitInvulnerable(udg_GoblinCinTransport, false)
    call ForGroupBJ(GetUnitsInRectOfPlayer(gg_rct_GoblinCinematicUnpause, Player(9)), function Trig_GoblinCinQ_Func027002)
    call IssueTargetOrderBJ(gg_unit_e000_0396, "attack", udg_GoblinCinTransport)
    call IssueTargetOrderBJ(gg_unit_e000_0395, "attack", udg_GoblinCinTransport)
