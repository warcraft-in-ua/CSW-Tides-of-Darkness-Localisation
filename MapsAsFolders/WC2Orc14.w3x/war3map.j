    call PlaySoundAtPointBJ(gg_snd_Dock, 100, GetRectCenter(gg_rct_GoblinShipArrives), 0)
    call CameraSetupApplyForPlayer(true, gg_cam_Kaboom, Player(0), ( ( DistanceBetweenPoints(GetRectCenter(gg_rct_GoblinIsDeployed), GetRectCenter(gg_rct_Kaboom)) / 270.00 ) + 1 ))
    call TriggerSleepAction(1.00)
    if ( (udg_GoblinCinematicSkipped == true) ) then // INLINED!!
        return
    else
