function Trig_QMCrossIslandDiscovered_Actions takes nothing returns nothing
    call DisableTrigger(GetTriggeringTrigger())
    call CreateQuestBJ(bj_QUESTTYPE_REQ_DISCOVERED, "TRIGSTR_3394", "TRIGSTR_3395", "ReplaceableTextures\\CommandButtons\\BTNArcaneObservatory.blp")
    set udg_QMCrossIsland=GetLastCreatedQuestBJ()
    call CreateQuestItemBJ(udg_QMCrossIsland, "TRIGSTR_3396")
    set udg_QMCrossIslandReq[1]=GetLastCreatedQuestItemBJ()
    call CreateQuestItemBJ(udg_QMCrossIsland, "TRIGSTR_3397")
    set udg_QMCrossIslandReq[2]=GetLastCreatedQuestItemBJ()
    call CreateQuestItemBJ(udg_QMCrossIsland, "TRIGSTR_3398")
    set udg_QMCrossIslandReq[3]=GetLastCreatedQuestItemBJ()
    call CreateQuestItemBJ(udg_QMCrossIsland, "TRIGSTR_3399")
    set udg_QMCrossIslandReq[4]=GetLastCreatedQuestItemBJ()
    call CreateQuestItemBJ(udg_QMCrossIsland, "TRIGSTR_3400")
    set udg_QMCrossIslandReq[5]=GetLastCreatedQuestItemBJ()
    call CreateQuestItemBJ(udg_QMCrossIsland, "TRIGSTR_3401")
    set udg_QMCrossIslandReq[6]=GetLastCreatedQuestItemBJ()
    // \Checkpoint
    call SaveGameCheckPointBJ("WC2Orc14SaveSecret", true)
    // /Checkpoint
    call TriggerSleepAction(bj_QUEUE_DELAY_QUEST)
    call QuestMessageBJ(GetPlayersAll(), bj_QUESTMESSAGE_DISCOVERED, "TRIGSTR_3402")
    // ***
    call CampaignMinimapIconUnitBJ(gg_unit_haro_0515, bj_CAMPPINGSTYLE_BONUS)
    set udg_QMCIObservatory[1]=GetLastCreatedMinimapIcon()
    call CampaignMinimapIconUnitBJ(gg_unit_haro_0512, bj_CAMPPINGSTYLE_BONUS)
    set udg_QMCIObservatory[2]=GetLastCreatedMinimapIcon()
    call CampaignMinimapIconUnitBJ(gg_unit_haro_0510, bj_CAMPPINGSTYLE_BONUS)
    set udg_QMCIObservatory[3]=GetLastCreatedMinimapIcon()
    call CampaignMinimapIconUnitBJ(gg_unit_h61P_0537, bj_CAMPPINGSTYLE_PRIMARY_RED)
    set udg_QMCIVioletCitadel=GetLastCreatedMinimapIcon()
    call CampaignMinimapIconUnitBJ(gg_unit_Hamg_0508, bj_CAMPPINGSTYLE_BOSS)
    set udg_QMCIMage=GetLastCreatedMinimapIcon()
    call EnableTrigger(gg_trg_QMCIObservatoriesDestroyedShieldOff)
    // ***
    call CreateLeaderboardBJ(GetPlayersAll(), "TRIGSTR_3416")
    set udg_ArcaneObservatoriesBoard=GetLastCreatedLeaderboard()
    call LeaderboardAddItemBJ(Player(15), udg_ArcaneObservatoriesBoard, "TRIGSTR_3417", udg_ObservatoriesDestroyed)
    call LeaderboardDisplayBJ(true, udg_ArcaneObservatoriesBoard)
endfunction

