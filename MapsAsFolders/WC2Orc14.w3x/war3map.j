    call TriggerSleepAction(( DistanceBetweenPoints(GetUnitLoc(udg_GoblinCinSappers), GetRectCenter(gg_rct_Kaboom)) / GetUnitMoveSpeed(udg_GoblinCinSappers) ))
    if ( (udg_GoblinCinematicSkipped == true) ) then // INLINED!!
        return
    else
        call DoNothing()
    endif
    call IssuePointOrderLocBJ(udg_GoblinCinSappers, "selfdestruct", GetRectCenter(gg_rct_Kaboom))
    call TriggerSleepAction(0.25)
    if ( (udg_GoblinCinematicSkipped == true) ) then // INLINED!!
