    //  
    // Executing Spell
    //  
    //  
    if ( Trig_Maim_I_Rampage_Func018C() ) then
        call UnitDamageTargetBJ(GetTriggerUnit(), GetSpellTargetUnit(), ( ( ( I2R(GetHeroStatBJ(bj_HEROSTAT_STR, GetTriggerUnit(), true)) + GetRandomReal(10.00, 20.00) ) + udg_Maim_Rampage_Damage ) * udg_Maim_Recklessness_Bonus ), ATTACK_TYPE_HERO, DAMAGE_TYPE_NORMAL)
