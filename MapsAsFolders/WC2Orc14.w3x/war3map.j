    return true
endfunction

function Trig_GoblinCinCleanUp_Func004002 takes nothing returns nothing
    call ShowUnitShow(GetEnumUnit())
endfunction

function Trig_GoblinCinCleanUp_Func005002 takes nothing returns nothing
    call GroupClear(udg_HiddenUnits)
endfunction

function Trig_GoblinCinCleanUp_Func016Func004Func001001 takes nothing returns boolean
    return ( GetDestructableTypeId(GetEnumDestructable()) == 'YTpb' )
endfunction

function Trig_GoblinCinCleanUp_Func016Func004A takes nothing returns nothing
    if ( (GetDestructableTypeId(GetEnumDestructable()) == 'YTpb') ) then // INLINED!!
        call RemoveDestructable(GetEnumDestructable())
    else
        call DoNothing()
    endif
endfunction

function Trig_GoblinCinCleanUp_Func016C takes nothing returns boolean
    if ( not ( udg_GoblinCinematicSkipped == true ) ) then
        return false
    endif
    if ( not ( IsDestructableAliveBJ(gg_dest_B00C_1843) == true ) ) then
        return false
    endif
    return true
endfunction

function Trig_GoblinCinCleanUp_Actions takes nothing returns nothing
    call DisableTrigger(GetTriggeringTrigger())
    if ( Trig_GoblinCinCleanUp_Func002C() ) then
        call CameraClearNoiseForPlayer(Player(0))
        call RemoveUnit(udg_GoblinCinSappers)
    else
    endif
    call RemoveUnit(udg_GoblinCinTransport)
    call ForGroupBJ(udg_HiddenUnits, function Trig_GoblinCinCleanUp_Func004002)
    call ForGroupBJ(udg_HiddenUnits, function Trig_GoblinCinCleanUp_Func005002)
    call PauseAllUnitsBJ(false)
    call CameraSetupApplyForPlayer(true, udg_CurrentCamera, Player(0), 0)
    // //
    call VolumeGroupResetBJ()
    call CinematicFadeBJ(bj_CINEFADETYPE_FADEIN, 1.00, "ReplaceableTextures\\CameraMasks\\Black_mask.blp", 0, 0, 0, 0)
    call TriggerSleepAction(1.00)
    call CinematicModeBJ(false, GetPlayersAll())
    call ConditionalTriggerExecute(gg_trg_OilUIShow)
    call SelectGroupForPlayerBJ(udg_SelectedUnits, Player(0))
    call GroupClear(udg_SelectedUnits)
    if ( Trig_GoblinCinCleanUp_Func016C() ) then
        call CameraSetupApplyForPlayer(true, gg_cam_GateDestroyedPort, Player(0), 1.00)
        call TriggerSleepAction(1.00)
        call KillDestructable(gg_dest_B00C_1843)
        call EnumDestructablesInRectAll(gg_rct_GatePB01, function Trig_GoblinCinCleanUp_Func016Func004A)
    else
    endif
    call QueuedTriggerRemoveBJ(gg_trg_GoblinCinQ)
    call SaveGameCheckPointBJ("WC2Orc14Save02", true)
    call TriggerSleepAction(bj_QUEUE_DELAY_QUEST)
    call QueuedTriggerAddBJ(gg_trg_QMSCDiscovered, true)
endfunction

