function Trig_MidgameCinematicQ_Actions takes nothing returns nothing
    call DisableTrigger(GetTriggeringTrigger())
    call ForGroupBJ(GetUnitsSelectedAll(Player(0)), function Trig_MidgameCinematicQ_Func002002)
    call ClearSelectionForPlayer(Player(0))
    set udg_CurrentCamera=GetCurrentCameraSetup()
    call ConditionalTriggerExecute(gg_trg_OilUIHide)
    call CinematicModeBJ(true, GetPlayersAll())
    call CinematicFadeBJ(bj_CINEFADETYPE_FADEOUT, 1.00, "ReplaceableTextures\\CameraMasks\\Black_mask.blp", 0, 0, 0, 0)
    call TriggerSleepAction(1.00)
    call ConditionalTriggerExecute(gg_trg_MidgameSetup)
    call CameraSetupApplyForPlayer(true, gg_cam_Mid01Shipyard, Player(0), 0)
    call CinematicFadeBJ(bj_CINEFADETYPE_FADEIN, 2, "ReplaceableTextures\\CameraMasks\\Black_mask.blp", 0, 0, 0, 0)
    call CameraSetupApplyForPlayer(true, gg_cam_Mid02PeonFalls, Player(0), ( ( DistanceBetweenPoints(GetUnitLoc(udg_Peon[4]), GetRectCenter(gg_rct_Peon03)) / GetUnitMoveSpeed(udg_Peon[4]) ) + 2.00 ))
    // Cinematic started and can be skipped now
    set udg_MidgameCinematic=true
    // //
    // Scene 1
    // //
    call PlayThematicMusicBJ("Warcraft2\\Music\\TheHorde.mp3")
    call PlaySoundOnUnitBJ(gg_snd_CS_Prologue2_Foley1, 100, udg_Peon[4])
    call IssuePointOrderLocBJ(udg_Peon[4], "move", GetRectCenter(gg_rct_Peon03))
    call TriggerSleepAction(( ( DistanceBetweenPoints(GetUnitLoc(udg_Peon[4]), GetRectCenter(gg_rct_Peon03)) / GetUnitMoveSpeed(udg_Peon[4]) ) - 0.50 ))
    if ( (udg_MidgameSkipped == true) ) then // INLINED!!
        return
    else
        call DoNothing()
    endif
    call SetUnitAnimation(udg_Peon[4], "cinematic injured one")
    call TriggerSleepAction(1.00)
    if ( (udg_MidgameSkipped == true) ) then // INLINED!!
        return
    else
        call DoNothing()
    endif
    call IssuePointOrderLocBJ(udg_FakeTharbek, "move", GetRectCenter(gg_rct_TharbekKicksPeonsAss))
    call PlaySoundOnUnitBJ(gg_snd_CS_Prologue2_Foley2, 100, udg_FakeTharbek)
    call SetUnitAnimation(udg_Peon[4], "cinematic injured two")
    call TriggerSleepAction(( DistanceBetweenPoints(GetUnitLoc(udg_FakeTharbek), GetRectCenter(gg_rct_TharbekKicksPeonsAss)) / GetUnitMoveSpeed(udg_FakeTharbek) ))
    if ( (udg_MidgameSkipped == true) ) then // INLINED!!
        return
    else
        call DoNothing()
    endif
    // Poke
    call SetUnitAnimation(udg_FakeTharbek, "attack defend")
    call QueueUnitAnimationBJ(udg_FakeTharbek, "cinematic angry one")
    // //
    call TransmissionFromUnitWithNameBJ(GetPlayersAll(), udg_FakeTharbek, "TRIGSTR_2916", gg_snd_WC2Orc14Tharbek2, "TRIGSTR_2917", bj_TIMETYPE_ADD, 0, false)
    call TriggerSleepAction(0.40)
    if ( (udg_MidgameSkipped == true) ) then // INLINED!!
        return
    else
        call DoNothing()
    endif
    call PlaySoundOnUnitBJ(gg_snd_MetalMediumSliceFlesh1, 50.00, udg_FakeTharbek)
    call TriggerSleepAction(0.60)
    if ( (udg_MidgameSkipped == true) ) then // INLINED!!
        return
    else
        call DoNothing()
    endif
    call IssuePointOrderLocBJ(udg_Peon[4], "move", GetRectCenter(gg_rct_Peon02))
    call AddUnitAnimationPropertiesBJ(true, "lumber", udg_Peon[4])
    call PlaySoundOnUnitBJ(gg_snd_CS_Prologue2_Foley1, 100, udg_Peon[4])
    call SetUnitFacingToFaceLocTimed(udg_FakeTharbek, GetRectCenter(gg_rct_Peon02), ( DistanceBetweenPoints(GetUnitLoc(udg_Peon[4]), GetRectCenter(gg_rct_Peon02)) / GetUnitMoveSpeed(udg_Peon[4]) ))
    call TriggerSleepAction(4.00)
    if ( (udg_MidgameSkipped == true) ) then // INLINED!!
        return
    else
        call DoNothing()
    endif
    call CameraSetupApplyForPlayer(true, gg_cam_Mid03TharbekPokes, Player(0), 5.00)
    call AddUnitAnimationPropertiesBJ(true, "cinematic", udg_FakeTharbek)
    call IssuePointOrderLocBJ(udg_FakeTharbek, "move", GetRectCenter(gg_rct_TharbekGoesAway))
    call PlaySoundOnUnitBJ(gg_snd_CS_HU_2_Intro_Foley2, 100, udg_FakeTharbek)
    call TriggerSleepAction(6.00)
    if ( (udg_MidgameSkipped == true) ) then // INLINED!!
        return
    else
        call DoNothing()
    endif
    call CinematicFadeBJ(bj_CINEFADETYPE_FADEOUT, 1.00, "ReplaceableTextures\\CameraMasks\\Black_mask.blp", 0, 0, 0, 0)
    call TriggerSleepAction(1.00)
    call CameraSetupApplyForPlayer(true, gg_cam_Mid04WarchiefLooksAtTheSea, Player(0), 0)
    call CameraSetFocalDistance(500.00)
    call CameraSetDepthOfFieldScale(200.00)
    call SetUnitPositionLocFacingLocBJ(udg_FakeTharbek, GetRectCenter(gg_rct_MidgameTharbek01), GetRectCenter(gg_rct_MidgameTharbek02))
    call IssuePointOrderLocBJ(udg_FakeDoomhammer, "move", GetRectCenter(gg_rct_MidgameOrgrim02))
    call IssuePointOrderLocBJ(udg_FakeTharbek, "move", GetRectCenter(gg_rct_MidgameTharbek02))
    call PlaySoundOnUnitBJ(gg_snd_CS_Prologue2_Foley1, 100, udg_FakeDoomhammer)
    call PlaySoundOnUnitBJ(gg_snd_CS_Prologue2_Foley101, 100, udg_FakeTharbek)
    call CinematicFadeBJ(bj_CINEFADETYPE_FADEIN, 2.00, "ReplaceableTextures\\CameraMasks\\Black_mask.blp", 0, 0, 0, 0)
    call TriggerSleepAction(1.00)
    if ( (udg_MidgameSkipped == true) ) then // INLINED!!
        return
    else
        call DoNothing()
    endif
    // //
    // Scene 2
    // //
    call CameraSetupApplyForPlayer(true, gg_cam_Mid05ShipsArrive, Player(0), 8.00)
    // //
    call PlaySoundBJ(gg_snd_TheHornOfCenarius)
    call TriggerSleepAction(4.00)
    if ( (udg_MidgameSkipped == true) ) then // INLINED!!
        return
    else
        call DoNothing()
    endif
    call TransmissionFromUnitWithNameBJ(GetPlayersAll(), udg_FakeDoomhammer, "TRIGSTR_2920", gg_snd_WC2Orc14Orgrim2, "TRIGSTR_2921", bj_TIMETYPE_ADD, 0, false)
