        endif
    endif
    // Maim_Recklessness_Bonus
    // ((Triggering unit) has buff Recjlessness (Maim)) Equal to True
    if ( Trig_Maim_I_Rampage_Func013C() ) then
        set udg_Maim_Recklessness_Bonus=1.50
    else
        set udg_Maim_Recklessness_Bonus=1.00
