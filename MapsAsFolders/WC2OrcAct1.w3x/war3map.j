        call SetWaterBaseColorBJ(udg_WaterColorRed, udg_WaterColorGreen, udg_WaterColorBlue, udg_WaterTransperancy)
        call TriggerSleepAction(0.50)
        set bj_forLoopBIndex=bj_forLoopBIndex + 1
    endloop
endfunction

