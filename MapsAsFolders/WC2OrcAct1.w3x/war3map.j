//===========================================================================
function Trig_WaterColorRun_Actions takes nothing returns nothing
    call DisableTrigger(GetTriggeringTrigger())
    set bj_forLoopBIndex=1
    set bj_forLoopBIndexEnd=12
    loop
        exitwhen bj_forLoopBIndex > bj_forLoopBIndexEnd
        set udg_WaterColorRed=( udg_WaterColorRed + 4.00 )
        set udg_WaterColorGreen=( udg_WaterColorGreen - 4.00 )
