function Trig_GulDanEscorted_Cin_Q_Actions takes nothing returns nothing
    call CameraSetupApplyForPlayer(true, gg_cam_CityEscort01, Player(0), 0)
    call SetSkyModel("Environment\\Sky\\PrologueSky\\PrologueSky.mdl")
    call SetDayNightModels("Environment\\DNC\\DNCLordaeron\\DNCLordaeronTerrain\\DNCLordaeronTerrain.mdl", "Environment\\DNC\\DNCLordaeron\\DNCLordaeronTerrain\\DNCLordaeronTerrain.mdl")
    call SetTimeOfDay(9.00)
    call ClearMapMusicBJ()
    call SetDoodadAnimationRectBJ("death", 'YOtf', gg_rct_StormwindCityFires)
    call SetDoodadAnimationRectBJ("death", 'YOfs', gg_rct_StormwindCityFires)
    call SetDoodadAnimationRectBJ("death", 'YOf3', gg_rct_StormwindCityFires)
    call SetUnitAnimation(gg_unit_o61D_0082, "cinematic injured two")
    call SetUnitAnimation(gg_unit_o004_0064, "cinematic standard two")
    call SetUnitAnimation(gg_unit_o002_0105, "cinematic standard one")
    call SetUnitAnimation(gg_unit_ogru_0103, "cinematic standard one")
    call SetUnitAnimation(gg_unit_o60G_0083, "cinematic standard three")
    call ForGroupBJ(GetUnitsInRectMatching(gg_rct_Corpses, Condition(function Trig_GulDanEscorted_Cin_Q_Func014001002)), function Trig_GulDanEscorted_Cin_Q_Func014A)
    call ResetUnitAnimation(gg_unit_n60D_0115)
    call EnumDestructablesInRectAll(gg_rct_FireIgnite, function Trig_GulDanEscorted_Cin_Q_Func016A)
    call ConditionalTriggerExecute(gg_trg_CityTour01)
    call TriggerSleepAction(0.50)
    call CinematicFadeBJ(bj_CINEFADETYPE_FADEIN, 1.00, "ReplaceableTextures\\CameraMasks\\White_mask.blp", 0, 0, 0, 0)
    call TriggerSleepAction(1.00)
    call PlayThematicMusicBJ("DarkAgents")
    // Cinematic Started and Can be Skipped
    if ( (udg_CinematicSkipped == true) ) then // INLINED!!
        return
    else
        call DoNothing()
    endif
    call TriggerSleepAction(7.00)
    if ( (udg_CinematicSkipped == true) ) then // INLINED!!
        return
    else
        call DoNothing()
    endif
    call TriggerSleepAction(8.00)
    if ( (udg_CinematicSkipped == true) ) then // INLINED!!
        return
    else
        call DoNothing()
    endif
    call VolumeGroupSetVolumeBJ(SOUND_VOLUMEGROUP_SPELLS, 0.00)
    call TriggerSleepAction(2.00)
    if ( (udg_CinematicSkipped == true) ) then // INLINED!!
        return
    else
        call DoNothing()
    endif
    call SetUnitAnimation(gg_unit_o001_0104, "spell")
    call QueueUnitAnimationBJ(gg_unit_o001_0104, "stand")
    call ModifyGateBJ(bj_GATEOPERATION_OPEN, gg_dest_B002_0058)
    call TriggerSleepAction(5.00)
    if ( (udg_CinematicSkipped == true) ) then // INLINED!!
        return
    else
        call DoNothing()
    endif
    call VolumeGroupSetVolumeBJ(SOUND_VOLUMEGROUP_SPELLS, 100.00)
endfunction

