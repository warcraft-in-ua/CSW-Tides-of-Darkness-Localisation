function Trig_IntroMountainSpawn_Actions takes nothing returns nothing
    if ( Trig_IntroMountainSpawn_Func001C() ) then
        set udg_IntroHoardChance=1
    else
        if ( Trig_IntroMountainSpawn_Func001Func001C() ) then
            set udg_IntroHoardChance=2
        else
            if ( Trig_IntroMountainSpawn_Func001Func001Func001C() ) then
                set udg_IntroHoardChance=3
            else
                if ( Trig_IntroMountainSpawn_Func001Func001Func001Func001C() ) then
                    set udg_IntroHoardChance=4
                else
                endif
            endif
        endif
    endif
    set udg_IntroHoardRegionChance=GetRandomInt(1, 3)
    if ( Trig_IntroMountainSpawn_Func003C() ) then
        set udg_IntroHoardRegion=gg_rct_HoardEnter03
    else
        if ( Trig_IntroMountainSpawn_Func003Func001C() ) then
            set udg_IntroHoardRegion=gg_rct_HoardEnter04
        else
            if ( Trig_IntroMountainSpawn_Func003Func001Func001C() ) then
                set udg_IntroHoardRegion=gg_rct_HoardEnter05
            else
            endif
        endif
    endif
    set udg_HoardOrigin=GetRandomLocInRect(udg_IntroHoardRegion)
    set udg_HoardDestination=GetRandomLocInRect(gg_rct_HoardExit02)
    if ( Trig_IntroMountainSpawn_Func006C() ) then
        call CreateNUnitsAtLoc(1, 'o005', Player(0), udg_HoardOrigin, 105.00)
    else
        if ( Trig_IntroMountainSpawn_Func006Func001C() ) then
            call CreateNUnitsAtLoc(1, 'o006', Player(0), udg_HoardOrigin, 105.00)
        else
            if ( Trig_IntroMountainSpawn_Func006Func001Func001C() ) then
                if ( Trig_IntroMountainSpawn_Func006Func001Func001Func001C() ) then
                    call CreateNUnitsAtLoc(1, 'n009', Player(0), udg_HoardOrigin, 105.00)
                else
                    call CreateNUnitsAtLoc(1, 'o006', Player(0), udg_HoardOrigin, 105.00)
                endif
            else
                if ( Trig_IntroMountainSpawn_Func006Func001Func001Func002C() ) then
                    set udg_IntroHoardChance_Catapult=GetRandomInt(1, 2)
                    if ( Trig_IntroMountainSpawn_Func006Func001Func001Func002Func002C() ) then
                        call CreateNUnitsAtLoc(1, 'o007', Player(0), udg_HoardOrigin, 105.00)
                    else
                        call CreateNUnitsAtLoc(1, 'o005', Player(0), udg_HoardOrigin, 105.00)
                    endif
                else
                endif
            endif
        endif
    endif
    call SetUnitPathing(GetLastCreatedUnit(), false)
    call SetUnitMoveSpeed(GetLastCreatedUnit(), 75.00)
    call IssuePointOrderLocBJ(GetLastCreatedUnit(), "move", udg_HoardDestination)
    call RemoveLocation(udg_HoardDestination)
    call RemoveLocation(udg_HoardOrigin)
endfunction

