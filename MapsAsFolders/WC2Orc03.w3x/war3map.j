    set udg_OilPatchVisibility[4]=GetLastCreatedFogModifier()
    call CampaignMinimapIconLocBJ(GetRectCenter(gg_rct_OilSplat05), bj_CAMPPINGSTYLE_PRIMARY)
    set udg_MainQuestIcon[4]=GetLastCreatedMinimapIcon()
    // //
    call PingMinimapLocForForce(bj_FORCE_PLAYER[0], GetRectCenter(gg_rct_OilSplat01), 5.00)
