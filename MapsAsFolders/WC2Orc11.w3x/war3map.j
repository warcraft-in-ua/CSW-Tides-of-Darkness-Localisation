//===========================================================================
function Trig_EarthSpawn_Conditions takes nothing returns boolean
    if ( not ( IsQuestItemCompleted(udg_QRSEarth) == false ) ) then
        return false
    endif
    if ( not ( udg_GuldanSummoning == true ) ) then
        return false
    endif
    return true
endfunction

function Trig_EarthSpawn_Func001Func002C takes nothing returns boolean
    if ( not ( IsTriggerQueuedBJ(gg_trg_WaygatesHint) == false ) ) then
        return false
    endif
    return true
endfunction

function Trig_EarthSpawn_Func001C takes nothing returns boolean
    if ( not ( IsTriggerEnabled(gg_trg_WaygatesHintQ) == true ) ) then
        return false
    endif
    return true
endfunction

function Trig_EarthSpawn_Func002Func001C takes nothing returns boolean
    if ( not ( GetDestructableTypeId(GetEnumDestructable()) == 'Yts1' ) ) then
        return false
    endif
    return true
endfunction

function Trig_EarthSpawn_Func002A takes nothing returns nothing
    if ( Trig_EarthSpawn_Func002Func001C() ) then
        call RemoveDestructable(GetEnumDestructable())
    else
    endif
endfunction

function Trig_EarthSpawn_Func021A takes nothing returns nothing
    call IssueTargetOrderBJ(GetEnumUnit(), "attack", udg_Guldan)
endfunction

function Trig_EarthSpawn_Actions takes nothing returns nothing
    if ( Trig_EarthSpawn_Func001C() ) then
        call DisableTrigger(gg_trg_WaygatesHintQ)
        if ( Trig_EarthSpawn_Func001Func002C() ) then
            call QueuedTriggerAddBJ(gg_trg_WaygatesHint, true)
        else
        endif
    else
    endif
    call EnumDestructablesInRectAll(gg_rct_DestroyTrees01, function Trig_EarthSpawn_Func002A)
    call AddSpecialEffectLocBJ(GetRectCenter(gg_rct_SpawnElf01), "Abilities\\Spells\\Human\\MassTeleport\\MassTeleportTo.mdl")
    set udg_TeleportEffect=GetLastCreatedEffectBJ()
    call CreateNUnitsAtLoc(1, 'hhes', Player(7), GetRectCenter(gg_rct_SpawnElf01), bj_UNIT_FACING)
    call GroupAddUnitSimple(GetLastCreatedUnit(), udg_ElvenDefendersEarth)
    call CreateNUnitsAtLoc(1, 'hmpr', Player(7), GetRectCenter(gg_rct_SpawnElf01), bj_UNIT_FACING)
    call GroupAddUnitSimple(GetLastCreatedUnit(), udg_ElvenDefendersEarth)
    call CreateNUnitsAtLoc(1, 'hsor', Player(7), GetRectCenter(gg_rct_SpawnElf01), bj_UNIT_FACING)
    call GroupAddUnitSimple(GetLastCreatedUnit(), udg_ElvenDefendersEarth)
    call CreateNUnitsAtLoc(1, 'hdhw', Player(7), GetRectCenter(gg_rct_SpawnElf01), bj_UNIT_FACING)
    call GroupAddUnitSimple(GetLastCreatedUnit(), udg_ElvenDefendersEarth)
    call CreateNUnitsAtLoc(1, 'n004', Player(7), GetRectCenter(gg_rct_SpawnElf01), bj_UNIT_FACING)
    call GroupAddUnitSimple(GetLastCreatedUnit(), udg_ElvenDefendersEarth)
    call CreateNUnitsAtLoc(1, 'e000', Player(7), GetRectCenter(gg_rct_SpawnElf01), bj_UNIT_FACING)
    call GroupAddUnitSimple(GetLastCreatedUnit(), udg_ElvenDefendersEarth)
    call CreateNUnitsAtLoc(1, 'h60O', Player(7), GetRectCenter(gg_rct_SpawnElf01), bj_UNIT_FACING)
    call GroupAddUnitSimple(GetLastCreatedUnit(), udg_ElvenDefendersEarth)
    call ForGroupBJ(udg_ElvenDefendersEarth, function Trig_EarthSpawn_Func021A)
    call TriggerSleepAction(5.00)
    call DestroyEffectBJ(udg_TeleportEffect)
    call TriggerSleepAction(10.00)
    call ConditionalTriggerExecute(GetTriggeringTrigger())
endfunction

