        call DoNothing()
    endif
    // //
    call SetTerrainFogExBJ(0, 1500.00, 4500.00, 0.50, 81.50, 52.94, 30.00)
    call CameraSetupApplyForPlayer(true, gg_cam_LireesaDeath03, Player(0), 0)
