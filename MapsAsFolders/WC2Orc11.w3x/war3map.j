    set udg_FinalCinematic=true
    // //
    // CinematicStarted
    // //
    // Scene 1 - Lireesa Execution
    // //
    call TransmissionFromUnitWithNameBJ(GetPlayersAll(), udg_FakeLireesa, "TRIGSTR_3195", gg_snd_WC2Orc11Lireesa_Final1, "TRIGSTR_3196", bj_TIMETYPE_ADD, 0, true)
