    set gg_trg_EarthSpawn=CreateTrigger()
    call TriggerAddCondition(gg_trg_EarthSpawn, Condition(function Trig_EarthSpawn_Conditions))
    call TriggerAddAction(gg_trg_EarthSpawn, function Trig_EarthSpawn_Actions)
