    // //
    call ConditionalTriggerExecute(gg_trg_FinalCinematicSetUp)
    call TriggerSleepAction(1.00)
    // //
    call CameraSetupApplyForPlayer(true, gg_cam_LireesaDeath01, Player(0), 0)
