function Trig_Next_Level_Prep_Actions takes nothing returns nothing
    call QueuedTriggerRemoveBJ(gg_trg_BetreyalCinematicQ)
    call SetPlayerAllianceStateBJ(Player(0), Player(1), bj_ALLIANCE_UNALLIED)
    call SetPlayerAllianceStateBJ(Player(1), Player(0), bj_ALLIANCE_UNALLIED)
    // Сохранение Героев
    call StoreUnitBJ(udg_Guldan, "Guldan", "WC2Orc11", udg_Cache)
    call StoreUnitBJ(udg_Chogall, "Chogall", "WC2Orc11", udg_Cache)
    call StoreUnitBJ(udg_Utok, "Utok", "WC2Orc11", udg_Cache)
    call StoreBooleanBJ(true, "Completed", "WC2Orc11", udg_Cache)
    call TriggerSleepAction(0.10)
    call SaveGameCacheBJ(udg_Cache)
    // SecretMission
    if ( Trig_Next_Level_Prep_Func012C() ) then
        call ShowCustomCampaignButton(true, 17)
        call SetNextLevelBJ("WC2Orc11s.w3x")
    else
        call SetNextLevelBJ("WC2OrcAct4.w3x")
    endif
    // Act4
    call ShowCustomCampaignButton(true, 18)
    call ShowCustomCampaignButton(true, 19)
    // Victory
    call CustomVictoryBJ(Player(0), true, false)
endfunction

