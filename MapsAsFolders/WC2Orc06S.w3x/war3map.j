    call TriggerRegisterAnyUnitEventBJ(trainingListener, EVENT_PLAYER_UNIT_TRAIN_FINISH)
    call TriggerAddAction(trainingListener, function AutoSmartRally___onTrainFinish)
    set trainingListener=null
endfunction

