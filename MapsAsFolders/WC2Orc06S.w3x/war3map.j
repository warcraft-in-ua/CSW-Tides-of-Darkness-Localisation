    // Showing Zuluhed
    set udg_ZuluhedTransporting=true
    call AddUnitAnimationPropertiesBJ(true, "alternate", udg_ZuluhedBoat)
    // //
    // Making Organic
    call UnitRemoveTypeBJ(UNIT_TYPE_MECHANICAL, udg_ZuluhedBoat)
    // //
