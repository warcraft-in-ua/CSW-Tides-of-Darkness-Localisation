    set bj_wantDestroyGroup=true
    call ForGroupBJ(GetUnitsInRangeOfLocMatching(300.00, udg_Nekros_TempPoint_R, Condition(function Trig_Nekros_I_Cursed_Ground_Copy_Func011001003)), function Trig_Nekros_I_Cursed_Ground_Copy_Func011A)
