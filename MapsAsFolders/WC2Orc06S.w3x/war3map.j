//===========================================================================
// Trigger: AlextraszaRaidStart
//===========================================================================
function Trig_AlextraszaRaidStart_Func012002 takes nothing returns nothing
    call IssuePointOrderLocBJ(GetEnumUnit(), "attack", GetRectCenter(gg_rct_Orc_Camp))
endfunction

function Trig_AlextraszaRaidStart_Actions takes nothing returns nothing
    set udg_AlextrazaInRaid=true
    // //
    call AddWeatherEffectSaveLast(gg_rct_CameraBoundsRTS, 'WOlw')
    set udg_AlextraszaWind=GetLastCreatedWeatherEffect()
    call EnableWeatherEffect(udg_AlextraszaWind, true)
    // //
    call GroupRemoveGroup(udg_DragonsAttackGroup, udg_DragonsAttackGroup)
    // //
    set bj_forLoopAIndex=1
    set bj_forLoopAIndexEnd=udg_DragonsAttackMaxAmount
    loop
        exitwhen bj_forLoopAIndex > bj_forLoopAIndexEnd
        call CreateNUnitsAtLoc(1, 'nrwm', Player(12), GetRectCenter(gg_rct_SpawnDragons), bj_UNIT_FACING)
        call GroupAddUnitSimple(GetLastCreatedUnit(), udg_DragonsAttackGroup)
        call CreateNUnitsAtLoc(1, 'nrdr', Player(12), GetRectCenter(gg_rct_SpawnDragons), bj_UNIT_FACING)
        call GroupAddUnitSimple(GetLastCreatedUnit(), udg_DragonsAttackGroup)
        set bj_forLoopAIndex=bj_forLoopAIndex + 1
    endloop
    // //
    call IssuePointOrderLocBJ(udg_Alextraza, "attack", GetRectCenter(gg_rct_Orc_Camp))
    call ForGroupBJ(udg_DragonsAttackGroup, function Trig_AlextraszaRaidStart_Func012002)
    // //
    call EnableTrigger(gg_trg_AlextrasaRaidInvoulneravbleShieldOn)
    // //
    // //
    call QuestMessageBJ(bj_FORCE_PLAYER[0], bj_QUESTMESSAGE_WARNING, "TRIGSTR_4077")
    // //
    call EnableTrigger(gg_trg_AlextraszaRaidEnd)
    call EnableTrigger(gg_trg_AlextrazaRaidFightAction)
endfunction

