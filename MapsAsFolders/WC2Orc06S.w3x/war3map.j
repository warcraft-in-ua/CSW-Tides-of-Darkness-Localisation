function Trig_TornadoActivate_Conditions takes nothing returns boolean
    if ( not ( GetEnteringUnit() == udg_Zuluhed ) ) then
        return false
    endif
    if ( not ( UnitHasItemOfTypeBJ(udg_Zuluhed, 'I000') == true ) ) then
        return false
    endif
    return true
endfunction

function Trig_TornadoActivate_Actions takes nothing returns nothing
    call DisableTrigger(GetTriggeringTrigger())
    call TriggerSleepAction(0.25)
    call PauseUnitBJ(true, udg_Zuluhed)
    // //
    // //
    call RemoveItem(GetItemOfTypeFromUnitBJ(udg_Zuluhed, 'I000'))
    // //
    call PlaySoundAtPointBJ(gg_snd_CycloneBirth1, 100, GetRectCenter(gg_rct_WhirlWindIn), 0)
    // //
    call CreateNUnitsAtLoc(1, 'n02G', Player(PLAYER_NEUTRAL_PASSIVE), GetRectCenter(gg_rct_WhirlWindIn), bj_UNIT_FACING)
    set udg_TornadoUnit=GetLastCreatedUnit()
    call AddSpecialEffectTargetUnitBJ("sprite first", udg_TornadoUnit, "Units\\Orc\\Zuluhed\\Zuluhed.mdx")
    set udg_ZuluhedTornadoEffect=GetLastCreatedEffectBJ()
    call BlzSetSpecialEffectColorByPlayer(udg_ZuluhedTornadoEffect, Player(21))
    call BlzSetSpecialEffectScale(udg_ZuluhedTornadoEffect, 0.80)
    call BlzSpecialEffectAddSubAnimation(udg_ZuluhedTornadoEffect, SUBANIM_TYPE_READY)
    call BlzPlaySpecialEffect(udg_ZuluhedTornadoEffect, ANIM_TYPE_STAND)
    // //
    call UnitShareVisionBJ(true, udg_TornadoUnit, Player(0))
    call ShowUnitHide(udg_Zuluhed)
    // //
    call SetCameraTargetControllerNoZForPlayer(Player(0), udg_TornadoUnit, 0, 400.00, false)
    // //
    call AttachSoundToUnitBJ(gg_snd_CycloneLoop1, udg_TornadoUnit)
    call PlaySoundBJ(gg_snd_CycloneLoop1)
    call IssuePointOrderLocBJ(udg_TornadoUnit, "move", GetRectCenter(gg_rct_WhirlWindOut))
    // //
    call EnableSelect(false, false)
    // //
endfunction

