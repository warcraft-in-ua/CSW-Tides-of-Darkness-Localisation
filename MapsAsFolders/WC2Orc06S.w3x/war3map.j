function InitTrig_DragonSoulRecharged takes nothing returns nothing
    set gg_trg_DragonSoulRecharged=CreateTrigger()
    call DisableTrigger(gg_trg_DragonSoulRecharged)
    call TriggerRegisterAnyUnitEventBJ(gg_trg_DragonSoulRecharged, EVENT_PLAYER_UNIT_DEATH)
    call TriggerAddCondition(gg_trg_DragonSoulRecharged, Condition(function Trig_DragonSoulRecharged_Conditions))
