    call SetUnitAbilityLevelSwapped('A00R', udg_ZuluhedBoat, GetUnitAbilityLevelSwapped('A00R', udg_Zuluhed))
    // //
    // Unblocking attack through Upgrade mechanic
    call SetPlayerTechResearchedSwap('R00N', 1, Player(0))
    // //
    // Assigning Skin in order to have propper icon and soundkit
    call BlzSetUnitSkin(udg_ZuluhedBoat, 'o001')
    call BlzSetUnitRealFieldBJ(udg_ZuluhedBoat, UNIT_RF_HIT_POINTS_REGENERATION_RATE, BlzGetUnitRealField(udg_Zuluhed, UNIT_RF_HIT_POINTS_REGENERATION_RATE))
    call BlzSetUnitRealFieldBJ(udg_ZuluhedBoat, UNIT_RF_MANA_REGENERATION, BlzGetUnitRealField(udg_Zuluhed, UNIT_RF_MANA_REGENERATION))
