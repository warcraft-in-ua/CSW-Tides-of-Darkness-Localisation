    endif
    return true
endfunction

function Trig_TornadoDeactivate_Actions takes nothing returns nothing
    call DisableTrigger(GetTriggeringTrigger())
    call TriggerSleepAction(0.25)
    // //
    call StopSoundBJ(gg_snd_CycloneLoop1, true)
    call PlaySoundAtPointBJ(gg_snd_CycloneDeath1, 100, GetRectCenter(gg_rct_WhirlWindOut), 0)
    call KillUnit(udg_TornadoUnit)
    call TriggerSleepAction(4.80)
    // //
    // //
    call SetUnitPositionLoc(udg_Zuluhed, GetRectCenter(gg_rct_WhirlWindOut))
    // //
    call ShowUnitShow(udg_Zuluhed)
    call PauseUnitBJ(false, udg_Zuluhed)
    call TriggerSleepAction(1.00)
    call DestroyEffectBJ(udg_ZuluhedTornadoEffect)
    // //
    call ResetToGameCameraForPlayer(Player(0), 1.00)
    // //
    call EnableSelect(true, true)
    call SelectUnitForPlayerSingle(udg_Zuluhed, Player(0))
    // //
    if ( Trig_TornadoDeactivate_Func024C() ) then
        call SetUnitOwner(gg_unit_o006_0292, Player(20), false)
