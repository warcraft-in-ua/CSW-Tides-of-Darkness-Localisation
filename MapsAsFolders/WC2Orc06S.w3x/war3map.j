//===========================================================================
// Trigger: DragonSoulRecharged
//===========================================================================
function Trig_DragonSoulRecharged_Conditions takes nothing returns boolean
    if ( not ( IsUnitType(GetDyingUnit(), UNIT_TYPE_MECHANICAL) == false ) ) then
        return false
    endif
    if ( not ( IsUnitInGroup(GetDyingUnit(), udg_CorruptedSoulsGroup) == true ) ) then
        return false
    endif
    return true
endfunction

function Trig_DragonSoulRecharged_Func027C takes nothing returns boolean
    if ( not ( udg_DragonSoulCharges >= 5 ) ) then
        return false
    endif
    return true
endfunction

function Trig_DragonSoulRecharged_Actions takes nothing returns nothing
    local effect SEDragonSoulSyphon
    local effect SEDragonSoulKillEffect
    local effect SEDragonSoulRecharged
    // //
    call ConditionalTriggerExecute(gg_trg_HintSoulDrained)
    set udg_DragonSoulCharges=( udg_DragonSoulCharges + 1 )
    // //
    call GroupRemoveUnitSimple(GetDyingUnit(), udg_CorruptedSoulsGroup)
    // //
    call AddSpecialEffectTargetUnitBJ("origin", GetDyingUnit(), "Objects\\Spawnmodels\\Undead\\UndeadDissipate\\UndeadDissipate.mdl")
    set SEDragonSoulKillEffect=GetLastCreatedEffectBJ()
    call PlaySoundBJ(gg_snd_FlashBack1Second)
    // //
    call TriggerSleepAction(2)
    call DestroyEffectBJ(SEDragonSoulKillEffect)
    // //
    call AddSpecialEffectTargetUnitBJ("origin", udg_Necros, "buildings\\other\\bookofsummoning\\bos_stand.mdl")
    set SEDragonSoulSyphon=GetLastCreatedEffectBJ()
    call PlaySoundBJ(gg_snd_FlashBackHalfSecondSecond)
    // //
    call TriggerSleepAction(2)
    call DestroyEffectBJ(SEDragonSoulSyphon)
    // //
    if ( Trig_DragonSoulRecharged_Func027C() ) then
        call ConditionalTriggerExecute(gg_trg_HintDragonSoulRecharged)
        call SetItemCharges(udg_ItemDragonSoulActive, ( GetItemCharges(udg_ItemDragonSoulActive) + 1 ))
        set udg_DragonSoulCharges=0
        call AddSpecialEffectTargetUnitBJ("origin", udg_Necros, "Abilities\\Spells\\Human\\Resurrect\\ResurrectTarget.mdl")
        set SEDragonSoulRecharged=GetLastCreatedEffectBJ()
    else
    endif
    // //
    call TriggerSleepAction(2)
    // //
    call DestroyEffectBJ(SEDragonSoulRecharged)
endfunction

