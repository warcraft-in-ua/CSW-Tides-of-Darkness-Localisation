    call CameraSetupApplyForPlayer(true, gg_cam_Zuljin06, Player(0), ( GetSoundDurationBJ(gg_snd_WC2Orc02Rend15) + ( GetSoundDurationBJ(gg_snd_WC2Orc02ZulJin6) + 2.00 ) ))
